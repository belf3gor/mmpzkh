#Область ПроцедурыИФункцииПроведенияДокумента

// Функция возвращает параметры проведения.
Функция ПодготовитьПараметрыПроведения(ДокументСсылка, Отказ) Экспорт
	
	ПараметрыПроведения = Новый Структура;
	
	Запрос = Новый Запрос;
	
	Запрос.УстановитьПараметр("Ссылка", ДокументСсылка);
	Запрос.Текст =
	"ВЫБРАТЬ
	|	Реквизиты.Ответственный,
	|	Реквизиты.Дата,
	|	Реквизиты.Номер,
	|	Реквизиты.Ссылка
	|ИЗ
	|	Документ.УПЖКХ_СменаСтепениРодстваФизЛиц КАК Реквизиты
	|ГДЕ
	|	Реквизиты.Ссылка = &Ссылка";
	
	Выборка = Запрос.Выполнить().Выбрать();
	Выборка.Следующий();
	
	Реквизиты = Новый Структура("Ссылка,Номер,Дата,Ответственный");
	
	ЗаполнитьЗначенияСвойств(Реквизиты, Выборка);
	
	ПараметрыПроведения.Вставить("Реквизиты", Реквизиты);
	
	Запрос.Текст =
	"ВЫБРАТЬ
	|	Реквизиты.Ссылка,
	|	Реквизиты.ФизЛицоОсновное,
	|	Реквизиты.ФизЛицоСвязанное,
	|	Реквизиты.СтепеньРодства
	|ИЗ
	|	Документ.УПЖКХ_СменаСтепениРодстваФизЛиц.СвязанныеФизЛица КАК Реквизиты
	|ГДЕ
	|	Реквизиты.Ссылка = &Ссылка";
	
	ТаблицаСвязанныеФизЛица = Запрос.Выполнить().Выгрузить();
	
	ПараметрыПроведения.Вставить("ТаблицаСвязанныеФизЛица", ТаблицаСвязанныеФизЛица);
	
	Возврат ПараметрыПроведения;
	
КонецФункции

#КонецОбласти

