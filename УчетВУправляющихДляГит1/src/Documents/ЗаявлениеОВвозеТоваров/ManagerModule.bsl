	Если НЕ ЗначениеЗаполнено(ИмяПодп) Тогда
		Сообщение = Новый СообщениеПользователю;
		Сообщение.Текст = НСтр("ru='Не заполнено имя подписанта. Требуется заполнить в форме ""Сведения об отправителе и подписантах"", вызываемой из командного меню документа.'");
		Сообщение.Сообщить();
		СтатусПроверки = Ложь;
