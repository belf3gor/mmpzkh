
&НаКлиенте
Процедура ОбработкаКоманды(ПараметрКоманды, ПараметрыВыполненияКоманды)

	ПараметрыФормы = Новый Структура;
	ПараметрыОтбор = Новый Структура;

	ПараметрыОтбор.Вставить("НематериальныйАктив", ПараметрКоманды);
	ПараметрыФормы.Вставить("Отбор", ПараметрыОтбор);

	ОткрытьФорму("РегистрНакопления.ОплатыНМАДляУСН.ФормаСписка", ПараметрыФормы,
		ПараметрыВыполненияКоманды.Источник,
		ПараметрыВыполненияКоманды.Уникальность,
		ПараметрыВыполненияКоманды.Окно);

КонецПроцедуры
