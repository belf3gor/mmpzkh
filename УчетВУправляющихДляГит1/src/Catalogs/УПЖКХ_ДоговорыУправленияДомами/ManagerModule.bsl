// Формирует таблицу с данными по договорам управления.
//
// Параметры:
//  ДоговораУправления - СписокЗначений - список ссылок на договора управления.
//
// Возвращаемое значение:
//  ТаблицаЗначений
//
Функция ПолучитьДанныеДоговоровУправления(ДоговораУправления) Экспорт
	
	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ РАЗРЕШЕННЫЕ
	|	УПЖКХ_ДоговорыУправленияДомами.Ссылка,
	|	УПЖКХ_ДоговорыУправленияДомами.Наименование,
	|	УПЖКХ_ДоговорыУправленияДомами.НомерДоговора,
	|	УПЖКХ_ДоговорыУправленияДомами.ДатаЗаключенияДоговора,
	|	УПЖКХ_ДоговорыУправленияДомами.УправляющаяОрганизация,
	|	УПЖКХ_ДоговорыУправленияДомами.ВидВторойСтороныДоговора,
	|	УПЖКХ_ДоговорыУправленияДомами.ВтораяСторонаДоговораКонтрагент,
	|	УПЖКХ_ДоговорыУправленияДомами.ВидОснованияЗаключенияДоговора,
	|	УПЖКХ_ДоговорыУправленияДомами.ОснованиеЗаключенияДоговора,
	|	УПЖКХ_ДоговорыУправленияДомами.УправляемыйОбъект,
	|	УПЖКХ_ДоговорыУправленияДомами.ПериодВыставленияКвитанцийТекущийМесяц,
	|	УПЖКХ_ДоговорыУправленияДомами.ПериодВыставленияКвитанцийЧисло,
	|	УПЖКХ_ДоговорыУправленияДомами.ДоговорУправления,
	|	УПЖКХ_ДоговорыУправленияДомами.ВтораяСторонаДоговораСобственники.(
	|		Ссылка,
	|		НомерСтроки,
	|		Собственник
	|	),
	|	УПЖКХ_ДоговорыУправленияДомами.ПериодПодачиПоказанийПоСчетчикамНачало,
	|	УПЖКХ_ДоговорыУправленияДомами.ПериодПодачиПоказанийПоСчетчикамКонец,
	|	УПЖКХ_ДоговорыУправленияДомами.ПереченьКоммунальныхУслуг.(
	|		Ссылка,
	|		НомерСтроки,
	|		Услуга,
	|		Предоставляет,
	|		ВидУслуги,
	|		ДатаНачалаПредоставления,
	|		ДатаОкончанияПредоставления
	|	),
	|	УПЖКХ_ДоговорыУправленияДомами.ДополнительныеСоглашения.(
	|		Ссылка,
	|		НомерСтроки,
	|		НомерСоглашения,
	|		ДатаСоглашения,
	|		ДополнительноеСоглашениеПредставление,
	|		ДополнительноеСоглашениеФайл
	|	),
	|	УПЖКХ_ДоговорыУправленияДомами.ПереченьДополнительныхУслуг.(
	|		Ссылка,
	|		НомерСтроки,
	|		ВидУслуги,
	|		ДатаНачалаПредоставления,
	|		ДатаОкончанияПредоставления
	|	)
	|ИЗ
	|	Справочник.УПЖКХ_ДоговорыУправленияДомами КАК УПЖКХ_ДоговорыУправленияДомами
	|ГДЕ
	|	УПЖКХ_ДоговорыУправленияДомами.Ссылка В(&ДоговораУправления)";
	Запрос.УстановитьПараметр("ДоговораУправления", ДоговораУправления);
	
	Возврат Запрос.Выполнить().Выгрузить();
	
КонецФункции // ПолучитьДанныеДоговоровУправления()

#КонецОбласти

#КонецЕсли