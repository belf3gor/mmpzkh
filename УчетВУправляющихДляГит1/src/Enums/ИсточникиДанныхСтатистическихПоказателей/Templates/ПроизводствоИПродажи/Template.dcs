			И Выручка.НоменклатурнаяГруппа = ДоляНДС.НоменклатурнаяГруппа
			И Выручка.СтавкаНДС = ДоляНДС.СтавкаНДС
{ГДЕ
	(Выручка.Сумма - ВЫБОР
			КОГДА ЕСТЬNULL(ДоляНДС.ВыручкаВключаяНДС, 0) = 0
				ТОГДА 0
			КОГДА ДоляНДС.ВыручкаВключаяНДС = Выручка.Сумма
				ТОГДА ДоляНДС.НДС
			ИНАЧЕ ДоляНДС.НДС * Выручка.Сумма / ДоляНДС.ВыручкаВключаяНДС
