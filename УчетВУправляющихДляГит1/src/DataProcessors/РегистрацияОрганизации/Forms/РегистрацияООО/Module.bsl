	Имена = Новый Структура;
	Имена.Вставить("ФизическоеЛицо",         "Руководитель");
	Имена.Вставить("ПаспортныеДанные",       "ПаспортныеДанныеРуководителя");
	Имена.Вставить("СерияНомерПаспорта",     "ПаспортСерияНомер");
