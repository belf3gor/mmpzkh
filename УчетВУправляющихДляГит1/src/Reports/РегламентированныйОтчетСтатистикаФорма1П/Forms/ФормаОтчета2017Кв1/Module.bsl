	Если СтрНайти(Область.Имя, "П000080001003_") > 0 Тогда
		ОбластьНазвание = СтрЗаменить(Область.Имя, "П000080001003_", "П000080001001_");
		ТабличныйДокумент.Области[ОбластьНазвание].Значение = РезультатНазвание;
	КонецЕсли;
	
	Если СтрНайти(Область.Имя, "П996600000004_") > 0 Тогда
		ОбластьНазвание = СтрЗаменить(Область.Имя, "П996600000004_", "П996600000001_");
		ТабличныйДокумент.Области[ОбластьНазвание].Значение = РезультатНазвание;
	КонецЕсли;
	
	Если СтрНайти(Область.Имя, "П997600000003_") > 0 Тогда
		ОбластьНазвание = СтрЗаменить(Область.Имя, "П997600000003_", "П997600000001_");
		ТабличныйДокумент.Области[ОбластьНазвание].Значение = РезультатНазвание;
	КонецЕсли;
	
	Если СтрНайти(Область.Имя, "П998600000003_") > 0 Тогда
		ОбластьНазвание = СтрЗаменить(Область.Имя, "П998600000003_", "П998600000001_");
		ТабличныйДокумент.Области[ОбластьНазвание].Значение = РезультатНазвание;
	КонецЕсли;
	
	Если СтрНайти(Область.Имя, "П996001000004_") > 0 Тогда
		ОбластьНазвание = СтрЗаменить(Область.Имя, "П996001000004_", "П996001000001_");
		ТабличныйДокумент.Области[ОбластьНазвание].Значение = РезультатНазвание;
	КонецЕсли;
	
	Если СтрНайти(Область.Имя, "П997001000003_") > 0 Тогда
		ОбластьНазвание = СтрЗаменить(Область.Имя, "П997001000003_", "П997001000001_");
		ТабличныйДокумент.Области[ОбластьНазвание].Значение = РезультатНазвание;
