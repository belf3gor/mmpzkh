// Функция РАСЧЕТ ()
// Выполняет расчет вычисляемых показателей на заданной странице отчета
// (ячеек, выделенных зеленым цветом). 
// В случае неуказания листа, ничего происходить не будет
// Параметры:
//  ИмяСтраницыПанели - строка, имя страницы основной панели формы.
//
&НаКлиентеНаСервереБезКонтекста
Процедура Расчет(Форма, ИмяТекущейСтраницыПанели = "", ОбластьИмя = "")
	
	Перем ТаблДанных;
	
	ИндексПоиска = СтрНайти(ОбластьИмя, "_");
	Если ИндексПоиска > 0 Тогда
		ОбластьИмя = Лев(ОбластьИмя, ИндексПоиска - 1);
	КонецЕсли;
	
	Если ИмяТекущейСтраницыПанели = "Раздел2_3" Тогда
		
		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П030001901, ОбластьИмя) > 0 Тогда
			Форма.ТабличныйДокумент.Области.П030001901.Значение = Форма.ТабличныйДокумент.Области.П030002001.Значение
			+ Форма.ТабличныйДокумент.Области.П030002101.Значение
			+ Форма.ТабличныйДокумент.Области.П030002201.Значение
			+ Форма.ТабличныйДокумент.Области.П030002301.Значение
			+ Форма.ТабличныйДокумент.Области.П030002501.Значение
			+ Форма.ТабличныйДокумент.Области.П030002601.Значение
			+ Форма.ТабличныйДокумент.Области.П030002701.Значение;
		КонецЕсли;
		
	ИначеЕсли ИмяТекущейСтраницыПанели = "Раздел4_6" Тогда
		
		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П04000310100, ОбластьИмя) > 0 Тогда
			
			ТаблДанных = Неопределено;
			Форма.мСтруктураМногострочныхРазделов.Свойство(Форма.СтруктураРеквизитовФормы.мИдГруппы1, ТаблДанных);
			ТаблДанных = Форма[ТаблДанных];
			ОбщЗначение = 0;
			Для Инд = 1 По ТаблДанных.Количество() Цикл
				ОбщЗначение = ОбщЗначение + Форма.ТабличныйДокумент.Области["П000040001004_" + Строка(Инд)].Значение;
			КонецЦикла;
			
			Форма.ТабличныйДокумент.Области.П04000310100.Значение = ОбщЗначение;
			
		КонецЕсли;
		
		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П060003601, ОбластьИмя) > 0 Тогда
			Форма.ТабличныйДокумент.Области.П060003601.Значение = Форма.ТабличныйДокумент.Области.П060003701.Значение
			+ Форма.ТабличныйДокумент.Области.П060004001.Значение
			+ Форма.ТабличныйДокумент.Области.П060004401.Значение;
		КонецЕсли;
		
	ИначеЕсли ИмяТекущейСтраницыПанели = "Раздел7" Тогда
		
		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П070015601, ОбластьИмя) > 0 Тогда
			
			Форма.ТабличныйДокумент.Области.П070007901.Значение = Форма.ТабличныйДокумент.Области.П070008001.Значение
			+ Форма.ТабличныйДокумент.Области.П070008101.Значение
			+ Форма.ТабличныйДокумент.Области.П070008201.Значение
			+ Форма.ТабличныйДокумент.Области.П070008301.Значение;
			
			ТекСумма = 0;
			Для Ном = 141 По 154 Цикл
				ТекСумма = ТекСумма + Форма.ТабличныйДокумент.Области["П0700" + Формат(Ном, "ЧЦ=3; ЧВН=") + "01"].Значение;
			КонецЦикла;
			
			Форма.ТабличныйДокумент.Области.П070012001.Значение = Форма.ТабличныйДокумент.Области.П070012101.Значение
			+ Форма.ТабличныйДокумент.Области.П070012901.Значение
			+ Форма.ТабличныйДокумент.Области.П070013001.Значение
			+ Форма.ТабличныйДокумент.Области.П070013101.Значение
			+ Форма.ТабличныйДокумент.Области.П070013201.Значение
			+ Форма.ТабличныйДокумент.Области.П070013701.Значение
			+ Форма.ТабличныйДокумент.Области.П070013801.Значение
			+ ТекСумма;
			
			Форма.ТабличныйДокумент.Области.П070015601.Значение = Форма.ТабличныйДокумент.Области.П070007501.Значение
			+ Форма.ТабличныйДокумент.Области.П070007901.Значение
			+ Форма.ТабличныйДокумент.Области.П070008401.Значение
			+ Форма.ТабличныйДокумент.Области.П070008801.Значение
			+ Форма.ТабличныйДокумент.Области.П070008901.Значение
			+ Форма.ТабличныйДокумент.Области.П070009001.Значение
			- Форма.ТабличныйДокумент.Области.П070009101.Значение
			+ Форма.ТабличныйДокумент.Области.П070009201.Значение
			- Форма.ТабличныйДокумент.Области.П070009301.Значение
			- Форма.ТабличныйДокумент.Области.П070009401.Значение
			- Форма.ТабличныйДокумент.Области.П070009501.Значение
			+ Форма.ТабличныйДокумент.Области.П070009801.Значение
			+ Форма.ТабличныйДокумент.Области.П070010001.Значение
			+ Форма.ТабличныйДокумент.Области.П070010101.Значение
			+ Форма.ТабличныйДокумент.Области.П070010201.Значение
			+ Форма.ТабличныйДокумент.Области.П070010301.Значение
			+ Форма.ТабличныйДокумент.Области.П070010401.Значение
			+ Форма.ТабличныйДокумент.Области.П070010701.Значение
			+ Форма.ТабличныйДокумент.Области.П070010801.Значение
			+ Форма.ТабличныйДокумент.Области.П070011001.Значение
			+ Форма.ТабличныйДокумент.Области.П070011101.Значение
			+ Форма.ТабличныйДокумент.Области.П070011201.Значение
			+ Форма.ТабличныйДокумент.Области.П070011301.Значение
			+ Форма.ТабличныйДокумент.Области.П070011801.Значение
			+ Форма.ТабличныйДокумент.Области.П070012001.Значение
			+ Форма.ТабличныйДокумент.Области.П070015501.Значение;
		КонецЕсли;
		
	ИначеЕсли (ИмяТекущейСтраницыПанели = "Раздел8")
		Или (ИмяТекущейСтраницыПанели = "Раздел8_Колонка1")
		Или (ИмяТекущейСтраницыПанели = "Раздел8_Колонка2")
		Или (ИмяТекущейСтраницыПанели = "Раздел8_Колонка3")
		Или (ИмяТекущейСтраницыПанели = "Раздел8_Колонка4")
		Или (ИмяТекущейСтраницыПанели = "Раздел8_Колонка5") Тогда
		
		Для Инд = 1 По 5 Цикл 
			ОбщЗначениеКолонка = 0;
			
			Стр = 0;
			Пока Истина Цикл 
				Стр = Стр + 1;
				Обл = Форма.ТабличныйДокумент.Области.Найти("П00008000100" + (3+Инд) + "_" + Строка(Стр));
				Если Обл = Неопределено Тогда 
					Прервать;
				Иначе 
					ОбщЗначениеКолонка = ОбщЗначениеКолонка + Обл.Значение;
				КонецЕсли;
			КонецЦикла;
			
			Форма.ТабличныйДокумент.Области["П08001670"+Инд].Значение = ОбщЗначениеКолонка;
			
		КонецЦикла;
		
		
	ИначеЕсли (ИмяТекущейСтраницыПанели = "Раздел9")
		Или (ИмяТекущейСтраницыПанели = "Раздел9_Колонка1")
		Или (ИмяТекущейСтраницыПанели = "Раздел9_Колонка2")
		Или (ИмяТекущейСтраницыПанели = "Раздел9_Колонка3")
		Или (ИмяТекущейСтраницыПанели = "Раздел9_Колонка4") Тогда
		
