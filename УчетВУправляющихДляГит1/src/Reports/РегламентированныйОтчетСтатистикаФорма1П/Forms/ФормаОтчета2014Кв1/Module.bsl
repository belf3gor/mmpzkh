		КонецЕсли;
		
	ИначеЕсли ИмяТекущейСтраницыПанели = "" Тогда
		
		Расчет(Форма, "Раздел10", ОбластьИмя);
		Расчет(Форма, "Раздел9", ОбластьИмя);
