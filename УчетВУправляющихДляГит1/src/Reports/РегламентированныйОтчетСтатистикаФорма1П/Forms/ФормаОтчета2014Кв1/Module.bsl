			КонецЦикла;
			
			Форма.ТабличныйДокумент.Области.П09001810101.Значение = ОбщЗначениеКолонка1;
			
		КонецЕсли;
		
		Если ((Колонка = "0") Или (Колонка = "2"))
			И (НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П09001810201, ОбластьИмя) > 0) Тогда
			
			ОбщЗначениеКолонка2 = 0;
			
			Для Инд = 1 По ТаблДанных.Количество() Цикл	
				ОбщЗначениеКолонка2 = ОбщЗначениеКолонка2 + Форма.ТабличныйДокумент.Области["П000090001005_" + Строка(Инд)].Значение;
			КонецЦикла;
			
			Форма.ТабличныйДокумент.Области.П09001810201.Значение = ОбщЗначениеКолонка2;
			
		КонецЕсли;
		
		Если ((Колонка = "0") Или (Колонка = "3"))
			И (НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П09001810301, ОбластьИмя) > 0) Тогда
			
			ОбщЗначениеКолонка3 = 0;
			
			Для Инд = 1 По ТаблДанных.Количество() Цикл	
				ОбщЗначениеКолонка3 = ОбщЗначениеКолонка3 + Форма.ТабличныйДокумент.Области["П000090001006_" + Строка(Инд)].Значение;
			КонецЦикла;
			
			Форма.ТабличныйДокумент.Области.П09001810301.Значение = ОбщЗначениеКолонка3;
			
		КонецЕсли;	
		
		Если ((Колонка = "0") Или (Колонка = "4"))
			И (НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П09001810401, ОбластьИмя) > 0) Тогда
			
			ОбщЗначениеКолонка4 = 0;
			
			Для Инд = 1 По ТаблДанных.Количество() Цикл	
				ОбщЗначениеКолонка4 = ОбщЗначениеКолонка4 + Форма.ТабличныйДокумент.Области["П000090001007_" + Строка(Инд)].Значение;
			КонецЦикла;
			
			Форма.ТабличныйДокумент.Области.П09001810401.Значение = ОбщЗначениеКолонка4;
			
		КонецЕсли;
		
	ИначеЕсли (ИмяТекущейСтраницыПанели = "Раздел10")
		Или (ИмяТекущейСтраницыПанели = "Раздел10_Колонка1")
		Или (ИмяТекущейСтраницыПанели = "Раздел10_Колонка2")
		Или (ИмяТекущейСтраницыПанели = "Раздел10_Колонка3")
		Или (ИмяТекущейСтраницыПанели = "Раздел10_Колонка4") Тогда
		
		ТаблДанных = Неопределено;
		Форма.мСтруктураМногострочныхРазделов.Свойство(Форма.СтруктураРеквизитовФормы.мИдГруппы4, ТаблДанных);
		ТаблДанных = Форма[ТаблДанных];
		Колонка = ?(СтрНайти(ИмяТекущейСтраницыПанели, "Колонка") > 0, Прав(ИмяТекущейСтраницыПанели, 1), "0");
		
