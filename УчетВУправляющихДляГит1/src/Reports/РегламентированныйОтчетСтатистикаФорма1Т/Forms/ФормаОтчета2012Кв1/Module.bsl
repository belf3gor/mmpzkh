&НаКлиенте
Процедура ПолеТабличногоДокументаФормаОтчетаВыбор(Элемент, Область, СтандартнаяОбработка)
	
	Если Область.Имя = "ОргАдрес" Тогда
		РегламентированнаяОтчетностьКлиент.ОбработкаАдресаВСтатистическомОтчете(ЭтаФорма, Область, СтандартнаяОбработка);
	Иначе
		Если СтрНайти(Область.Имя, "П0000100001") > 0 Тогда
			СтандартнаяОбработка = Ложь;		
			Если  СтрНайти("П000010000101П000010000102П000010000103", Область.Имя) > 0 Тогда
				Огр = ?(Формат(СтруктураРеквизитовФормы.мДатаКонцаПериодаОтчета, "ДФ = 'ММ'") = "12", 3, 2);
				Для Идн = 1 По Огр Цикл
					Если ("П00001000010"+Идн) = Область.Имя Тогда
						
						ПолеТабличногоДокументаФормаОтчета.Области["П00001000010"+Идн].Значение = 
						?(ПолеТабличногоДокументаФормаОтчета.Области["П00001000010"+Идн].Значение = "", "Х", "");
					Иначе	 
						ПолеТабличногоДокументаФормаОтчета.Области["П00001000010"+Идн].Значение = "";
					КонецЕсли; 	 
				КонецЦикла;	
				
			Иначе
				Для Идн = 4 По 8 Цикл
					Если ("П00001000010"+Идн) = Область.Имя Тогда
						
						ПолеТабличногоДокументаФормаОтчета.Области["П00001000010"+Идн].Значение = 
						?(ПолеТабличногоДокументаФормаОтчета.Области["П00001000010"+Идн].Значение = "", "Х", "");
					Иначе	 
						ПолеТабличногоДокументаФормаОтчета.Области["П00001000010"+Идн].Значение = "";
					КонецЕсли;
				КонецЦикла;
				
			КонецЕсли;
			
		КонецЕсли;
	КонецЕсли;
	Модифицированность = Истина;
	
КонецПроцедуры

&НаКлиенте
