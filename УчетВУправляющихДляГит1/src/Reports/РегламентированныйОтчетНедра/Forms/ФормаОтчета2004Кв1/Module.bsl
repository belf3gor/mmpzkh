&НаКлиенте
Процедура ОбновитьАдресВТабличномДокументе(Результат, РоссийскийАдрес)
	
	Если ТипЗнч(Результат) = Тип("Структура") Тогда
		
		// Обход ошибки платформы: в веб-клиенте, в результате выполнения процедуры "СформироватьАдрес"
		// общего модуля "РегламентированнаяОтчетностьВызовСервера", не изменяются значения ключей
		// соответствия "РоссийскийАдрес", передаваемого в качестве параметра.
		РоссийскийАдрес_ = РоссийскийАдрес;
		
		РегламентированнаяОтчетностьВызовСервера.СформироватьАдрес(Результат.КонтактнаяИнформация, РоссийскийАдрес_);
		
		СтруктураРеквизитовФормы.мФизЛицИнд = РоссийскийАдрес_["Индекс"];			
		
		ТабличныйДокумент.Области.ФизЛицС.Значение        = РоссийскийАдрес_["КодРегиона"];
		ТабличныйДокумент.Области.ФизЛицИнд.Значение      = РоссийскийАдрес_["Индекс"]; 			
		ТабличныйДокумент.Области.ФизЛицСубъект.Значение  = РоссийскийАдрес_["Регион"];
		ТабличныйДокумент.Области.ФизЛицРайон.Значение    = РоссийскийАдрес_["Район"];
		ТабличныйДокумент.Области.ФизЛицГород.Значение    = РоссийскийАдрес_["Город"];
		ТабличныйДокумент.Области.ФизЛицНП.Значение 	  = РоссийскийАдрес_["НаселенныйПункт"];
		ТабличныйДокумент.Области.ФизЛицУлица.Значение    = РоссийскийАдрес_["Улица"];
		ТабличныйДокумент.Области.ФизЛицДом.Значение      = РоссийскийАдрес_["Дом"];
		ТабличныйДокумент.Области.ФизЛицКорпус.Значение   = РоссийскийАдрес_["Корпус"];
		ТабличныйДокумент.Области.ФизЛицКвартира.Значение = РоссийскийАдрес_["Квартира"];
		
		Модифицированность = Истина;
		
	КонецЕсли;
	
КонецПроцедуры

&НаКлиенте
Процедура ПолеТабличногоДокументаРаздел21Выбор(Элемент, Область, СтандартнаяОбработка)
             	
	ИмяЯчейки = Область.имя;
	Раздел = ЭтаФорма.ТабличныйДокумент;
	         	
	Если  ИмяЯчейки = "П000210002000" Или ИмяЯчейки = "П000210001000" Тогда

		ПоисковаяКолонка = "Код";
		ПервоначальноеЗначение = Область.Значение;

		Если (ИмяЯчейки = "П000210001000") И (СтруктураРеквизитовФормы.мАвтоВыборКодов) Тогда
			
			Возврат; // Вводится на листе
			
		ИначеЕсли (ИмяЯчейки = "П000210001000") И (НЕ СтруктураРеквизитовФормы.мАвтоВыборКодов) Тогда
			
			ПоисковаяКолонка = "Название";
			ПервоначальноеЗначение = Раздел.Области.П000210001000.Значение;
			
		КонецЕсли;

		СтандартнаяОбработка = Ложь;

		МаксДлина = 4;

		Если СтруктураРеквизитовФормы.мАвтоВыборКодов Тогда
			
			ДополнительныеПараметры = Новый Структура("Раздел, ПервоначальноеЗначение", Раздел, ПервоначальноеЗначение);
			ОписаниеОповещения = Новый ОписаниеОповещения("ВвестиСтрокуКодПолезногоИскопаемогоЗавершение", ЭтотОбъект, ДополнительныеПараметры);
			ПоказатьВводСтроки(ОписаниеОповещения, ПервоначальноеЗначение, "Код полезного ископаемого", МаксДлина);
			
		Иначе
			
			ПараметрыФормы = Новый Структура;
			ПараметрыФормы.Вставить("Заголовок",          "Выбор кода");
			ПараметрыФормы.Вставить("ТаблицаЗначений",    мПолезныеИскопаемыеНедра);
			ПараметрыФормы.Вставить("СтруктураДляПоиска", Новый Структура(ПоисковаяКолонка, ПервоначальноеЗначение));
			
			ДополнительныеПараметры = Новый Структура("Раздел", Раздел);
			ОписаниеОповещения = Новый ОписаниеОповещения("ОткрытьФормуВыбораКодаПолезногоИскопаемогоЗавершение", ЭтотОбъект, ДополнительныеПараметры);
			ОткрытьФорму("Обработка.ОбщиеОбъектыРеглОтчетности.Форма.ФормаВыбораЗначенияИзТаблицы", ПараметрыФормы, ЭтаФорма,,,, ОписаниеОповещения, РежимОткрытияОкнаФормы.БлокироватьОкноВладельца);
			
		КонецЕсли;

	ИначеЕсли (ИмяЯчейки = "П000210004000") Или (ИмяЯчейки = "П000210003000") Тогда

		ПоисковаяКолонка = "Код";
		ПервоначальноеЗначение = Область.Значение;

		Если (Область.Имя = "П000210003000") И (СтруктураРеквизитовФормы.мАвтоВыборКодов) Тогда
			
			Возврат; // Вводится на листе
			
		ИначеЕсли (Область.Имя = "П000210003000") И (НЕ СтруктураРеквизитовФормы.мАвтоВыборКодов) Тогда
			
			ПоисковаяКолонка = "Название";
			ПервоначальноеЗначение = Раздел.Области.П000210003000.Значение;
			
		КонецЕсли;

		СтандартнаяОбработка = Ложь;
		
		МаксДлина = 8;

		Если СтруктураРеквизитовФормы.мАвтоВыборКодов Тогда
			ДополнительныеПараметры = Новый Структура("Раздел, ПервоначальноеЗначение", Раздел, ПервоначальноеЗначение);
			ОписаниеОповещения = Новый ОписаниеОповещения("ВвестиСтрокуКодВидаДеятельностиРаздел2Завершение", ЭтотОбъект, ДополнительныеПараметры);
			ПоказатьВводСтроки(ОписаниеОповещения, ПервоначальноеЗначение, "Код вида деятельности", МаксДлина);
			
		Иначе
			
			ПараметрыФормы = Новый Структура;
			ПараметрыФормы.Вставить("Заголовок",          "Выбор кода");
			ПараметрыФормы.Вставить("ТаблицаЗначений",    мВидыДеятельности);
			ПараметрыФормы.Вставить("СтруктураДляПоиска", Новый Структура(ПоисковаяКолонка, ПервоначальноеЗначение));
			
			ДополнительныеПараметры = Новый Структура("Раздел", Раздел);
			ОписаниеОповещения = Новый ОписаниеОповещения("ОткрытьФормуВыбораКодаВидДеятельностиРаздел2Завершение", ЭтотОбъект, ДополнительныеПараметры);
			ОткрытьФорму("Обработка.ОбщиеОбъектыРеглОтчетности.Форма.ФормаВыбораЗначенияИзТаблицы", ПараметрыФормы, ЭтаФорма,,,, ОписаниеОповещения, РежимОткрытияОкнаФормы.БлокироватьОкноВладельца);
			
		КонецЕсли;

				
    ИначеЕсли ИмяЯчейки =  "П000210009000" Тогда
		
		РасчетНаКлиенте("Раздел21", ИмяЯчейки);
		
	// Ставка
	ИначеЕсли ИмяЯчейки = "П000210010000" Тогда
		
		РасчетНаКлиенте("Раздел21", ИмяЯчейки);
    
	ИначеЕсли (Область.Имя = "П000210007000") И (НЕ СтруктураРеквизитовФормы.мАвтоВыборКодов) Тогда

		СтандартнаяОбработка = Ложь;

		ОпределениеСтроки = "";

		ПервоначальноеЗначение = Область.Значение;
		
		ПараметрыФормы = Новый Структура;
		ПараметрыФормы.Вставить("НачальноеЗначениеВыбора", 		 ПервоначальноеЗначение);
		
		ФормаВводаЛицензии = ПолучитьФорму("Отчет.РегламентированныйОтчетНедра.Форма.ФормаВводаЛицензии", ПараметрыФормы);
		
		ДополнительныеПараметры = Новый Структура("Область, ФормаВводаЛицензии", Область, ФормаВводаЛицензии);
		ОписаниеОповещения = Новый ОписаниеОповещения("ОткрытьФормуВводаЛицензииЗавершение", ЭтотОбъект, ДополнительныеПараметры);
		ФормаВводаЛицензии.ОписаниеОповещенияОЗакрытии = ОписаниеОповещения;
		ФормаВводаЛицензии.РежимОткрытияОкна = РежимОткрытияОкнаФормы.БлокироватьОкноВладельца;
		ФормаВводаЛицензии.Открыть();
		
	КонецЕсли;
	       	
КонецПроцедуры

&НаКлиенте
Процедура ВвестиСтрокуКодПолезногоИскопаемогоЗавершение(Строка, ДополнительныеПараметры) Экспорт
    
	Раздел = ДополнительныеПараметры.Раздел;
	ПервоначальноеЗначение = ?(Строка = Неопределено, ДополнительныеПараметры.ПервоначальноеЗначение, Строка);
	Значение = Неопределено;

	
	Если (Строка <> Неопределено) Тогда
		
		Значение = ПервоначальноеЗначение;
		
		
	КонецЕсли;
	Если Значение <> Неопределено Тогда
		
		Раздел.Области.П000210002000.Значение = Значение;
	
	КонецЕсли;
	
