			ОткрытьФорму("Обработка.ОбщиеОбъектыРеглОтчетности.Форма.ФормаВыбораЗначенияИзТаблицы", ПараметрыФормы, ЭтаФорма,,,, ОписаниеОповещения, РежимОткрытияОкнаФормы.БлокироватьОкноВладельца);
			
		КонецЕсли;
	
	ИначеЕсли ИмяЯчейки = "П000220007000" Или ИмяЯчейки = "П000220006000" Тогда

		ПоисковаяКолонка = "Код";

		Если (Область.Имя = "П000220006000") И (СтруктураРеквизитовФормы.мАвтоВыборКодов) Тогда
			Возврат; // Вводится на листе
		ИначеЕсли (Область.Имя = "П000220006000") И (НЕ СтруктураРеквизитовФормы.мАвтоВыборКодов) Тогда
			ПоисковаяКолонка = "Название";
			ПервоначальноеЗначение = ТабличныйДокумент.Области.П000220006000.Значение;
		КонецЕсли;

		СтандартнаяОбработка = Ложь;
		
		МаксДлина = 3;

		Если СтруктураРеквизитовФормы.мАвтоВыборКодов Тогда

			ДополнительныеПараметры = Новый Структура("ПервоначальноеЗначение", ПервоначальноеЗначение);
			ОписаниеОповещения = Новый ОписаниеОповещения("ВвестиСтрокуКодОКЕИЗавершение", ЭтотОбъект, ДополнительныеПараметры);
			ПоказатьВводСтроки(ОписаниеОповещения, ПервоначальноеЗначение, "Код ОКЕИ", МаксДлина);

		Иначе

			ПараметрыФормы = Новый Структура;
			ПараметрыФормы.Вставить("Заголовок",          "Выбор кода");
			ПараметрыФормы.Вставить("ТаблицаЗначений",    мОКЕИ);
			ПараметрыФормы.Вставить("СтруктураДляПоиска", Новый Структура(ПоисковаяКолонка, ПервоначальноеЗначение));
			
			ОписаниеОповещения = Новый ОписаниеОповещения("ОткрытьФормуВыбораКодаОКЕИРаздел22Завершение", ЭтотОбъект);
			ОткрытьФорму("Обработка.ОбщиеОбъектыРеглОтчетности.Форма.ФормаВыбораЗначенияИзТаблицы", ПараметрыФормы, ЭтаФорма,,,, ОписаниеОповещения, РежимОткрытияОкнаФормы.БлокироватьОкноВладельца);

		КонецЕсли;

	ИначеЕсли ИмяЯчейки = "П000220008000" Тогда
        		
		РасчетНаКлиенте("Раздел22", ИмяЯчейки);
	                
	// Ставка
	ИначеЕсли ИмяЯчейки = "П000220009000" Тогда
        		
		РасчетНаКлиенте("Раздел22", ИмяЯчейки);

	ИначеЕсли (Область.Имя = "П000220005000") И (НЕ СтруктураРеквизитовФормы.мАвтоВыборКодов) Тогда

		СтандартнаяОбработка = Ложь;

		ОпределениеСтроки = "";

		ПараметрыФормы = Новый Структура;
		ПараметрыФормы.Вставить("НачальноеЗначениеВыбора", 	ПервоначальноеЗначение);
		
		ФормаВводаЛицензии = ПолучитьФорму("Отчет.РегламентированныйОтчетНедра.Форма.ФормаВводаЛицензии", ПараметрыФормы);
		ДополнительныеПараметры = Новый Структура("Область, ФормаВводаЛицензии", Область, ФормаВводаЛицензии);
		ОписаниеОповещения = Новый ОписаниеОповещения("ОткрытьФормуВводаЛицензииРаздел2Завершение", ЭтотОбъект, ДополнительныеПараметры);
		ФормаВводаЛицензии.ОписаниеОповещенияОЗакрытии = ОписаниеОповещения;
		ФормаВводаЛицензии.РежимОткрытияОкна = РежимОткрытияОкнаФормы.БлокироватьОкноВладельца;
		ФормаВводаЛицензии.Открыть();
		
	КонецЕсли;
	 	
КонецПроцедуры

