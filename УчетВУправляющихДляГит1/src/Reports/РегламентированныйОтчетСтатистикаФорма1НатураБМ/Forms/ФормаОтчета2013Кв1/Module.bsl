			ОбластьДляВвода = ПолеТабличногоДокументаФормаОтчета.Области["П000005000103_" + НомерСтроки];
			ИмяСмежнойОбласти = ПолеТабличногоДокументаФормаОтчета.Области["П000005000101_" + НомерСтроки].Имя;
			ОбластиДляОбнуления.Добавить(ПолеТабличногоДокументаФормаОтчета.Области["П000005000102_" + НомерСтроки]);
			ОбластиДляОбнуления.Добавить(ПолеТабличногоДокументаФормаОтчета.Области["П000005000104_" + НомерСтроки]);
