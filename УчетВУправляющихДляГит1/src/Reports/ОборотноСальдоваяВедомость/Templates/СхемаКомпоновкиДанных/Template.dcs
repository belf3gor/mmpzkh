				<dataPath>КонтрольНачальныйРазвернутыйОстатокКт</dataPath>
				<field>КонтрольНачальныйРазвернутыйОстатокКт</field>
				<title xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>ru</v8:lang>
						<v8:content>Контроль начальный развернутый остаток кт</v8:content>
					</v8:item>
				</title>
			</field>
			<dataSource>ИсточникДанных1</dataSource>
			<query>//Начало ЗапросПоСубконто РазвернутоеСальдо
ВЫБРАТЬ
	ВложенныйЗапрос.Счет,
	//Null КАК ВложенныйЗапрос.Субконто1,
	//Null КАК ВложенныйЗапрос.Субконто2,
	//Null КАК ВложенныйЗапрос.Субконто3,
	ВложенныйЗапрос.Валюта,
	ВложенныйЗапрос.БУНачальныйРазвернутыйОстатокДт,
	ВложенныйЗапрос.БУНачальныйРазвернутыйОстатокКт,
	ВложенныйЗапрос.БУКонечныйРазвернутыйОстатокДт,
	ВложенныйЗапрос.БУКонечныйРазвернутыйОстатокКт,
	ВложенныйЗапрос.НУНачальныйРазвернутыйОстатокДт,
	ВложенныйЗапрос.НУНачальныйРазвернутыйОстатокКт,
	ВложенныйЗапрос.НУКонечныйРазвернутыйОстатокДт,
	ВложенныйЗапрос.НУКонечныйРазвернутыйОстатокКт,
	ВложенныйЗапрос.ПРНачальныйРазвернутыйОстатокДт,
	ВложенныйЗапрос.ПРНачальныйРазвернутыйОстатокКт,
	ВложенныйЗапрос.ПРКонечныйРазвернутыйОстатокДт,
	ВложенныйЗапрос.ПРКонечныйРазвернутыйОстатокКт,
	ВложенныйЗапрос.ВРНачальныйРазвернутыйОстатокДт,
	ВложенныйЗапрос.ВРНачальныйРазвернутыйОстатокКт,
	ВложенныйЗапрос.ВРКонечныйРазвернутыйОстатокДт,
	ВложенныйЗапрос.ВРКонечныйРазвернутыйОстатокКт,
	ВложенныйЗапрос.КонтрольНачальныйРазвернутыйОстатокДт,
	ВложенныйЗапрос.КонтрольКонечныйРазвернутыйОстатокДт,
	ВложенныйЗапрос.КонтрольНачальныйРазвернутыйОстатокКт,
	ВложенныйЗапрос.КонтрольКонечныйРазвернутыйОстатокКт,
	ВложенныйЗапрос.Показатель,
	ВложенныйЗапрос.ЭтоРазвернутоеСальдо
ИЗ
	(ВЫБРАТЬ
		ОстаткиИОбороты.Счет КАК Счет,
		//Null КАК Субконто1,
		//Null КАК Субконто2,
		//Null КАК Субконто3,
		ОстаткиИОбороты.Валюта КАК Валюта,
		ОстаткиИОбороты.СуммаНачальныйРазвернутыйОстатокДт КАК БУНачальныйРазвернутыйОстатокДт,
		ОстаткиИОбороты.СуммаНачальныйРазвернутыйОстатокКт КАК БУНачальныйРазвернутыйОстатокКт,
		ОстаткиИОбороты.СуммаКонечныйРазвернутыйОстатокДт КАК БУКонечныйРазвернутыйОстатокДт,
		ОстаткиИОбороты.СуммаКонечныйРазвернутыйОстатокКт КАК БУКонечныйРазвернутыйОстатокКт,
		ОстаткиИОбороты.СуммаНУНачальныйРазвернутыйОстатокДт КАК НУНачальныйРазвернутыйОстатокДт,
		ОстаткиИОбороты.СуммаНУНачальныйРазвернутыйОстатокКт КАК НУНачальныйРазвернутыйОстатокКт,
		ОстаткиИОбороты.СуммаНУКонечныйРазвернутыйОстатокДт КАК НУКонечныйРазвернутыйОстатокДт,
		ОстаткиИОбороты.СуммаНУКонечныйРазвернутыйОстатокКт КАК НУКонечныйРазвернутыйОстатокКт,
		ОстаткиИОбороты.СуммаПРНачальныйРазвернутыйОстатокДт КАК ПРНачальныйРазвернутыйОстатокДт,
		ОстаткиИОбороты.СуммаПРНачальныйРазвернутыйОстатокКт КАК ПРНачальныйРазвернутыйОстатокКт,
		ОстаткиИОбороты.СуммаПРКонечныйРазвернутыйОстатокДт КАК ПРКонечныйРазвернутыйОстатокДт,
		ОстаткиИОбороты.СуммаПРКонечныйРазвернутыйОстатокКт КАК ПРКонечныйРазвернутыйОстатокКт,
		ОстаткиИОбороты.СуммаВРНачальныйРазвернутыйОстатокДт КАК ВРНачальныйРазвернутыйОстатокДт,
		ОстаткиИОбороты.СуммаВРНачальныйРазвернутыйОстатокКт КАК ВРНачальныйРазвернутыйОстатокКт,
		ОстаткиИОбороты.СуммаВРКонечныйРазвернутыйОстатокДт КАК ВРКонечныйРазвернутыйОстатокДт,
		ОстаткиИОбороты.СуммаВРКонечныйРазвернутыйОстатокКт КАК ВРКонечныйРазвернутыйОстатокКт,
		ВЫБОР
			КОГДА ОстаткиИОбороты.Счет.НалоговыйУчет
				ТОГДА ЕСТЬNULL(ОстаткиИОбороты.СуммаНачальныйРазвернутыйОстатокДт, 0) - ЕСТЬNULL(ОстаткиИОбороты.СуммаНУНачальныйРазвернутыйОстатокДт, 0) - ЕСТЬNULL(ОстаткиИОбороты.СуммаПРНачальныйРазвернутыйОстатокДт, 0) - ЕСТЬNULL(ОстаткиИОбороты.СуммаВРНачальныйРазвернутыйОстатокДт, 0)
			ИНАЧЕ 0
		КОНЕЦ КАК КонтрольНачальныйРазвернутыйОстатокДт,
		ВЫБОР
			КОГДА ОстаткиИОбороты.Счет.НалоговыйУчет
				ТОГДА ЕСТЬNULL(ОстаткиИОбороты.СуммаКонечныйРазвернутыйОстатокДт, 0) - ЕСТЬNULL(ОстаткиИОбороты.СуммаНУКонечныйРазвернутыйОстатокДт, 0) - ЕСТЬNULL(ОстаткиИОбороты.СуммаПРКонечныйРазвернутыйОстатокДт, 0) - ЕСТЬNULL(ОстаткиИОбороты.СуммаВРКонечныйРазвернутыйОстатокДт, 0)
			ИНАЧЕ 0
		КОНЕЦ КАК КонтрольКонечныйРазвернутыйОстатокДт,
		ВЫБОР
			КОГДА ОстаткиИОбороты.Счет.НалоговыйУчет
				ТОГДА ЕСТЬNULL(ОстаткиИОбороты.СуммаНачальныйРазвернутыйОстатокКт, 0) - ЕСТЬNULL(ОстаткиИОбороты.СуммаНУНачальныйРазвернутыйОстатокКт, 0) - ЕСТЬNULL(ОстаткиИОбороты.СуммаПРНачальныйРазвернутыйОстатокКт, 0) - ЕСТЬNULL(ОстаткиИОбороты.СуммаВРНачальныйРазвернутыйОстатокКт, 0)
			ИНАЧЕ 0
		КОНЕЦ КАК КонтрольНачальныйРазвернутыйОстатокКт,
		ВЫБОР
			КОГДА ОстаткиИОбороты.Счет.НалоговыйУчет
				ТОГДА ЕСТЬNULL(ОстаткиИОбороты.СуммаКонечныйРазвернутыйОстатокКт, 0) - ЕСТЬNULL(ОстаткиИОбороты.СуммаНУКонечныйРазвернутыйОстатокКт, 0) - ЕСТЬNULL(ОстаткиИОбороты.СуммаПРКонечныйРазвернутыйОстатокКт, 0) - ЕСТЬNULL(ОстаткиИОбороты.СуммаВРКонечныйРазвернутыйОстатокКт, 0)
			ИНАЧЕ 0
		КОНЕЦ КАК КонтрольКонечныйРазвернутыйОстатокКт,
		1 КАК Показатель,
		ИСТИНА КАК ЭтоРазвернутоеСальдо
	ИЗ
		РегистрБухгалтерии.Хозрасчетный.ОстаткиИОбороты(
				{(&amp;НачалоПериода)},
				{(&amp;КонецПериода)},
				,
				, //Начало УсловиеСчета РазвернутоеСальдо
				Счет В ИЕРАРХИИ (&amp;СчетРазвернутоеСальдо)
					И НЕ Счет В (&amp;СчетаИсключенныеИзЗапросаПоСчетамРазвернутое)
					И (НЕ Счет.Забалансовый
						ИЛИ &amp;ВыводитьЗабалансовыеСчета)
				  //Конец УсловиеСчета РазвернутоеСальдо
