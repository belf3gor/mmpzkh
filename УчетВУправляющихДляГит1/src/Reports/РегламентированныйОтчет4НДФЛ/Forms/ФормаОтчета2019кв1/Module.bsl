#Область ОбработчикиСобытийТабличногоДокумента

&НаКлиенте
Процедура ОбработкаВыбораВПолеТабличногоДокумента(Элемент, Область, СтандартнаяОбработка)
	
	Если РегламентированнаяОтчетностьКлиент.ВыбраннаяОбластьРегламентированногоОтчетаНеВерна(ЭтотОбъект, Область, СтандартнаяОбработка) Тогда
		Возврат;
	КонецЕсли;
	
	ИмяРаздела = "Титульный";
	ИмяЯчейки = Область.Имя;
	
	//специальная обработка некоторых областей
	Если Лев(Область.Имя, 7) = "КодИМНС" Тогда
		СтандартнаяОбработка = Ложь;
		Модифицированность   = Истина;
		ОбработкаВыбораИФНС();
		Возврат;
	КонецЕсли;
	Если Лев(Область.Имя, 4) = "ИНН1" Тогда
		СтандартнаяОбработка = Ложь;
		Модифицированность   = Истина;
		ОбработкаВыбораИННФизЛица(Область);
		
		Возврат;
	КонецЕсли;
	
	Если Лев(Область.Имя, 5) = "ОКАТО" Тогда
		Возврат;
	КонецЕсли;
	
	ПараметрВводаПоля = РегламентированнаяОтчетностьКлиентСервер.НайтиСвойстваПоказателя(ЭтотОбъект, ИмяЯчейки);
	
	//создание динамического списка выбора
	Если ПараметрВводаПоля <> Неопределено И ПараметрВводаПоля.ТаблицаЗначений <> Неопределено Тогда
		//обработка таблицы значений - удаление вариантов, не проходящих по условиям
		КодПоказателя = ПараметрВводаПоля.Показатель;
		
		ВвестиПоказатель(Элемент, Область, КодПоказателя, Ложь, ПараметрВводаПоля.ТекстПриВыборе, ПараметрВводаПоля.ТаблицаЗначений, СтандартнаяОбработка);
		
	КонецЕсли;
	
КонецПроцедуры

&НаКлиенте
Процедура ОбработкаВыбораИННФизЛица(Область)
	
	ИНН = СтрЗаменить(Область.Значение, "-", "");
	ДополнительныеПараметры = Новый Структура("Область, ИНН", Область, ИНН);
	ОписаниеОповещения = Новый ОписаниеОповещения("ОбработкаВыбораИННФизЛицаЗавершение", ЭтотОбъект, ДополнительныеПараметры);
	ПоказатьВводСтроки(ОписаниеОповещения, ИНН, "Укажите ИНН налогоплательщика", 12, Ложь);
	
КонецПроцедуры

&НаКлиенте
Процедура ОбработкаВыбораИННФизЛицаЗавершение(Строка, ДополнительныеПараметры) Экспорт
	
	Область = ДополнительныеПараметры.Область;
	ИНН = ?(Строка = Неопределено, ДополнительныеПараметры.ИНН, Строка);
	
	
	Если НЕ (Строка <> Неопределено) Тогда
		Возврат;
	КонецЕсли;
	
	Если НЕ СтроковыеФункцииКлиентСервер.ТолькоЦифрыВСтроке(СокрЛП(ИНН)) Тогда
		ПоказатьПредупреждение(,НСтр("ru = 'В ИНН должны быть указаны только цифры!'"),, Заголовок );
		Возврат;
	КонецЕсли; 
	
	// заполняем ИНН
	ИНН = СокрЛП(ИНН);
	Для Инд = СтрДлина(ИНН) + 1 По 12 Цикл
		ИНН = ИНН + "-";
	КонецЦикла;
	
	СохранитьЗначениеПоказателяВСтруктуреНаСервере("Титульный" , "ИНН1", ИНН);
	Область.Значение = СокрЛП(ИНН);
	
КонецПроцедуры

&НаКлиенте
Процедура ОбработкаИзмененияСодержимогоВПолеТабличногоДокумента(Элемент, Область)
	
	Модифицированность = Истина;
	
	ПараметрВводаПоля = СвойстваПоказателей.НайтиСтроки(Новый Структура("Показатель", Область.Имя));
	
	ИмяПоказателя = Область.Имя;
	ЗначПоказателя = Область.Значение;
	ИмяРаздела = "Титульный";
	
	
	Модифицированность = Истина;
	
	ПараметрВводаПоля = СвойстваПоказателей.НайтиСтроки(Новый Структура("Показатель", Область.Имя));
	
	Если ПараметрВводаПоля.Количество() > 0 Тогда
		
		ПараметрВводаПоля = ПараметрВводаПоля[0];
				
		Если ПараметрВводаПоля.Тип = "КБК" И ЗначениеЗаполнено(Область.Значение) Тогда
			Область.Значение = СтрЗаменить(Область.Значение," ","");
		КонецЕсли;
			
		Если СтрДлина(СокрЛП(Область.Значение)) <> 0 И ПараметрВводаПоля.Длина <> 0 И СтрДлина(СокрЛП(Область.Значение)) <> ПараметрВводаПоля.Длина Тогда
			
			Сообщение = Новый СообщениеПользователю;
			
			Сообщение.Текст = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(НСтр("ru='Длина значения показателя должна быть равна %1!'"), ПараметрВводаПоля.Длина);
			
			Сообщение.Сообщить();
			
		КонецЕсли;
		
	КонецЕсли;
		
	//Изменение "по месту нахождения (учета) (код)" на Титульном листе
	Если СтрНайти(ИмяПоказателя, "ПоМесту") > 0 Тогда
		
		НовыйПоМесту = РегламентированнаяОтчетностьКлиентСервер.ВернутьЗначениеПараметраСЛиста(ЭтотОбъект, "Титульный", "ПоМесту");
		
		ТабличныйДокумент.Области.ПоМесту.Значение = НовыйПоМесту;
	ИначеЕсли ИмяПоказателя = "ДатаПодписи" Тогда
		
		ДатаПодписи = ЗначПоказателя;
		
	ИначеЕсли ИмяПоказателя = "НомерКорректировки" Тогда
		
		НомерКорректировки = ЗначПоказателя;
		СтруктураРеквизитовФормы.ВидДокумента = ?(НомерКорректировки > 0, 1, 0);
		
	КонецЕсли;
	
КонецПроцедуры

