// Процедура очищает содержимое редактируемых и вычисляемых ячеек
// поля табличного документа, переданного параметром ВыбТабличноеПоле.
//
// Параметры:
//  ВыбТабличноеПоле - поле табличного документа.
//
&НаКлиентеНаСервереБезКонтекста
Процедура ОчиститьТабличноеПоле(Форма, ВыбТабличноеПолеИмя) Экспорт
	
	// Предварительно проверим наличие дополнительных строк
	// в заданном поле табличного документа.
	// При наличии таких строк, удаляем их.
	ВыбТабличноеПоле = Форма["ТабличныйДокумент"];
	ИмяГруппыДопСтрок = Форма.ОпределитьИдМногострочногоРазделаПоТабличномуПолю(ВыбТабличноеПолеИмя);
	Если ИмяГруппыДопСтрок <> Неопределено Тогда
		// Поле табличного документа содержит многострочный раздел.
		
		Форма.УдалитьНеобходимоеКоличествоДопСтрокНаСервере(ВыбТабличноеПоле, ВыбТабличноеПолеИмя, ИмяГруппыДопСтрок);

		Форма["ТабличныйДокумент"] = ВыбТабличноеПоле;

	КонецЕсли;
	
	Если ВыбТабличноеПолеИмя = "МПм" Тогда
		
		ОргНазв = Форма.ТабличныйДокумент.Области.ОргНазв.Значение;
		ОргАдрес = Форма.ТабличныйДокумент.Области.ОргАдрес.Значение;
		ОргКодОКПО = Форма.ТабличныйДокумент.Области.ОргКодОКПО.Значение;
		ОргКодОКВЭД = Форма.ТабличныйДокумент.Области.ОргКодОКВЭД.Значение;
		ОргДолжностьИсп = Форма.ТабличныйДокумент.Области.ОргДолжностьИсп.Значение;
		ОргИсполнитель = Форма.ТабличныйДокумент.Области.ОргИсполнитель.Значение;
		ОргТелефонИсп = Форма.ТабличныйДокумент.Области.ОргТелефонИсп.Значение;
		ОргКодНазв = Форма.ТабличныйДокумент.Области.ОргКодНазв.Значение;
		ОргКодНазв1 = Форма.ТабличныйДокумент.Области.ОргКодНазв1.Значение;
		ОргКодЗнач = Форма.ТабличныйДокумент.Области.ОргКодЗнач.Значение;
		ОргКодЗнач1 = Форма.ТабличныйДокумент.Области.ОргКодЗнач1.Значение;
		ОргАдресЭлектроннойПочты = Форма.ТабличныйДокумент.Области.ОргАдресЭлектроннойПочты.Значение;
		ДатаСоставленияОтчета = Форма.ТабличныйДокумент.Области.ДатаСоставленияОтчета.Значение;
		НомерКорректировки = Форма.ТабличныйДокумент.Области.НомерКорректировки.Значение;
		
	Иначе
		
		ОргНазв = Форма.СтруктураДанныхМПм.ОргНазв;
		ОргАдрес = Форма.СтруктураДанныхМПм.ОргАдрес;
		ОргКодОКПО =Форма.СтруктураДанныхМПм.ОргКодОКПО;
		ОргКодОКВЭД = Форма.СтруктураДанныхМПм.ОргКодОКВЭД;
		ОргДолжностьИсп = Форма.СтруктураДанныхМПм.ОргДолжностьИсп;
		ОргИсполнитель = Форма.СтруктураДанныхМПм.ОргИсполнитель;
		ОргТелефонИсп = Форма.СтруктураДанныхМПм.ОргТелефонИсп;
		ОргКодНазв = Форма.СтруктураДанныхМПм.ОргКодНазв;
		ОргКодНазв1 = Форма.СтруктураДанныхМПм.ОргКодНазв1;
		ОргКодЗнач = Форма.СтруктураДанныхМПм.ОргКодЗнач;
		ОргКодЗнач1 = Форма.СтруктураДанныхМПм.ОргКодЗнач1;
		ОргАдресЭлектроннойПочты = Форма.СтруктураДанныхМПм.ОргАдресЭлектроннойПочты;
		ДатаСоставленияОтчета = Форма.СтруктураДанныхМПм.ДатаСоставленияОтчета;
		НомерКорректировки = Форма.СтруктураДанныхМПм.НомерКорректировки;
		
	КонецЕсли; 
	
	
	// Непосредственно очищаем ячейки табличного документа.
	Для Инд = 0 По ВыбТабличноеПоле.Области.Количество() - 1 Цикл
		ТекущаяОбласть = ВыбТабличноеПоле.Области[Инд];

		Если НЕ Тип(ТекущаяОбласть) = Тип("РисунокТабличногоДокумента") И Не ТекущаяОбласть.ТипОбласти = ТипОбластиЯчеекТабличногоДокумента.Прямоугольник Тогда
			Продолжить;
		КонецЕсли;

		Если ТекущаяОбласть.СодержитЗначение <> Истина Тогда
			Продолжить;
		КонецЕсли;

		Если ТекущаяОбласть.Защита Тогда
			Продолжить;
		КонецЕсли;

		Если Тип(ТекущаяОбласть) = Тип("РисунокТабличногоДокумента") Тогда
			ТекущаяОбласть.Значение = "";
		Иначе
			ТекущаяОбласть.Очистить();
			ТекущаяОбласть.Формат = РегламентированнаяОтчетностьКлиентСервер.ЗаменитьПредставлениеНуляВСтрокеФормата(ТекущаяОбласть.Формат, "ЧН=' '");
		КонецЕсли;
		
	КонецЦикла;

	ИмяСтраницыПанели = ВыбТабличноеПолеИмя;
	
	#Если НаКлиенте Тогда
		Форма.ОбнулитьПеременныеМодуляХранящиеЗначенияКвадратногоПредставленияПоРазделуНаКлиенте(ИмяСтраницыПанели);
	#Иначе
		Форма.ОбнулитьПеременныеМодуляХранящиеЗначенияКвадратногоПредставленияПоРазделуНаСервере(ИмяСтраницыПанели);
	#КонецЕсли
		
	Если ИмяГруппыДопСтрок <> Неопределено Тогда
		ОбновитьСтруктуруМногострочногоРаздела(Форма, ИмяГруппыДопСтрок);
	КонецЕсли;
	
	Форма.СтруктураДанныхМПм.ОргНазв = ОргНазв;
	Форма.СтруктураДанныхМПм.ОргАдрес = ОргАдрес;
	Форма.СтруктураДанныхМПм.ОргКодОКПО = ОргКодОКПО;
	Форма.СтруктураДанныхМПм.ОргКодОКВЭД = ОргКодОКВЭД;
	Форма.СтруктураДанныхМПм.ОргДолжностьИсп = ОргДолжностьИсп;
	Форма.СтруктураДанныхМПм.ОргИсполнитель = ОргИсполнитель;
	Форма.СтруктураДанныхМПм.ОргТелефонИсп = ОргТелефонИсп;
	Форма.СтруктураДанныхМПм.ОргКодНазв = ОргКодНазв;
	Форма.СтруктураДанныхМПм.ОргКодНазв1 = ОргКодНазв1;
	Форма.СтруктураДанныхМПм.ОргКодЗнач = ОргКодЗнач;
	Форма.СтруктураДанныхМПм.ОргКодЗнач1 = ОргКодЗнач1;
	Форма.СтруктураДанныхМПм.ОргАдресЭлектроннойПочты = ОргАдресЭлектроннойПочты;
	Форма.СтруктураДанныхМПм.ДатаСоставленияОтчета = ДатаСоставленияОтчета;
	Форма.СтруктураДанныхМПм.Вставить("НомерКорректировки", НомерКорректировки);
	Форма.СтруктураДанныхПриложение3.ОргНазв = ОргНазв;
	Форма.СтруктураДанныхПриложение3.ОргКодОКПО = ОргКодОКПО;
	
	Если ИмяСтраницыПанели = "МПм" Тогда
		Форма.ТабличныйДокумент.Области.ОргНазв.Значение = ОргНазв;
		Форма.ТабличныйДокумент.Области.ОргАдрес.Значение = ОргАдрес;
		Форма.ТабличныйДокумент.Области.ОргКодОКПО.Значение = ОргКодОКПО;
		Форма.ТабличныйДокумент.Области.ОргКодОКВЭД.Значение = ОргКодОКВЭД;
		Форма.ТабличныйДокумент.Области.ОргДолжностьИсп.Значение = ОргДолжностьИсп;
		Форма.ТабличныйДокумент.Области.ОргИсполнитель.Значение = ОргИсполнитель;
		Форма.ТабличныйДокумент.Области.ОргТелефонИсп.Значение = ОргТелефонИсп;
		Форма.ТабличныйДокумент.Области.ОргКодНазв.Значение = ОргКодНазв;
		Форма.ТабличныйДокумент.Области.ОргКодНазв1.Значение = ОргКодНазв1;
		Форма.ТабличныйДокумент.Области.ОргКодЗнач.Значение = ОргКодЗнач;
		Форма.ТабличныйДокумент.Области.ОргКодЗнач1.Значение = ОргКодЗнач1;
		Форма.ТабличныйДокумент.Области.ОргАдресЭлектроннойПочты.Значение = ОргАдресЭлектроннойПочты;
		Форма.ТабличныйДокумент.Области.ДатаСоставленияОтчета.Значение = ДатаСоставленияОтчета;
		Форма.ТабличныйДокумент.Области.НомерКорректировки.Значение = НомерКорректировки;
		
		Для Инд = 5 По 28 Цикл 
			Если Инд = 11 Или Инд = 17 Тогда 
				Продолжить;
			КонецЕсли;
			Форма.ТабличныйДокумент.Области["П00001000" + Формат(Инд, "ЧЦ=2; ЧВН=") + "003"].Значение = Ложь;
		КонецЦикла;
	ИначеЕсли ИмяСтраницыПанели = "Приложение3" Тогда
		Форма.ТабличныйДокумент.Области.ОргНазв.Значение = ОргНазв;
		Форма.ТабличныйДокумент.Области.ОргКодОКПО.Значение = ОргКодОКПО;
	КонецЕсли;
	
	Расчет(Форма, ИмяСтраницыПанели);
	
КонецПроцедуры

&НаСервере
Процедура УдалитьНеобходимоеКоличествоДопСтрокНаСервере(ВыбТабличноеПоле, ВыбТабличноеПолеИмя, ИмяГруппыДопСтрок) Экспорт
	
	// Получим таблицу значений, содержащую данные дополнительных строк.
	ПромТаблицаДопСтрокРаздела = Неопределено;
	мСтруктураМногострочныхРазделов.Свойство(ИмяГруппыДопСтрок, ПромТаблицаДопСтрокРаздела);
	
	ПромТаблицаДопСтрокРаздела = ЭтаФорма[ПромТаблицаДопСтрокРаздела];
	
	ИсходноеКоличествоСтрокГруппы = 0;
	мСтруктураИсхКолвоСтрокРазделов.Свойство(ИмяГруппыДопСтрок, ИсходноеКоличествоСтрокГруппы);
	
	ТекущееКоличествоСтрок = ПромТаблицаДопСтрокРаздела.Количество();
	
	Если ТекущееКоличествоСтрок > ИсходноеКоличествоСтрокГруппы Тогда
		// Имеются дополнительные строки.
		// Удаляем их.
		
		НСтр = ПромТаблицаДопСтрокРаздела.Количество();
		
		Пока НСтр > ИсходноеКоличествоСтрокГруппы Цикл
			
			ТекСтрокаТаблицы = ПромТаблицаДопСтрокРаздела[НСтр - 1];
			
			ИмяПервойКолонки = ИмяГруппыДопСтрок + "01_";
			ИмяПервойЯчейкиУдаляемойСтроки  = ИмяПервойКолонки + НСтр;
			
			// Область первой ячейки удаляемой строки многострочного раздела.
			ОбластьЯчейки = ВыбТабличноеПоле.Области[ИмяПервойЯчейкиУдаляемойСтроки];
			
			НомерВерхЯчейкиОбласти = ОбластьЯчейки.Верх;
			НомерНижнЯчейкиОбласти = ОбластьЯчейки.Низ;
			
			РегламентированнаяОтчетностьВызовСервера.СпециальногоУдаленияСтроки(ВыбТабличноеПоле, ВыбТабличноеПолеИмя, ИмяГруппыДопСтрок, НомерВерхЯчейкиОбласти, НомерНижнЯчейкиОбласти);
			
			// Удаляем из структуры многострочного раздела соответствующую строку.
			ПромТаблицаДопСтрокРаздела.Удалить(ТекСтрокаТаблицы);
			
			НСтр = НСтр - 1;
			
		КонецЦикла;
		
		Если ВыбТабличноеПоле.Области.Найти("МногострочнаяЧасть") = Неопределено Тогда
			
			ВыбТабличноеПоле.Область(НомерВерхЯчейкиОбласти - 1, , НомерНижнЯчейкиОбласти - 1).Имя = "МногострочнаяЧасть";
			
		КонецЕсли;
		
	КонецЕсли;
	
КонецПроцедуры

&НаСервере
Процедура ЗаполнитьСведенияОбОрганизацииНаСервере(ВПрограммеИзмененаДатаПодписи = Ложь)
	
	ЗаполнитьСведенияОбОрганизации(ЭтаФорма, ВПрограммеИзмененаДатаПодписи);
	
КонецПроцедуры

// Процедура управляет выводом сведений о выбранной организации
// в соответствующие ячейки табличного документа.
//
// Параметры:
// ВПрограммеИзмененаДатаПодписи - Истина, если процедура запускается для обновления параметров завясящих
// от даты подписания документа, например ФИО должностных лиц
//
&НаКлиентеНаСервереБезКонтекста
Процедура ЗаполнитьСведенияОбОрганизации(Форма, ВПрограммеИзмененаДатаПодписи = Ложь, НеобходимоПерезаполнить10раздел = Ложь)
	
	Если (Форма.СтруктураРеквизитовФормы.Организация = Неопределено)
	 Или (Форма.СтруктураРеквизитовФормы.Организация = РегламентированнаяОтчетностьКлиентСервер.ПустоеЗначениеТипа("СправочникСсылка.Организации")) Тогда
		Возврат;
	КонецЕсли;
	
	Сведения = Новый СписокЗначений;
	
	Если НЕ ВПрограммеИзмененаДатаПодписи Тогда
		Сведения.Добавить("", "ИННЮЛ");
		Сведения.Добавить("", "ОКПО");
		Сведения.Добавить("", "АдрПочт");
		Сведения.Добавить("", "НаимЮЛПол");
		Сведения.Добавить("", "ОснВидДеят");
		Сведения.Добавить("", "ОКВЭД2");
		Сведения.Добавить("", "ОКПО");
		Сведения.Добавить("", "ОКАТО");
	КонецЕсли;
	
	РегламентированнаяОтчетностьВызовСервера.ДополнитьЗаправшиваемыеСведенияОбОрганизации(Форма.СтруктураРеквизитовФормы.Организация, Сведения);
	ОргСведения = РегламентированнаяОтчетностьВызовСервера.ПолучитьСведенияОбОрганизации(
		Форма.СтруктураРеквизитовФормы.Организация, Форма.ДатаПодписи, Сведения);
	
	Если НЕ ВПрограммеИзмененаДатаПодписи Тогда
		ИННЮЛ      = "";
		ОКПО       = "";
		АдрПочт    = "";
		НаимЮЛПол  = "";
		ОснВидДеят = "";
		ОКВЭД      = "";
		ОКПО       = "";
		ОКАТО      = "";
	КонецЕсли;
	
	ДолжнИсп        = "";
	ФИОИсп          = "";
	ТелИсп          = "";
	АдресЭлПочтыИсп = "";
	
	Если НЕ ВПрограммеИзмененаДатаПодписи Тогда
		ОргСведения.Свойство("ИННЮЛ",     ИННЮЛ);
		ОргСведения.Свойство("ОКПО",      ОКПО);
		ОргСведения.Свойство("АдрПочт",   АдрПочт);
		ОргСведения.Свойство("НаимЮЛПол", НаимЮЛПол);
		ОргСведения.Свойство("ОКВЭД2",    ОКВЭД);
		ОргСведения.Свойство("ОКПО",      ОКПО);
		ОргСведения.Свойство("ОКАТО",     ОКАТО);
	КонецЕсли;
	
	РегламентированнаяОтчетностьВызовСервера.ДополнитьПодписантаСтатистическогоОтчета(ФИОИсп, ДолжнИсп, ТелИсп, АдресЭлПочтыИсп, ОргСведения, Форма.СтруктураРеквизитовФормы.Организация);
	
	Если ВПрограммеИзмененаДатаПодписи Тогда
		Форма.ТабличныйДокумент.Области.ОргИсполнитель.Значение           = ФИОИсп;
		Форма.ТабличныйДокумент.Области.ОргДолжностьИсп.Значение          = ДолжнИсп;
		Форма.ТабличныйДокумент.Области.ОргТелефонИсп.Значение            = ТелИсп;
		Форма.ТабличныйДокумент.Области.ОргАдресЭлектроннойПочты.Значение = АдресЭлПочтыИсп;
		Возврат;
	КонецЕсли;
	
	Если (НЕ ЗначениеЗаполнено(Форма.ТабличныйДокумент.Области.ОргКодОКПО.Значение))
		ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
		Если НЕ РегламентированнаяОтчетностьВызовСервера.ЭтоЮридическоеЛицо(Форма.СтруктураРеквизитовФормы.Организация) Тогда
			Форма.ТабличныйДокумент.Области.ОргКодОКПО.Значение = "";
		Иначе
			Форма.ТабличныйДокумент.Области.ОргКодОКПО.Значение = ОКПО;
		КонецЕсли;
	КонецЕсли;
	
	Если (НЕ ЗначениеЗаполнено(Форма.ТабличныйДокумент.Области.ОргКодОКВЭД.Значение))
		ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
		Если НЕ РегламентированнаяОтчетностьВызовСервера.ЭтоЮридическоеЛицо(Форма.СтруктураРеквизитовФормы.Организация) Тогда
			Форма.ТабличныйДокумент.Области.ОргКодОКВЭД.Значение = "";
		Иначе
			Форма.ТабличныйДокумент.Области.ОргКодОКВЭД.Значение = ОКВЭД;
		КонецЕсли;
	КонецЕсли;
	
	Если (НЕ ЗначениеЗаполнено(Форма.ТабличныйДокумент.Области.ОргНазв.Значение))
		ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
		Форма.ТабличныйДокумент.Области.ОргНазв.Значение = НаимЮЛПол;
	КонецЕсли;
	
	Если (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация)
		Или (ПустаяСтрока(Форма.ТабличныйДокумент.Области.ОргАдрес.Значение)) Тогда
		Если (ЗначениеЗаполнено(СтрЗаменить(АдрПочт, ",", ""))) Тогда
			
			Форма.ТабличныйДокумент.Области.ОргАдрес.Значение
				= РегламентированнаяОтчетностьКлиентСервер.ПредставлениеАдресаВФормате9Запятых(АдрПочт);
			Форма.СтруктураРеквизитовФормы.мАдресвФорматеДляВыгрузки = АдрПочт;
		Иначе
			Форма.ТабличныйДокумент.Области.ОргАдрес.Значение = "";
			Форма.СтруктураРеквизитовФормы.мАдресвФорматеДляВыгрузки = "";
		КонецЕсли;
		
	КонецЕсли;
	
	Если (НЕ ЗначениеЗаполнено(Форма.ТабличныйДокумент.Области.ОргИсполнитель.Значение))
		ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
		Форма.ТабличныйДокумент.Области.ОргИсполнитель.Значение = ФИОИсп;
	КонецЕсли;
	
	Если (НЕ ЗначениеЗаполнено(Форма.ТабличныйДокумент.Области.ОргДолжностьИсп.Значение))
		ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
		Форма.ТабличныйДокумент.Области.ОргДолжностьИсп.Значение = ДолжнИсп;
	КонецЕсли;
	
	Если (НЕ ЗначениеЗаполнено(Форма.ТабличныйДокумент.Области.ОргТелефонИсп.Значение))
		ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
		Форма.ТабличныйДокумент.Области.ОргТелефонИсп.Значение = ТелИсп;
	КонецЕсли;
	
	Если (НЕ ЗначениеЗаполнено(Форма.ТабличныйДокумент.Области.ОргАдресЭлектроннойПочты.Значение))
		ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
		Форма.ТабличныйДокумент.Области.ОргАдресЭлектроннойПочты.Значение = АдресЭлПочтыИсп;
	КонецЕсли;
	
	СтрОргНазв = Неопределено;
	Если НЕ (Форма.СтруктураДанныхПриложение3.Свойство("ОргНазв", СтрОргНазв) И ЗначениеЗаполнено(СтрОргНазв))
		ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
		Форма.СтруктураДанныхПриложение3.Вставить("ОргНазв", НаимЮЛПол);
	КонецЕсли;
	
	СтрОргКодОКПО = Неопределено;
	Если НЕ (Форма.СтруктураДанныхПриложение3.Свойство("ОргКодОКПО", СтрОргКодОКПО) И ЗначениеЗаполнено(СтрОргКодОКПО))
		ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
		Если НЕ РегламентированнаяОтчетностьВызовСервера.ЭтоЮридическоеЛицо(Форма.СтруктураРеквизитовФормы.Организация) Тогда
			Форма.СтруктураДанныхПриложение3.Вставить("ОргКодОКПО", "");
		Иначе
			Форма.СтруктураДанныхПриложение3.Вставить("ОргКодОКПО", ОКПО);
		КонецЕсли;
	КонецЕсли;
	
	Форма.Модифицированность = Истина;
	
КонецПроцедуры

&НаСервере
Процедура СформироватьСпискиВыбора()
	
	МакетСоставаПоказателей = ОбъектОтчета(ЭтаФорма.ИмяФормы).ПолучитьМакет("Список2013Кв1");
	
	Массив = Новый Массив;
	Массив.Добавить(Тип("Строка"));
	Строка12 = Новый ОписаниеТипов(Массив, , Новый КвалификаторыСтроки(12, ДопустимаяДлина.Переменная));
	Строка64 = Новый ОписаниеТипов(Массив, , Новый КвалификаторыСтроки(64, ДопустимаяДлина.Переменная));
	Строка512 = Новый ОписаниеТипов(Массив, , Новый КвалификаторыСтроки(512, ДопустимаяДлина.Переменная));
	
	КоллекцияСписковВыбора = Новый Соответствие;
	Для Каждого Область Из МакетСоставаПоказателей.Области Цикл
		Если Область.ТипОбласти = ТипОбластиЯчеекТабличногоДокумента.Строки Тогда
			ВерхОбласти = Область.Верх;
			НизОбласти = Область.Низ;
			ТаблСписка = Новый ТаблицаЗначений;
			ТаблСписка.Колонки.Добавить("Код",Строка12,, МакетСоставаПоказателей.Область(ВерхОбласти, 1, ВерхОбласти, 1).ШиринаКолонки);
			ТаблСписка.Колонки.Добавить("Название",Строка512,, МакетСоставаПоказателей.Область(ВерхОбласти, 2, ВерхОбласти, 2).ШиринаКолонки);
			ТаблСписка.Колонки.Добавить("НазваниеДоп",Строка64,, МакетСоставаПоказателей.Область(ВерхОбласти, 3, ВерхОбласти, 3).ШиринаКолонки);
			Для НомСтр = ВерхОбласти По НизОбласти Цикл
				КодПоказателя = СокрП(МакетСоставаПоказателей.Область(НомСтр, 1).Текст);
				Если КодПоказателя <> "###" Тогда
					НовСтрока = ТаблСписка.Добавить();
					НовСтрока.Код = КодПоказателя;
					НовСтрока.Название = СокрП(МакетСоставаПоказателей.Область(НомСтр, 2).Текст);
					НовСтрока.НазваниеДоп = СокрП(МакетСоставаПоказателей.Область(НомСтр, 3).Текст);
				КонецЕсли;
			КонецЦикла;
			КоллекцияСписковВыбора.Вставить(Область.Имя, ТаблСписка);
		КонецЕсли;
	КонецЦикла;
	
	СвойстваПоказателей.Очистить();
	
	Запрос = Новый Запрос;
	Запрос.УстановитьПараметр("ОКПД", КоллекцияСписковВыбора["ОКПД"]);
	Запрос.УстановитьПараметр("ОКПД_55", КоллекцияСписковВыбора["ОКПД_55"]);
	Запрос.УстановитьПараметр("ЕИОКПД", КоллекцияСписковВыбора["ЕИОКПД"]);
	
	Запрос.Текст = 
	"ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ОКПД.Код КАК Код,
	|	ОКПД.Название КАК Название
	|ПОМЕСТИТЬ ВТ_ОКПД
	|ИЗ
	|	&ОКПД КАК ОКПД
	|ИНДЕКСИРОВАТЬ ПО
	|	ОКПД.Код
	|;
	|
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ОКПД_55.Код КАК Код
	|ПОМЕСТИТЬ ВТ_ОКПД_55
	|ИЗ &ОКПД_55 КАК ОКПД_55
	|ИНДЕКСИРОВАТЬ ПО
	|	ОКПД_55.Код
	|;
	|
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ЕИОКПД.Код КАК Код,
	|	ЕИОКПД.Название КАК КодОКПД,
	|	ЕИОКПД.НазваниеДоп КАК Название
	|ПОМЕСТИТЬ ВТ_ЕИОКПД
	|ИЗ &ЕИОКПД КАК ЕИОКПД
	|ИНДЕКСИРОВАТЬ ПО
	|	ЕИОКПД.Название
	|;
	|
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ОКПД.Код КАК Код,
	|	ОКПД.Название КАК Название
	|ИЗ
	|	ВТ_ОКПД_55 КАК ОКПД_55
	|	ЛЕВОЕ СОЕДИНЕНИЕ
	|	ВТ_ОКПД КАК ОКПД
	|	ПО ОКПД_55.Код = ОКПД.Код
	|;
	|
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ЕИОКПД.Код КАК Код,
	|	ЕИОКПД.Название КАК Название,
	|	ЕИОКПД.КодОКПД КАК ФильтрОКПД
	|ИЗ
	|	ВТ_ЕИОКПД КАК ЕИОКПД
	|;
	|
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ЕИОКПД.Код КАК Код,
	|	ЕИОКПД.Название КАК Название
	|ИЗ
	|	ВТ_ЕИОКПД КАК ЕИОКПД";
	
	Результат = Запрос.ВыполнитьПакет();
	ТЗКодыОКПД = Результат[3].Выгрузить();
	ТЗКодыЕдиницИзмерения = Результат[4].Выгрузить();
	ТЗКодыЕдиницИзмеренияНеотфильтрованные = Результат[5].Выгрузить();
	
	НоваяСтрокаТЗВыбора = ТЗКодыЕдиницИзмеренияНеотфильтрованные.Вставить(0);
	НоваяСтрокаТЗВыбора.Код = "";
	НоваяСтрокаТЗВыбора.Название = "Пустое значение";
	
	НоваяСтрокаТЗВыбора = ТЗКодыОКПД.Вставить(0);
	НоваяСтрокаТЗВыбора.Код = "";
	НоваяСтрокаТЗВыбора.Название = "Пустое значение";
	
	НоваяСтрокаТЗВыбора = ТЗКодыЕдиницИзмерения.Вставить(0);
	НоваяСтрокаТЗВыбора.Код = "";
	НоваяСтрокаТЗВыбора.Название = "Пустое значение";
	
	НоваяСтрокаТЗВыбора = КоллекцияСписковВыбора["ОКСМ"].Вставить(0);
	НоваяСтрокаТЗВыбора.Код = "";
	НоваяСтрокаТЗВыбора.Название = "Пустое значение";
	
	РегламентированнаяОтчетность.ДобавитьСтрокуОписанияВвода(СвойстваПоказателей, "ОКСМ",                                   254, , "Выбор страны",         КоллекцияСписковВыбора["ОКСМ"]);
	РегламентированнаяОтчетность.ДобавитьСтрокуОписанияВвода(СвойстваПоказателей, "ТЗКодыОКПД",                             512, , "Выбор кода по ОКПД",   ТЗКодыОКПД);
	РегламентированнаяОтчетность.ДобавитьСтрокуОписанияВвода(СвойстваПоказателей, "ТЗКодыЕдиницИзмерения",                  512, , "Выбор кода по ОКЕИ",   ТЗКодыЕдиницИзмерения);
	РегламентированнаяОтчетность.ДобавитьСтрокуОписанияВвода(СвойстваПоказателей, "ТЗКодыЕдиницИзмеренияНеотфильтрованные", 512, , "Выбор кода по ОКЕИ",   ТЗКодыЕдиницИзмеренияНеотфильтрованные);

КонецПроцедуры

&НаКлиенте
Процедура ВвестиПоказатель(Элемент, Область, КодПоказателя, Знач ЭтоСумма, Знач ТекстВыбора, ТаблицаВыбора, СтандартнаяОбработка, КолонкаПоиска = "Код") Экспорт
	
	Если ТаблицаВыбора = Неопределено ИЛИ СтруктураРеквизитовФормы.мАвтоВыборКодов Тогда
		Возврат;
	КонецЕсли;
	
	СтандартнаяОбработка = Ложь;
	
	ИсходноеЗначение = ?(ТипЗнч(Область.Значение) = Тип("Строка"), СтрЗаменить(Область.Значение, " ", ""), Область.Значение);
	
	ПараметрыФормы = Новый Структура;
	ПараметрыФормы.Вставить("Заголовок",          ТекстВыбора);
	ПараметрыФормы.Вставить("ТаблицаЗначений",    ТаблицаВыбора);
	ПараметрыФормы.Вставить("СтруктураДляПоиска", Новый Структура(КолонкаПоиска, ИсходноеЗначение));
	
	ДополнительныеПараметры = Новый Структура("Область", Область);
	ОписаниеОповещения = Новый ОписаниеОповещения("ВвестиПоказательЗавершение", ЭтотОбъект, ДополнительныеПараметры);
	ОткрытьФорму("Обработка.ОбщиеОбъектыРеглОтчетности.Форма.ФормаВыбораЗначенияИзТаблицы", ПараметрыФормы, ЭтаФорма,,,, ОписаниеОповещения, РежимОткрытияОкнаФормы.БлокироватьОкноВладельца);
	
КонецПроцедуры

&НаКлиенте
Процедура ВвестиПоказательЗавершение(РезультатВыбора, ДополнительныеПараметры) Экспорт
    
    Область = ДополнительныеПараметры.Область;
    
	Если РезультатВыбора = Неопределено Тогда
		Возврат;
	КонецЕсли;
	
	РезультатКод = РезультатВыбора["Код"];
	Область.Значение = РезультатКод;
	РезультатНазвание = ?(РезультатВыбора["Название"] = "Пустое значение" , "", РезультатВыбора["Название"]);
	
	Если СтрНайти(Область.Имя, "П000060000303_") = 1 Тогда
		ОбластьНазвание = СтрЗаменить(Область.Имя, "П000060000303_", "П000060000301_");
		ТабличныйДокумент.Области[ОбластьНазвание].Значение = РезультатНазвание;
	ИначеЕсли СтрНайти(Область.Имя, "П000040000104_") = 1 Тогда
		ОбластьНазвание = СтрЗаменить(Область.Имя, "П000040000104_", "П000040000101_");
		ТабличныйДокумент.Области[ОбластьНазвание].Значение = РезультатНазвание;
		ТабличныйДокумент.Области[СтрЗаменить(Область.Имя, "П000040000104_", "П000040000102_")].Значение = "";
		ТабличныйДокумент.Области[СтрЗаменить(Область.Имя, "П000040000104_", "П000040000103_")].Значение = "";
	ИначеЕсли СтрНайти(Область.Имя, "П000040000103_") = 1 Тогда
		ОбластьНазвание = СтрЗаменить(Область.Имя, "П000040000103_", "П000040000102_");
		ТабличныйДокумент.Области[ОбластьНазвание].Значение = РезультатНазвание;
	КонецЕсли;
	
	Модифицированность = Истина;
	
КонецПроцедуры

