		<field xsi:type="DataSetFieldField">
			<dataPath>ФинансированиеФедеральнымБюджетом</dataPath>
			<field>ФинансированиеФедеральнымБюджетом</field>
		</field>
		<field xsi:type="DataSetFieldField">
