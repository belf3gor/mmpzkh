				</valueType>
			</field>
			<dataSource>ИнформационнаяБаза</dataSource>
			<query>ВЫБРАТЬ
    НачОст.Счет,
	ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ПустаяСсылка) КАК КорСчет,
    НачОст.Субконто1,
    НачОст.Субконто2,
    НачОст.Субконто3,
    НачОст.Подразделение,
    НачОст.СуммаНУОстаток КАК НУНачальныйОстаток,
    NULL КАК НУКонечныйОстаток,
    ВЫБОР
        КОГДА НачОст.Счет.Вид = ЗНАЧЕНИЕ(ВидСчета.Активный)
            ТОГДА НачОст.СуммаНУОстатокДт - НачОст.СуммаНУОстатокКт
        ИНАЧЕ НачОст.СуммаНУОстатокКт - НачОст.СуммаНУОстатокДт
    КОНЕЦ КАК НУНачальныйОстатокРасчетов,
    NULL КАК НУКонечныйОстатокРасчетов,
    НачОст.СуммаОстаток КАК БУНачальныйОстаток,
    НачОст.СуммаОстатокДт КАК БУНачальныйОстатокДт,
    НачОст.СуммаОстатокКт КАК БУНачальныйОстатокКт,
    НачОст.СуммаНУОстатокДт КАК НУНачальныйОстатокДт,
    НачОст.СуммаНУОстатокКт КАК НУНачальныйОстатокКт,
    НачОст.СуммаНУРазвернутыйОстатокДт КАК НУНачальныйРазвернутыйОстатокДт,
    НачОст.СуммаНУРазвернутыйОстатокКт КАК НУНачальныйРазвернутыйОстатокКт,
    НачОст.СуммаРазвернутыйОстатокДт КАК БУНачальныйРазвернутыйОстатокДт,
    НачОст.СуммаРазвернутыйОстатокКт КАК БУНачальныйРазвернутыйОстатокКт,
    НачОст.ВалютнаяСуммаОстаток КАК ВалютнаяСуммаНачальныйОстаток,
    НачОст.ВалютнаяСуммаОстатокДт КАК ВалютнаяСуммаНачальныйОстатокДт,
    НачОст.ВалютнаяСуммаОстатокКт КАК ВалютнаяСуммаНачальныйОстатокКт,
    НачОст.КоличествоОстаток КАК КоличествоНачальныйОстаток,
    НачОст.КоличествоОстатокДт КАК КоличествоНачальныйОстатокДт,
    НачОст.КоличествоОстатокКт КАК КоличествоНачальныйОстатокКт,
    NULL КАК БУКонечныйОстаток,
    NULL КАК БУКонечныйОстатокДт,
    NULL КАК БУКонечныйОстатокКт,
    NULL КАК НУКонечныйОстатокДт,
    NULL КАК НУКонечныйОстатокКт,
    NULL КАК НУКонечныйРазвернутыйОстатокДт,
    NULL КАК НУКонечныйРазвернутыйОстатокКт,
    NULL КАК БУКонечныйРазвернутыйОстатокДт,
    NULL КАК БУКонечныйРазвернутыйОстатокКт,
    NULL КАК ВалютнаяСуммаКонечныйОстаток,
    NULL КАК ВалютнаяСуммаКонечныйОстатокДт,
    NULL КАК ВалютнаяСуммаКонечныйОстатокКт,
    NULL КАК КоличествоКонечныйОстаток,
    NULL КАК КоличествоКонечныйОстатокДт,
    NULL КАК КоличествоКонечныйОстатокКт,
    ВЫБОР
        КОГДА &amp;Субконто1ПрочиеДоходыИРасходы
            ТОГДА НачОст.Субконто1.ВидПрочихДоходовИРасходов
        КОГДА &amp;Субконто1СтатьяЗатрат
            ТОГДА НачОст.Субконто1.ВидРасходовНУ
        ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.ВидыПрочихДоходовИРасходов.ПустаяСсылка)
    КОНЕЦ КАК ВидРасходов,
    ВЫБОР
        КОГДА &amp;Субконто1ПрочиеДоходыИРасходы
                ИЛИ &amp;Субконто1СтатьяЗатрат
            ТОГДА НачОст.Субконто1.ВидДеятельностиДляНалоговогоУчетаЗатрат
        ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.ВидыДеятельностиДляНалоговогоУчетаЗатрат.ПустаяСсылка)
    КОНЕЦ КАК ВидДеятельности,
    НачОст.СуммаОстаток КАК БУОстаток,
    НачОст.СуммаНУОстаток КАК НУОстаток,
    НАЧАЛОПЕРИОДА(&amp;НачалоПериода, ДЕНЬ) КАК Период,
    НачОст.Счет.Вид,
    ВЫБОР
        КОГДА НачОст.Счет.Вид = ЗНАЧЕНИЕ(ВидСчета.Активный)
            ТОГДА НачОст.СуммаОстатокДт - НачОст.СуммаОстатокКт
        ИНАЧЕ НачОст.СуммаОстатокКт - НачОст.СуммаОстатокДт
    КОНЕЦ КАК БУНачальныйОстатокРасчетов,
    NULL КАК БУКонечныйОстатокРасчетов
ИЗ
    РегистрБухгалтерии.Хозрасчетный.Остатки(&amp;НачалоПериодаГраница, {(Счет).* КАК Счет}, &amp;Субконто, {(Организация).* КАК Организация, (Подразделение).* КАК Подразделение, (Валюта).* КАК Валюта, (Субконто1).* КАК Субконто1, (Субконто2).* КАК Субконто2, (Субконто3).* КАК Субконто3}) КАК НачОст

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
    КонОст.Счет,
	ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ПустаяСсылка),
    КонОст.Субконто1,
    КонОст.Субконто2,
    КонОст.Субконто3,
    КонОст.Подразделение,
    NULL,
    КонОст.СуммаНУОстаток,
    NULL,
    ВЫБОР
        КОГДА КонОст.Счет.Вид = ЗНАЧЕНИЕ(ВидСчета.Активный)
            ТОГДА КонОст.СуммаНУОстатокДт - КонОст.СуммаНУОстатокКт
        ИНАЧЕ КонОст.СуммаНУОстатокКт - КонОст.СуммаНУОстатокДт
    КОНЕЦ,
    NULL,
    NULL,
    NULL,
    NULL,
    NULL,
    NULL,
    NULL,
    NULL,
    NULL,
    NULL,
    NULL,
    NULL,
    NULL,
    NULL,
    NULL,
    КонОст.СуммаОстаток,
    КонОст.СуммаОстатокДт,
    КонОст.СуммаОстатокКт,
    КонОст.СуммаНУОстатокДт,
    КонОст.СуммаНУОстатокКт,
    КонОст.СуммаНУРазвернутыйОстатокДт,
    КонОст.СуммаНУРазвернутыйОстатокКт,
    КонОст.СуммаРазвернутыйОстатокДт,
    КонОст.СуммаРазвернутыйОстатокКт,
    КонОст.ВалютнаяСуммаОстаток,
    КонОст.ВалютнаяСуммаОстатокДт,
    КонОст.ВалютнаяСуммаОстатокКт,
    КонОст.КоличествоОстаток,
    КонОст.КоличествоОстатокДт,
    КонОст.КоличествоОстатокКт,
    ВЫБОР
        КОГДА &amp;Субконто1ПрочиеДоходыИРасходы
            ТОГДА КонОст.Субконто1.ВидПрочихДоходовИРасходов
        КОГДА &amp;Субконто1СтатьяЗатрат
            ТОГДА КонОст.Субконто1.ВидРасходовНУ
        ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.ВидыПрочихДоходовИРасходов.ПустаяСсылка)
    КОНЕЦ,
    ВЫБОР
        КОГДА &amp;Субконто1ПрочиеДоходыИРасходы
                ИЛИ &amp;Субконто1СтатьяЗатрат
            ТОГДА КонОст.Субконто1.ВидДеятельностиДляНалоговогоУчетаЗатрат
        ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.ВидыДеятельностиДляНалоговогоУчетаЗатрат.ПустаяСсылка)
    КОНЕЦ,
    КонОст.СуммаОстаток,
    КонОст.СуммаНУОстаток,
    НАЧАЛОПЕРИОДА(&amp;КонецПериода, ДЕНЬ),
    КонОст.Счет.Вид,
    NULL,
    ВЫБОР
        КОГДА КонОст.Счет.Вид = ЗНАЧЕНИЕ(ВидСчета.Активный)
            ТОГДА КонОст.СуммаОстатокДт - КонОст.СуммаОстатокКт
        ИНАЧЕ КонОст.СуммаОстатокКт - КонОст.СуммаОстатокДт
    КОНЕЦ
ИЗ
    РегистрБухгалтерии.Хозрасчетный.Остатки(&amp;КонецПериодаГраница, {(Счет).* КАК Счет}, &amp;Субконто, {(Организация).* КАК Организация, (Подразделение).* КАК Подразделение, (Валюта).* КАК Валюта, (Субконто1).* КАК Субконто1, (Субконто2).* КАК Субконто2, (Субконто3).* КАК Субконто3}) КАК КонОст</query>
