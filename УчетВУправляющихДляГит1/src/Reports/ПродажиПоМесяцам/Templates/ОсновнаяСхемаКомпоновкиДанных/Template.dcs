	<totalField>
		<dataPath>Отклонение</dataPath>
		<expression>ВЫБОР
    КОГДА ВычислитьВыражение("Год","Год", , "Текущая", "Текущая") = ВычислитьВыражение("Год","Год",, "Последняя", "Последняя") 
    ТОГДА 0 
    КОГДА ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год", , "Текущая", "Текущая") = 0 ИЛИ
          ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год",, "Следующая", "Следующая") = 0
    ТОГДА 0
	
    ИНАЧЕ (ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год", , "Текущая", "Текущая")
        / ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год",, "Следующая", "Следующая") - 1) * 100
 * ( Выбор КОГДА  
	ВычислитьВыражение("ЕстьNull(СУММА(Сумма), 0)","Год",, "Следующая", "Следующая") &lt; 0 Тогда -1 ИНаче 1 Конец)
КОНЕЦ </expression>
	</totalField>
