&НаКлиентеНаСервереБезКонтекста
Процедура Расчет(Форма, ИмяТекущейСтраницыПанели = "", ОбластьИмя = "")
	
	мФормаОтчета = Форма.ПолеТабличногоДокументаФормаОтчета;
	Если (Форма.СтруктураРеквизитовФормы.ФлажокОтклАвтоРасчет = Истина) Тогда
		Возврат; // отключен режим авторасчета
	КонецЕсли;
	Области = мФормаОтчета.Области;
	
	Области.П030104.Значение = Области.П030105.Значение + Области.П030106.Значение;
	Области.П030204.Значение = Области.П030205.Значение + Области.П030206.Значение;
	Области.П030304.Значение = Области.П030305.Значение + Области.П030306.Значение;
	Области.П030404.Значение = Области.П030405.Значение;
	Области.П0105.Значение = Области.П0106.Значение + Области.П0107.Значение + Области.П0108.Значение + Области.П0109.Значение
							+ Области.П0111.Значение + Области.П0112.Значение + Области.П0113.Значение + Области.П0114.Значение + Области.П0115.Значение;
	
КонецПроцедуры

