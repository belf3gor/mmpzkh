// Функция РАСЧЕТ ()
// Выполняет расчет вычисляемых показателей на заданной странице отчета
// (ячеек, выделенных зеленым цветом). 
// В случае неуказания листа, ничего происходить не будет
// Параметры:
//  ИмяСтраницыПанели - строка, имя страницы основной панели формы.
//
&НаКлиентеНаСервереБезКонтекста
Процедура Расчет(Форма, ИмяСтраницыПанели = "", ОбластьИмя = "")
	
	Если Форма.СтруктураРеквизитовФормы.ФлажокОтклАвтоРасчет Тогда
		Возврат; // отключен режим АвтоЗаполнения
	КонецЕсли;
	
	Области = Форма.ПолеТабличногоДокументаФормаОтчета.Области;
	
	Для Стр = 10 По 461 Цикл 
		ИмяОблНач = "П01000" + Формат(Стр, "ЧЦ=3; ЧВН=");
		Если Области.Найти(ИмяОблНач + "03") = Неопределено Тогда 
			ИмяОблНач = "П0100" + Формат(Стр, "ЧЦ=3; ЧВН=");
			Если Области.Найти(ИмяОблНач + "03") = Неопределено Тогда 
				Продолжить;
			КонецЕсли;
		КонецЕсли;
		
		Области[ИмяОблНач+"08"].Значение = Области[ИмяОблНач+"03"].Значение + Области[ИмяОблНач+"04"].Значение
		+Области[ИмяОблНач+"06"].Значение+Области[ИмяОблНач+"07"].Значение-Области[ИмяОблНач+"05"].Значение;
	КонецЦикла;
	
	Для Стр = 500 По 600 Цикл 
		Если Стр = 530 Тогда 
			ИмяОблНач = "П02000530";
			Области[ИмяОблНач+"08"].Значение = Области[ИмяОблНач+"03"].Значение + Области[ИмяОблНач+"04"].Значение
			+Области[ИмяОблНач+"07"].Значение-Области[ИмяОблНач+"05"].Значение;
		Иначе 
			ИмяОблНач = "П02000" + Формат(Стр, "ЧЦ=3; ЧВН=");
			Если Области.Найти(ИмяОблНач + "03") = Неопределено Тогда 
				Продолжить;
			КонецЕсли;
			
			Области[ИмяОблНач+"08"].Значение = Области[ИмяОблНач+"03"].Значение + Области[ИмяОблНач+"04"].Значение
			+Области[ИмяОблНач+"06"].Значение+Области[ИмяОблНач+"07"].Значение-Области[ИмяОблНач+"05"].Значение;
		КонецЕсли;
	КонецЦикла;
	
	Для Гр = 1 По 7 Цикл
		ГрСтр = Формат(Гр+2, "ЧЦ=2; ЧВН=; ЧГ=");
		Области["П0100010" + ГрСтр].Значение = ОкруглитьЧислоПоФормату(Форма, Области["П0100020" + ГрСтр].Значение)
