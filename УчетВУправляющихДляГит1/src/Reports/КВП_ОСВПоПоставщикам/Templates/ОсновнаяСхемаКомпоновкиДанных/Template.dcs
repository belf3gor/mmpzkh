	ЕСТЬNULL(ТаблицаНачислений.ВидУслуги, "&lt;Услуга не указана&gt;") КАК ВидУслуги,
	СУММА(ТаблицаНачислений.СуммаНачисленияВсего) КАК СуммаНачисленияВсего,
