&НаКлиентеНаСервереБезКонтекста
Процедура Расчет(Форма, ИмяРаздела = "", ОбластьИмя = "") Экспорт
	
	Если Форма.СтруктураРеквизитовФормы.ФлажокОтклАвтоРасчет ИЛИ Форма.СтруктураРеквизитовФормы.мРежимПечати Тогда
		Возврат;
	КонецЕсли;
	
	Если ИмяРаздела = "Расчет" Тогда
		
		// Стр. 013 = (стр. 011 + стр. 012) : стр. 010 х 100%.
		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ (ЗначениеЗаполнено(ОбластьИмя)
			И СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П000010001303, ОбластьИмя) > 0) Тогда
			Если Форма.СтруктураРеквизитовФормы.НаимТекущегоРаздела = ИмяРаздела Тогда
				Если Форма.ТабличныйДокумент.Области.П000010001003.Значение = 0 Тогда
					Форма.ТабличныйДокумент.Области.П000010001303.Значение = 0;
				Иначе
					Форма.ТабличныйДокумент.Области.П000010001303.Значение
					= (Форма.ТабличныйДокумент.Области.П000010001103.Значение
					+ Форма.ТабличныйДокумент.Области.П000010001203.Значение)
					/ Форма.ТабличныйДокумент.Области.П000010001003.Значение * 100;
				КонецЕсли;
			Иначе
				Если Форма.СтруктураДанныхРасчет.П000010001003 = 0 Тогда
					Форма.СтруктураДанныхРасчет.Вставить("П000010001303", 0);
				Иначе
					Форма.СтруктураДанныхРасчет.Вставить("П000010001303",
					(Форма.СтруктураДанныхРасчет.П000010001103
					+ Форма.СтруктураДанныхРасчет.П000010001203)
					/ Форма.СтруктураДанныхРасчет.П000010001003 * 100);
				КонецЕсли;
			КонецЕсли;
		КонецЕсли;
		
	ИначеЕсли ИмяРаздела = "" Тогда
		
		Расчет(Форма, "Расчет");
		
	КонецЕсли;
	
КонецПроцедуры

