			Сообщение.Сообщить();
								
		Иначе
			
			СтрТаблицаСтраницы = СтрТаблицаСтраницы[0];
			
		КонецЕсли;
		
		Если ПустаяСтрока(СтрТаблицаСтраницы.Данные[0].Значение.ФизЛицФИО) Тогда
			
			СтрокаДляВставкиДанных = СтрТаблицаСтраницы;
			
			// Пишем в этот лист
			
