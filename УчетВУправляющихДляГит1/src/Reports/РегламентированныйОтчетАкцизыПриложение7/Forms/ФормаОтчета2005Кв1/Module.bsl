		// Строка 490
		Если Пересчет ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П000020049003, ОбластьИмя) > 0 Тогда
			Раздел.Области.П000020049003.Значение = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФорматуДелителя(Форма, Раздел.Области.П000020050003.Значение) + РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФорматуДелителя(Форма, Раздел.Области.П000020051003.Значение);
		КонецЕсли;
		
		// Строка 530
		Если Пересчет ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П000020053003, ОбластьИмя) > 0 Тогда
			Раздел.Области.П000020053003.Значение = Раздел.Области.П000020043003.Значение;
		КонецЕсли;
		
		// Строка 540
		Если Пересчет ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П000020054003, ОбластьИмя) > 0 Тогда
			Раздел.Области.П000020054003.Значение = Раздел.Области.П000020044003.Значение;
		КонецЕсли;
				
		// Строка 545
		Если Пересчет ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П000020054503, ОбластьИмя) > 0 Тогда
			Раздел.Области.П000020054503.Значение = Раздел.Области.П000020044503.Значение;
		КонецЕсли;
		
		// Строка 550
		Если Пересчет ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П000020055003, ОбластьИмя) > 0 Тогда
			Форма.ТабличныйДокумент.Области.П000020055003.Значение = Форма.ТабличныйДокумент.Области.П000020045003.Значение + Форма.ТабличныйДокумент.Области.П000020049003.Значение;
		КонецЕсли;
		
		// Строка 520
		Если Пересчет ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П000020052003, ОбластьИмя) > 0 Тогда
			Форма.ТабличныйДокумент.Области.П000020052003.Значение = Форма.ТабличныйДокумент.Области.П000020053003.Значение + Форма.ТабличныйДокумент.Области.П000020054003.Значение + Форма.ТабличныйДокумент.Области.П000020054503.Значение + Форма.ТабличныйДокумент.Области.П000020055003.Значение;
		КонецЕсли;
		    		
		Если Форма.ТаблицаСтраницыРаздела1.Количество() > 0  Тогда
			
			// Цикл по многостраничности
			Для ИндексРаздела = 0 По Форма.ТаблицаСтраницыРаздела1.Количество()-1  Цикл
				
				Если Форма.ТаблицаСтраницыРаздела1[ИндексРаздела].Данные.Количество() > 0 Тогда
					
					СтруктураДанных = Форма.ТаблицаСтраницыРаздела1[ИндексРаздела].Данные[0].Значение;
					СтруктураДанных.Вставить("СуммаНалога1", Форма.ТабличныйДокумент.Области.П000020052003.Значение); 
					Форма.ТаблицаСтраницыРаздела1[ИндексРаздела].Данные[0].Значение = СтруктураДанных;
				
				КонецЕсли;
				
			КонецЦикла;
			
		 КонецЕсли;
		      		 
	ИначеЕсли ИмяСтраницыПанели = "" Тогда
		
		// Выполняем расчет вычисляемых показателей отчета на
		// всех страницах основной панели формы
		Расчет(Форма, "Раздел2");
		Расчет(Форма, "Раздел1");
		
	КонецЕсли;
	       	
	Модифицированность = Истина;
	
КонецПроцедуры

