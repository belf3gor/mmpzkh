					СтрокаДляВставкиДанных.Данные[0].Значение.ФизЛицДатаРожд =ДатаРожд;       

				КонецЕсли;

			КонецЕсли;
		КонецЕсли;
	Исключение
	// В случае, если на листе ФЛ такого параметра нет, то пропускаем, и делаем все через Попытку/Исключить
