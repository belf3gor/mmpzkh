			НаименованиеПоказателя, ЗначениеСтроки020, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел42",
			Неопределено, , СчетчикФормулДляРасшифровок);
		
		ИмяПоказателя = "П000420006003";
		НаименованиеПоказателя = "Раздел 4.2, Строка 060, Страница № " + НомерСтраницы;
		ЗначениеСтроки060 = СтраницаР42.Данные[0].Значение[ИмяПоказателя];
		ЗначениеСтроки060Стр = Формат(ЗначениеСтроки060, "ЧРД=.;ЧН=0");
		РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей, ИмяПоказателя,
			НаименованиеПоказателя, ЗначениеСтроки060, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел42",
			Неопределено, , СчетчикФормулДляРасшифровок);
		
		ПраваяЧасть = ЗначениеСтроки010 + ЗначениеСтроки020 - ЗначениеСтроки060;
		ПраваяЧастьСтр = Формат(ПраваяЧасть, "ЧРД=.;ЧН=0");
		
		Если ЛеваяЧасть = ПраваяЧасть Тогда 
			ЭтоОшибка = Ложь;
