				СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел43", Неопределено, НомерСтраницы,
				СчетчикФормулДляРасшифровок);
			
			// Зарегистрируем правую часть соотношения.
			
			ИмяПоказателя020 = "П000430001020_" + НомерСтроки;
			НаименованиеПоказателя020 = "Сумма налогового вычета, образовавшаяся на месяц и год, указанные в строке 010, Строка № "
			+ НомерСтроки + ", Страница № " + НомерСтраницы;
			ЗначениеСтроки020 = СтраницаР43.Данные[0].Значение[ИмяПоказателя020];
			ЗначениеСтроки020Стр = Формат(ЗначениеСтроки020, "ЧРД=.;ЧН=0");
			РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей,
				ИмяПоказателя020, НаименованиеПоказателя020, ЗначениеСтроки020, "",
				СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел43", Неопределено, НомерСтраницы,
				СчетчикФормулДляРасшифровок);
			
			ИмяПоказателя030 = "П000430001030_" + НомерСтроки;
			НаименованиеПоказателя030 = "Сумма налогового вычета, использованная в предыдущие налоговые периоды, Строка № "
			+ НомерСтроки + ", Страница № " + НомерСтраницы;
			ЗначениеСтроки030 = СтраницаР43.Данные[0].Значение[ИмяПоказателя030];
			ЗначениеСтроки030Стр = Формат(ЗначениеСтроки030, "ЧРД=.;ЧН=0");
			РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей,
