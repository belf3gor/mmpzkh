		НомерСтроки = 1;
		ЗаполненоБлоков = 0;
		ИмяПоказателя010 = "П000430001010_";
		ИмяПоказателя020 = "П000430001020_";
		ИмяПоказателя030 = "П000430001030_";
		ИмяПоказателя040 = "П000430001040_";
		ИмяПоказателя050 = "П000430001050_";
		ИмяПоказателя060 = "П000430001060_";
		Пока СтраницаР43.Данные[0].Значение.Свойство(ИмяПоказателя010 + НомерСтроки,) Цикл
			Если ЗначениеЗаполнено(СтраницаР43.Данные[0].Значение[ИмяПоказателя010 + НомерСтроки])
				ИЛИ ЗначениеЗаполнено(СтраницаР43.Данные[0].Значение[ИмяПоказателя020 + НомерСтроки])
				ИЛИ ЗначениеЗаполнено(СтраницаР43.Данные[0].Значение[ИмяПоказателя030 + НомерСтроки])
				ИЛИ ЗначениеЗаполнено(СтраницаР43.Данные[0].Значение[ИмяПоказателя040 + НомерСтроки])
				ИЛИ ЗначениеЗаполнено(СтраницаР43.Данные[0].Значение[ИмяПоказателя050 + НомерСтроки])
