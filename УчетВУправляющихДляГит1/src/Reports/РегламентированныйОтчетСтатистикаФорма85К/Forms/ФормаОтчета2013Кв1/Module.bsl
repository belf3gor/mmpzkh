// Процедура очищает содержимое редактируемых и вычисляемых ячеек
// поля табличного документа, переданного параметром ВыбТабличноеПоле.
//
// Параметры:
//  ВыбТабличноеПоле - поле табличного документа.
//
&НаКлиентеНаСервереБезКонтекста
Процедура ОчиститьТабличноеПоле(Форма, ВыбТабличноеПолеИмя) Экспорт
	
	Перем НомерКорректировки;
	// Предварительно проверим наличие дополнительных строк,
	// в заданном поле табличного документа.
	// При наличии таких строк удаляем их.
	ВыбТабличноеПоле = Форма["ТабличныйДокумент"];
	ИмяГруппыДопСтрок = Форма.ОпределитьИдМногострочногоРазделаПоТабличномуПолю(ВыбТабличноеПолеИмя);
	Если ИмяГруппыДопСтрок <> Неопределено Тогда
		// Поле табличного документа содержит многострочный раздел.
		
		Форма.УдалитьНеобходимоеКоличествоДопСтрокНаСервере(ВыбТабличноеПоле, ВыбТабличноеПолеИмя, ИмяГруппыДопСтрок);

		Форма["ТабличныйДокумент"] = ВыбТабличноеПоле;

	КонецЕсли;
	
	Если ВыбТабличноеПолеИмя = "Титульный" Тогда
		
		ОргНазв = Форма.ТабличныйДокумент.Области.ОргНазв.Значение;
		ОргКодОКПО = Форма.ТабличныйДокумент.Области.ОргКодОКПО.Значение;
		ОргАдрес = Форма.ТабличныйДокумент.Области.ОргАдрес.Значение;
		Код1 = Форма.ТабличныйДокумент.Области.Код1.Значение;
		Код2 = Форма.ТабличныйДокумент.Области.Код2.Значение;
		Код3 = Форма.ТабличныйДокумент.Области.Код3.Значение;
		Код4 = Форма.ТабличныйДокумент.Области.Код4.Значение;
		НомерКорректировки = Форма.ТабличныйДокумент.Области.НомерКорректировки.Значение;
	ИначеЕсли ВыбТабличноеПолеИмя = "Подписи" Тогда
		ОргАдресЭлектроннойПочты = Форма.ТабличныйДокумент.Области.ОргАдресЭлектроннойПочты.Значение;
		ОргДолжностьИсп = Форма.ТабличныйДокумент.Области.ОргДолжностьИсп.Значение;
		ОргИсполнитель = Форма.ТабличныйДокумент.Области.ОргИсполнитель.Значение;
		ОргТелефонИсп = Форма.ТабличныйДокумент.Области.ОргТелефонИсп.Значение;
	Иначе
		
		ОргНазв = Форма.СтруктураДанныхТитульный.ОргНазв;
		ОргКодОКПО = Форма.СтруктураДанныхТитульный.ОргКодОКПО;
		ОргАдрес = Форма.СтруктураДанныхТитульный.ОргАдрес;
		Код1 = Форма.СтруктураДанныхТитульный.Код1;
		Код2 = Форма.СтруктураДанныхТитульный.Код2;
		Код3 = Форма.СтруктураДанныхТитульный.Код3;
		Код4 = Форма.СтруктураДанныхТитульный.Код4;
		Форма.СтруктураДанныхТитульный.Свойство("НомерКорректировки", НомерКорректировки);
		ОргАдресЭлектроннойПочты = Форма.СтруктураДанныхПодписи.ОргАдресЭлектроннойПочты;
		ОргДолжностьИсп = Форма.СтруктураДанныхПодписи.ОргДолжностьИсп;
		ОргИсполнитель = Форма.СтруктураДанныхПодписи.ОргИсполнитель;
		ОргТелефонИсп = Форма.СтруктураДанныхПодписи.ОргТелефонИсп;
	
	КонецЕсли; 
	
	// Непосредственно очищаем ячейки табличного документа.
	Для Инд = 0 По ВыбТабличноеПоле.Области.Количество() - 1 Цикл
		ТекущаяОбласть = ВыбТабличноеПоле.Области[Инд];

		Если НЕ Тип(ТекущаяОбласть) = Тип("РисунокТабличногоДокумента") И Не ТекущаяОбласть.ТипОбласти = ТипОбластиЯчеекТабличногоДокумента.Прямоугольник Тогда
			Продолжить;
		КонецЕсли;

		Если ТекущаяОбласть.СодержитЗначение <> Истина Тогда
			Продолжить;
		КонецЕсли;

		Если ТекущаяОбласть.Защита Тогда
			Продолжить;
		КонецЕсли;

		Если Тип(ТекущаяОбласть) = Тип("РисунокТабличногоДокумента") Тогда
			ТекущаяОбласть.Значение = "";
		Иначе
			ТекущаяОбласть.Очистить();
			ТекущаяОбласть.Формат = РегламентированнаяОтчетностьКлиентСервер.ЗаменитьПредставлениеНуляВСтрокеФормата(ТекущаяОбласть.Формат, "ЧН=' '");
		КонецЕсли;
		
	КонецЦикла;

	ИмяСтраницыПанели = ВыбТабличноеПолеИмя;
	
	#Если НаКлиенте Тогда
		Форма.ОбнулитьПеременныеМодуляХранящиеЗначенияКвадратногоПредставленияПоРазделуНаКлиенте(ИмяСтраницыПанели);
	#Иначе
		Форма.ОбнулитьПеременныеМодуляХранящиеЗначенияКвадратногоПредставленияПоРазделуНаСервере(ИмяСтраницыПанели);
	#КонецЕсли
		
	Если ИмяГруппыДопСтрок <> Неопределено Тогда
		ОбновитьСтруктуруМногострочногоРаздела(Форма, ИмяГруппыДопСтрок);
	КонецЕсли;
		
	Форма.СтруктураДанныхТитульный.ОргНазв = ОргНазв;
	Форма.СтруктураДанныхТитульный.ОргКодОКПО = ОргКодОКПО;
	Форма.СтруктураДанныхТитульный.ОргАдрес = ОргАдрес;
	Форма.СтруктураДанныхТитульный.Код1 = Код1;
	Форма.СтруктураДанныхТитульный.Код2 = Код2;
	Форма.СтруктураДанныхТитульный.Код3 = Код3;
	Форма.СтруктураДанныхТитульный.Код4 = Код4;
	Форма.СтруктураДанныхТитульный.Вставить("НомерКорректировки", НомерКорректировки);
	Форма.СтруктураДанныхПодписи.ОргАдресЭлектроннойПочты = ОргАдресЭлектроннойПочты;
	Форма.СтруктураДанныхПодписи.ОргДолжностьИсп = ОргДолжностьИсп;
	Форма.СтруктураДанныхПодписи.ОргИсполнитель = ОргИсполнитель;
	Форма.СтруктураДанныхПодписи.ОргТелефонИсп = ОргТелефонИсп;
	
	Если ИмяСтраницыПанели = "Титульный" Тогда
		Форма.ТабличныйДокумент.Области.ОргНазв.Значение = ОргНазв;
		Форма.ТабличныйДокумент.Области.ОргКодОКПО.Значение = ОргКодОКПО;
		Форма.ТабличныйДокумент.Области.ОргАдрес.Значение = ОргАдрес;
		Форма.ТабличныйДокумент.Области.Код1.Значение = Код1;
		Форма.ТабличныйДокумент.Области.Код2.Значение = Код2;
		Форма.ТабличныйДокумент.Области.Код3.Значение = Код3;
		Форма.ТабличныйДокумент.Области.Код4.Значение = Код4;
		Форма.ТабличныйДокумент.Области.НомерКорректировки.Значение = НомерКорректировки;
	ИначеЕсли ИмяСтраницыПанели = "Подписи" Тогда
		Форма.ТабличныйДокумент.Области.ОргАдресЭлектроннойПочты.Значение = ОргАдресЭлектроннойПочты;
		Форма.ТабличныйДокумент.Области.ОргДолжностьИсп.Значение = ОргДолжностьИсп;
		Форма.ТабличныйДокумент.Области.ОргИсполнитель.Значение = ОргИсполнитель;
		Форма.ТабличныйДокумент.Области.ОргТелефонИсп.Значение = ОргТелефонИсп;
	КонецЕсли;
	
	ПеренумероватьСтроки(Форма);
	Расчет(Форма, ИмяСтраницыПанели);
	
КонецПроцедуры

&НаСервере
Процедура УдалитьНеобходимоеКоличествоДопСтрокНаСервере(ВыбТабличноеПоле, ВыбТабличноеПолеИмя, ИмяГруппыДопСтрок) Экспорт
	
	// Получим таблицу значений, содержащую данные дополнительных строк.
	ПромТаблицаДопСтрокРаздела = Неопределено;
	мСтруктураМногострочныхРазделов.Свойство(ИмяГруппыДопСтрок, ПромТаблицаДопСтрокРаздела);
	
	ПромТаблицаДопСтрокРаздела = ЭтаФорма[ПромТаблицаДопСтрокРаздела];
	
	ИсходноеКоличествоСтрокГруппы = 0;
	мСтруктураИсхКолвоСтрокРазделов.Свойство(ИмяГруппыДопСтрок, ИсходноеКоличествоСтрокГруппы);
	
	ТекущееКоличествоСтрок = ПромТаблицаДопСтрокРаздела.Количество();
	
	Если ТекущееКоличествоСтрок > ИсходноеКоличествоСтрокГруппы Тогда
		// Имеются дополнительные строки.
		// Удаляем их.
		
		НСтр = ПромТаблицаДопСтрокРаздела.Количество();
		
		Пока НСтр > ИсходноеКоличествоСтрокГруппы Цикл
			
			ТекСтрокаТаблицы = ПромТаблицаДопСтрокРаздела[НСтр - 1];
			
			ИмяПервойКолонки = ИмяГруппыДопСтрок + "01_";
			ИмяПервойЯчейкиУдаляемойСтроки  = ИмяПервойКолонки + НСтр;
			
			// область первой ячейки удаляемой строки многострочного раздела
			ОбластьЯчейки = ВыбТабличноеПоле.Области[ИмяПервойЯчейкиУдаляемойСтроки];
			
			НомерВерхЯчейкиОбласти = ОбластьЯчейки.Верх;
			НомерНижнЯчейкиОбласти = ОбластьЯчейки.Низ;
			
			РегламентированнаяОтчетностьВызовСервера.СпециальногоУдаленияСтроки(ВыбТабличноеПоле, ВыбТабличноеПолеИмя, ИмяГруппыДопСтрок, НомерВерхЯчейкиОбласти, НомерНижнЯчейкиОбласти);
			
			// удаляем из структуры многострочного раздела соответствующую строку
			ПромТаблицаДопСтрокРаздела.Удалить(ТекСтрокаТаблицы);
			
			НСтр = НСтр - 1;
						
		КонецЦикла;	
		
		Если ВыбТабличноеПоле.Области.Найти("МногострочнаяЧасть") = Неопределено Тогда
			
			ВыбТабличноеПоле.Область(НомерВерхЯчейкиОбласти - 1, , НомерНижнЯчейкиОбласти - 1).Имя = "МногострочнаяЧасть";
			
		КонецЕсли;
		
	КонецЕсли;
	
КонецПроцедуры

&НаСервере
Процедура ЗаполнитьСведенияОбОрганизацииНаСервере(ВПрограммеИзмененаДатаПодписи = Ложь)
	
	ЗаполнитьСведенияОбОрганизации(ЭтаФорма, ВПрограммеИзмененаДатаПодписи);
	
КонецПроцедуры

// Процедура управляет выводом сведений о выбранной организации
// в соответствующие ячейки табличного документа.
//
// Параметры:
// ВПрограммеИзмененаДатаПодписи - Истина, если процедура запускается для обновления параметров завясящих
// от даты подписания документа, например ФИО должностных лиц
//
&НаКлиентеНаСервереБезКонтекста
Процедура ЗаполнитьСведенияОбОрганизации(Форма, ВПрограммеИзмененаДатаПодписи = Ложь, НеобходимоПерезаполнить10раздел = Ложь)

	Если (Форма.СтруктураРеквизитовФормы.Организация  = Неопределено) Или (Форма.СтруктураРеквизитовФормы.Организация = РегламентированнаяОтчетностьКлиентСервер.ПустоеЗначениеТипа("СправочникСсылка.Организации")) Тогда
		Возврат;
	КонецЕсли;

	Сведения = Новый СписокЗначений;

	Если НЕ ВПрограммеИзмененаДатаПодписи Тогда
		Сведения.Добавить("", "ИННЮЛ"); // ИНН
		Сведения.Добавить("", "АдрПочт");
		Сведения.Добавить("", "НаимЮЛПол");
		Сведения.Добавить("", "ОснВидДеят");
		Сведения.Добавить("", "ОКАТО");
		Сведения.Добавить("", "ОКПО");
	КонецЕсли;

	РегламентированнаяОтчетностьВызовСервера.ДополнитьЗаправшиваемыеСведенияОбОрганизации(Форма.СтруктураРеквизитовФормы.Организация, Сведения);
	ОргСведения = РегламентированнаяОтчетностьВызовСервера.ПолучитьСведенияОбОрганизации(Форма.СтруктураРеквизитовФормы.Организация, Форма.ДатаПодписи, Сведения);

	Если НЕ ВПрограммеИзмененаДатаПодписи Тогда

		ИННЮЛ = "";
		АдрПочт     = "";
		НаимЮЛПол = "";
		ОснВидДеят = "";
		ОКАТО = "";
		ОКПО = "";

	КонецЕсли;

	ДолжнИсп = "";
	ФИОИсп = "";
	ТелИсп = "";
	АдресЭлПочтыИсп = "";

	Если НЕ ВПрограммеИзмененаДатаПодписи Тогда

		ОргСведения.Свойство("ИННЮЛ",          ИННЮЛ);
		ОргСведения.Свойство("АдрПочт",        АдрПочт);
		ОргСведения.Свойство("НаимЮЛПол",      НаимЮЛПол);
		ОргСведения.Свойство("ОКАТО",          ОКАТО);
		ОргСведения.Свойство("ОКПО",           ОКПО);

	КонецЕсли;

	РегламентированнаяОтчетностьВызовСервера.ДополнитьПодписантаСтатистическогоОтчета(ФИОИсп, ДолжнИсп, ТелИсп, АдресЭлПочтыИсп, ОргСведения, Форма.СтруктураРеквизитовФормы.Организация);

	Если ВПрограммеИзмененаДатаПодписи Тогда

		Если Форма.ТабличныйДокумент.Области.Найти("ОргИсполнитель") <> Неопределено Тогда 
			Форма.ТабличныйДокумент.Области.ОргИсполнитель.Значение           = ФИОИсп;
			Форма.ТабличныйДокумент.Области.ОргДолжностьИсп.Значение          = ДолжнИсп;
			Форма.ТабличныйДокумент.Области.ОргТелефонИсп.Значение            = ТелИсп;
			Форма.ТабличныйДокумент.Области.ОргАдресЭлектроннойПочты.Значение = АдресЭлПочтыИсп;
		Иначе 
			Форма.СтруктураДанныхПодписи.Вставить("ОргИсполнитель", ФИОИсп);
			Форма.СтруктураДанныхПодписи.Вставить("ОргДолжностьИсп", ДолжнИсп);
			Форма.СтруктураДанныхПодписи.Вставить("ОргТелефонИсп", ТелИсп);
			Форма.СтруктураДанныхПодписи.Вставить("ОргАдресЭлектроннойПочты", АдресЭлПочтыИсп);
		КонецЕсли;
	
		Возврат;
		
	КонецЕсли;

	Если Форма.ТабличныйДокумент.Области.Найти("ОргИсполнитель") <> Неопределено Тогда 
		Если (НЕ ЗначениеЗаполнено(Форма.ТабличныйДокумент.Области.ОргИсполнитель.Значение)) ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
			Форма.ТабличныйДокумент.Области.ОргИсполнитель.Значение = ФИОИсп;
		КонецЕсли;

		Если (НЕ ЗначениеЗаполнено(Форма.ТабличныйДокумент.Области.ОргДолжностьИсп.Значение)) ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
			Форма.ТабличныйДокумент.Области.ОргДолжностьИсп.Значение = ДолжнИсп;
		КонецЕсли;

		Если (НЕ ЗначениеЗаполнено(Форма.ТабличныйДокумент.Области.ОргТелефонИсп.Значение)) ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
			Форма.ТабличныйДокумент.Области.ОргТелефонИсп.Значение = ТелИсп;
		КонецЕсли;
		
		Если (НЕ ЗначениеЗаполнено(Форма.ТабличныйДокумент.Области.ОргАдресЭлектроннойПочты.Значение)) ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
			Форма.ТабличныйДокумент.Области.ОргАдресЭлектроннойПочты.Значение = АдресЭлПочтыИсп;
		КонецЕсли;
		
		ОргНазв = "";
		Форма.СтруктураДанныхТитульный.Свойство("ОргНазв", ОргНазв);
		Если (НЕ ЗначениеЗаполнено(ОргНазв)) ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
			Форма.СтруктураДанныхТитульный.Вставить("ОргНазв", НаимЮЛПол);
		КонецЕсли;
		
		ОргКодОКПО = "";
		Форма.СтруктураДанныхТитульный.Свойство("ОргКодОКПО", ОргКодОКПО);
		Если (НЕ ЗначениеЗаполнено(ОргКодОКПО)) ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
			Форма.СтруктураДанныхТитульный.Вставить("ОргКодОКПО", ОКПО);
		КонецЕсли;

		ОргАдрес = "";
		Форма.СтруктураДанныхТитульный.Свойство("ОргАдрес", ОргАдрес);
		Если (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Или (ПустаяСтрока(ОргАдрес)) Тогда
			Если (ЗначениеЗаполнено(СтрЗаменить(ОргАдрес, ",", ""))) Тогда
				Форма.СтруктураДанныхТитульный.Вставить("ОргАдрес", РегламентированнаяОтчетностьКлиентСервер.ПредставлениеАдресаВФормате9Запятых(АдрПочт));
				Форма.СтруктураРеквизитовФормы.мАдресвФорматеДляВыгрузки = АдрПочт;
			Иначе
				Форма.СтруктураДанныхТитульный.Вставить("ОргДолжностьИсп", "");
				Форма.СтруктураРеквизитовФормы.мАдресвФорматеДляВыгрузки = "";
			КонецЕсли;
		КонецЕсли;
	Иначе
		ОргИсполнитель = "";
		Форма.СтруктураДанныхПодписи.Свойство("ОргИсполнитель", ОргИсполнитель);
		Если (НЕ ЗначениеЗаполнено(ОргИсполнитель)) ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
			Форма.СтруктураДанныхПодписи.Вставить("ОргИсполнитель", ФИОИсп);
		КонецЕсли;

		ОргДолжностьИсп = "";
		Форма.СтруктураДанныхПодписи.Свойство("ОргДолжностьИсп", ОргДолжностьИсп);
		Если (НЕ ЗначениеЗаполнено(ОргДолжностьИсп)) ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
			Форма.СтруктураДанныхПодписи.Вставить("ОргДолжностьИсп", ДолжнИсп);
		КонецЕсли;

		ОргТелефонИсп = "";
		Форма.СтруктураДанныхПодписи.Свойство("ОргТелефонИсп", ОргТелефонИсп);
		Если (НЕ ЗначениеЗаполнено(ОргТелефонИсп)) ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
			Форма.СтруктураДанныхПодписи.Вставить("ОргТелефонИсп", ТелИсп);
		КонецЕсли;
		
		ОргАдресЭлектроннойПочты = "";
		Форма.СтруктураДанныхПодписи.Свойство("ОргАдресЭлектроннойПочты", ОргАдресЭлектроннойПочты);
		Если (НЕ ЗначениеЗаполнено(ОргАдресЭлектроннойПочты)) ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
			Форма.СтруктураДанныхПодписи.Вставить("ОргАдресЭлектроннойПочты", АдресЭлПочтыИсп);
		КонецЕсли;
		
		Если (НЕ ЗначениеЗаполнено(Форма.ТабличныйДокумент.Области.ОргНазв.Значение)) ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
			Форма.ТабличныйДокумент.Области.ОргНазв.Значение = НаимЮЛПол;
		КонецЕсли;
		
		Если (НЕ ЗначениеЗаполнено(Форма.ТабличныйДокумент.Области.ОргКодОКПО.Значение)) ИЛИ (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Тогда
			Форма.ТабличныйДокумент.Области.ОргКодОКПО.Значение = ОКПО;
		КонецЕсли;

		Если (Форма.СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация) Или (ПустаяСтрока(Форма.ТабличныйДокумент.Области.ОргАдрес.Значение)) Тогда

			Если (ЗначениеЗаполнено(СтрЗаменить(АдрПочт, ",", ""))) Тогда
				
				Форма.ТабличныйДокумент.Области.ОргАдрес.Значение = РегламентированнаяОтчетностьКлиентСервер.ПредставлениеАдресаВФормате9Запятых(АдрПочт);
				Форма.СтруктураРеквизитовФормы.мАдресвФорматеДляВыгрузки = АдрПочт;
			Иначе
				Форма.ТабличныйДокумент.Области.ОргАдрес.Значение = "";
				Форма.СтруктураРеквизитовФормы.мАдресвФорматеДляВыгрузки = "";
			КонецЕсли;

		КонецЕсли;
	КонецЕсли;
	
	
	Форма.Модифицированность = Истина;
	
КонецПроцедуры

&НаСервере
Процедура СформироватьСпискиВыбора()
	
	МакетСоставаПоказателей = Отчеты[Сред(Лев(ЭтаФорма.ИмяФормы, СтрНайти(ЭтаФорма.ИмяФормы, ".Форма.") - 1), 7)].ПолучитьМакет("Список2013Кв1");
	
	Массив = Новый Массив;
	Массив.Добавить(Тип("Строка"));
	Строка12 = Новый ОписаниеТипов(Массив, , Новый КвалификаторыСтроки(12, ДопустимаяДлина.Переменная));
	Строка64 = Новый ОписаниеТипов(Массив, , Новый КвалификаторыСтроки(64, ДопустимаяДлина.Переменная));
	Строка512 = Новый ОписаниеТипов(Массив, , Новый КвалификаторыСтроки(512, ДопустимаяДлина.Переменная));
	
	КоллекцияСписковВыбора = Новый Соответствие;
	Для Каждого Область Из МакетСоставаПоказателей.Области Цикл
		Если Область.ТипОбласти = ТипОбластиЯчеекТабличногоДокумента.Строки Тогда
			ВерхОбласти = Область.Верх;
			НизОбласти = Область.Низ;
			ТаблСписка = Новый ТаблицаЗначений;
			ТаблСписка.Колонки.Добавить("Код",Строка12,, МакетСоставаПоказателей.Область(ВерхОбласти, 1, ВерхОбласти, 1).ШиринаКолонки);
			ТаблСписка.Колонки.Добавить("Название",Строка512,, МакетСоставаПоказателей.Область(ВерхОбласти, 2, ВерхОбласти, 2).ШиринаКолонки);
			Для НомСтр = ВерхОбласти По НизОбласти Цикл
				КодПоказателя = СокрП(МакетСоставаПоказателей.Область(НомСтр, 1).Текст);
				Если КодПоказателя <> "###" Тогда
					НовСтрока = ТаблСписка.Добавить();
					НовСтрока.Код = КодПоказателя;
					НовСтрока.Название = СокрП(МакетСоставаПоказателей.Область(НомСтр, 2).Текст);
				КонецЕсли;
			КонецЦикла;
			КоллекцияСписковВыбора.Вставить(Область.Имя, ТаблСписка);
		КонецЕсли;
	КонецЦикла;
	
	СвойстваПоказателей.Очистить();
	
	РегламентированнаяОтчетность.ДобавитьСтрокуОписанияВвода(СвойстваПоказателей, "Языки", 512, , "Выбор кода", КоллекцияСписковВыбора["s_lang"]);
	РегламентированнаяОтчетность.ДобавитьСтрокуОписанияВвода(СвойстваПоказателей, "ДаНет", 512, , "Выбор кода", КоллекцияСписковВыбора["YesNo"]);
	РегламентированнаяОтчетность.ДобавитьСтрокуОписанияВвода(СвойстваПоказателей, "56дневка", 512, , "Выбор кода", КоллекцияСписковВыбора["Day56"]);
	РегламентированнаяОтчетность.ДобавитьСтрокуОписанияВвода(СвойстваПоказателей, "КодТерритории", 512, , "Выбор кода", КоллекцияСписковВыбора["Type"]);
	РегламентированнаяОтчетность.ДобавитьСтрокуОписанияВвода(СвойстваПоказателей, "КапРемонт", 512, , "Выбор кода", КоллекцияСписковВыбора["КапРемонт"]);
	РегламентированнаяОтчетность.ДобавитьСтрокуОписанияВвода(СвойстваПоказателей, "ПриостановкаДеятельности", 512, , "Выбор кода", КоллекцияСписковВыбора["ПриостановкаДеятельности"]);
	РегламентированнаяОтчетность.ДобавитьСтрокуОписанияВвода(СвойстваПоказателей, "ОрганУправления", 512, , "Выбор кода", КоллекцияСписковВыбора["ОрганУправления"]);

КонецПроцедуры

&НаКлиенте
Процедура ВвестиПоказатель(Элемент, Область, КодПоказателя, Знач ЭтоСумма, Знач ТекстВыбора, ТаблицаВыбора, СтандартнаяОбработка, ОбязательныйВыборИзСписка, ИмяСмежнойОбласти = "") Экспорт
	
	Если ТаблицаВыбора = Неопределено ИЛИ (СтруктураРеквизитовФормы.мАвтоВыборКодов И (Не ОбязательныйВыборИзСписка)) Тогда
		Возврат;
	КонецЕсли;
	
	СтандартнаяОбработка = Ложь;
	
	ИсходноеЗначение = ?(ТипЗнч(Область.Значение) = Тип("Строка"), СтрЗаменить(Область.Значение, " ", ""), Область.Значение);
	
	ПараметрыФормы = Новый Структура;
	ПараметрыФормы.Вставить("Заголовок",          ТекстВыбора);
	ПараметрыФормы.Вставить("ТаблицаЗначений",    ТаблицаВыбора);
	ПараметрыФормы.Вставить("СтруктураДляПоиска", Новый Структура("Код", ИсходноеЗначение));
	
	ДополнительныеПараметры = Новый Структура("ИмяСмежнойОбласти, Область", ИмяСмежнойОбласти, Область);
	ОписаниеОповещения = Новый ОписаниеОповещения("ВвестиПоказательЗавершение", ЭтотОбъект, ДополнительныеПараметры);
	
	ОткрытьФорму("Обработка.ОбщиеОбъектыРеглОтчетности.Форма.ФормаВыбораЗначенияИзТаблицы", ПараметрыФормы, ЭтаФорма,,,, ОписаниеОповещения, РежимОткрытияОкнаФормы.БлокироватьОкноВладельца);
	
КонецПроцедуры

&НаКлиенте
Процедура ВвестиПоказательЗавершение(РезультатВыбора, ДополнительныеПараметры) Экспорт
    
    ИмяСмежнойОбласти = ДополнительныеПараметры.ИмяСмежнойОбласти;
    Область = ДополнительныеПараметры.Область;
    
	Если РезультатВыбора = Неопределено Тогда
		Возврат;
	КонецЕсли;
	
	РезультатКод = РезультатВыбора["Код"];
	Область.Значение = РезультатКод;
	РезультатНазвание = ?(РезультатВыбора["Название"] = "Пустое значение" , "", РезультатВыбора["Название"]);
	
	Если ПолеДаНет(Область.Имя) Тогда 
		Если РезультатВыбора["Код"] = "" Тогда
			Область.Формат = "";
		Иначе
			Область.Формат = "ЧН=0;";
		КонецЕсли;
	КонецЕсли;
	
	Если ЗначениеЗаполнено(ИмяСмежнойОбласти) Тогда
		ТабличныйДокумент.Области[ИмяСмежнойОбласти].Значение = РезультатНазвание;
	КонецЕсли;
	
	Модифицированность = Истина;
	
КонецПроцедуры

