Функция НайтиТелоВМекетеКомпановки(Тело,Имя)
	Для п = 0 по Тело.Количество() - 1 Цикл
		Если ТипЗнч(Тело[п]) <> Тип("МакетОбластиМакетаКомпоновкиДанных") Тогда
			Если Тело[п].Имя = Имя тогда
				Возврат Тело[п];	
			КонецЕсли;
		КонецЕсли;
	КонецЦикла;
	Возврат Неопределено;
КонецФункции	

