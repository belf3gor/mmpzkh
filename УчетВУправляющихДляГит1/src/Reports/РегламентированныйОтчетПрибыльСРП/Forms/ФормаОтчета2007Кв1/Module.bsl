&НаКлиентеНаСервереБезКонтекста
Процедура Расчет(Форма, ИмяТекущейСтраницыПанели = "", ОбластьИмя = "")

	Если Форма.СтруктураРеквизитовФормы.ФлажокОтклАвтоРасчет Или Форма.СтруктураРеквизитовФормы.мРежимПечати Тогда
		Возврат; // отключен режим АвтоЗаполнения
	КонецЕсли;
	
	Если ИмяТекущейСтраницыПанели = "Лист02" Тогда
		
		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П000020001003, ОбластьИмя) > 0 Тогда		
			Если Форма.СтруктураРеквизитовФормы.НаимТекущегоРаздела = "Лист02" Тогда
				Форма.ТабличныйДокумент.Области.П000020001003.Значение = Форма.ТабличныйДокумент.Области.П000020002003.Значение + Форма.ТабличныйДокумент.Области.П000020003003.Значение;
			КонецЕсли;
		КонецЕсли;
		
		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П002040016003, ОбластьИмя) > 0 Тогда
			
			Разница = 0;
			
			Если Форма.СтруктураРеквизитовФормы.НаимТекущегоРаздела = "Лист02" Тогда
				Разница = Форма.ТабличныйДокумент.Области.П000020001003.Значение - Форма.ТабличныйДокумент.Области.П000020004003.Значение;
			ИначеЕсли Форма.СтруктураДанныхЛист02.Свойство("П000020001003") И Форма.СтруктураДанныхЛист02.Свойство("П000020004003") Тогда
				Разница = Форма.СтруктураДанныхЛист02.П000020001003 - Форма.СтруктураДанныхЛист02.П000020004003;
			КонецЕсли;
					
			Остаток = 0;
			
			Если Форма.СтруктураДанныхЛист02_Прил4.Свойство("П002040001003") Тогда
				Остаток = Форма.СтруктураДанныхЛист02_Прил4.П002040001003;
			КонецЕсли;
						
			Если Разница > Остаток Тогда
				Результат = Остаток;
			КонецЕсли;
			Если Разница <= Остаток И Разница > 0 Тогда
				Результат = Разница;
			КонецЕсли;
			Если Разница <= 0 Тогда
				Результат = 0;
			КонецЕсли;
			
			Форма.СтруктураДанныхЛист02_Прил4.Вставить("П002040016003", Результат);
			
			Если Форма.СтруктураРеквизитовФормы.НаимТекущегоРаздела = "Лист02_Прил4" Тогда
				Форма.ТабличныйДокумент.Области.П002040016003.Значение = Результат;
			КонецЕсли;
									
		КонецЕсли;	
		
		Если (НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П000020005003, ОбластьИмя) > 0)
		   И (Форма.СтруктураДанныхЛист02_Прил4.Свойство("П002040016003")) Тогда		
		   
		   Если Форма.СтруктураРеквизитовФормы.НаимТекущегоРаздела = "Лист02" Тогда
			   Форма.ТабличныйДокумент.Области.П000020005003.Значение = Форма.СтруктураДанныхЛист02_Прил4.П002040016003;
		   Иначе
			   Форма.СтруктураДанныхЛист02.Вставить("П000020005003", Форма.СтруктураДанныхЛист02_Прил4.П002040016003);
		   КонецЕсли;
			
		КонецЕсли;
		
		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П000020006003, ОбластьИмя) > 0 Тогда		
			Если Форма.СтруктураРеквизитовФормы.НаимТекущегоРаздела = "Лист02" Тогда
				Форма.ТабличныйДокумент.Области.П000020006003.Значение = Форма.ТабличныйДокумент.Области.П000020001003.Значение - Форма.ТабличныйДокумент.Области.П000020004003.Значение - Форма.ТабличныйДокумент.Области.П000020005003.Значение;
			ИначеЕсли Форма.СтруктураДанныхЛист02.Свойство("П000020001003") И Форма.СтруктураДанныхЛист02.Свойство("П000020004003") И Форма.СтруктураДанныхЛист02.Свойство("П000020005003") Тогда
				Форма.СтруктураДанныхЛист02.Вставить("П000020006003", Форма.СтруктураДанныхЛист02.П000020001003 - Форма.СтруктураДанныхЛист02.П000020004003 - Форма.СтруктураДанныхЛист02.П000020005003);
			КонецЕсли;	
		КонецЕсли;
		
		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П000020008003, ОбластьИмя) > 0 Тогда		
			Если Форма.СтруктураРеквизитовФормы.НаимТекущегоРаздела = "Лист02" Тогда
				Форма.ТабличныйДокумент.Области.П000020008003.Значение = (Форма.ТабличныйДокумент.Области.П000020006003.Значение * Форма.ТабличныйДокумент.Области.П000020007003.Значение) / 100;
			ИначеЕсли Форма.СтруктураДанныхЛист02.Свойство("П000020006003") И Форма.СтруктураДанныхЛист02.Свойство("П000020007003") Тогда
				Форма.СтруктураДанныхЛист02.Вставить("П000020008003", (Форма.СтруктураДанныхЛист02.П000020006003 * Форма.СтруктураДанныхЛист02.П000020007003) / 100);
			КонецЕсли;
		КонецЕсли;
		
		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П000020009003, ОбластьИмя) > 0 Тогда		
			Если Форма.СтруктураРеквизитовФормы.НаимТекущегоРаздела = "Лист02" Тогда
				Форма.ТабличныйДокумент.Области.П000020009003.Значение = Форма.ТабличныйДокумент.Области.П000020010003.Значение + Форма.ТабличныйДокумент.Области.П000020011003.Значение;			
			КонецЕсли;
		КонецЕсли;
		
		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П000020012003, ОбластьИмя) > 0 Тогда		
			Если Форма.СтруктураРеквизитовФормы.НаимТекущегоРаздела = "Лист02" Тогда
				Форма.ТабличныйДокумент.Области.П000020012003.Значение = Форма.ТабличныйДокумент.Области.П000020013003.Значение + Форма.ТабличныйДокумент.Области.П000020014003.Значение;			
			КонецЕсли;
		КонецЕсли;
						
	ИначеЕсли ИмяТекущейСтраницыПанели = "Лист02_Прил4" Тогда
		
		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П002040001003, ОбластьИмя) > 0 Тогда
			Форма.ТабличныйДокумент.Области.П002040001003.Значение = Форма.ТабличныйДокумент.Области.П002040002003.Значение
																   + Форма.ТабличныйДокумент.Области.П002040003003.Значение
																   + Форма.ТабличныйДокумент.Области.П002040004003.Значение
																   + Форма.ТабличныйДокумент.Области.П002040005003.Значение
																   + Форма.ТабличныйДокумент.Области.П002040006003.Значение
																   + Форма.ТабличныйДокумент.Области.П002040007003.Значение
																   + Форма.ТабличныйДокумент.Области.П002040008003.Значение
																   + Форма.ТабличныйДокумент.Области.П002040009003.Значение
																   + Форма.ТабличныйДокумент.Области.П002040010003.Значение
																   + Форма.ТабличныйДокумент.Области.П002040011003.Значение
																   + Форма.ТабличныйДокумент.Области.П002040012003.Значение
																   + Форма.ТабличныйДокумент.Области.П002040013003.Значение
																   + Форма.ТабличныйДокумент.Области.П002040014003.Значение
																   + Форма.ТабличныйДокумент.Области.П002040014503.Значение
																   + Форма.ТабличныйДокумент.Области.П002040015003.Значение;
																   
			Форма.СтруктураДанныхЛист02_Прил4.Вставить("П002040001003", Форма.ТабличныйДокумент.Области.П002040001003.Значение);
			
			Расчет(Форма, "Лист02", ОбластьИмя);
		КонецЕсли;
		
	ИначеЕсли ИмяТекущейСтраницыПанели = "" Тогда

		Расчет(Форма, "Лист02_Прил4", ОбластьИмя);
		Расчет(Форма, "Лист02",       ОбластьИмя);	
																	 
	КонецЕсли;
	
КонецПроцедуры // Расчет()

&НаКлиенте
