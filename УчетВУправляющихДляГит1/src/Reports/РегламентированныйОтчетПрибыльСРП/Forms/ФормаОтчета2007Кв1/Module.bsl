					СтруктураДанныхТитульный.ОКВЭД = СокрЛП(СтруктураДанныхТитульный.ОКВЭД) + ".";
				конецЕсли;
			КонецЦикла;	
		ИначеЕсли ИмяПоказателя = "КППСРП1" Тогда
			СтруктураДанныхТитульный.Вставить("КППСРП");
			Для Ном = 1 По 9 Цикл
				СтруктураДанныхТитульный.КППСРП = СокрЛП(СтруктураДанныхТитульный.КППСРП) + ПоказателиТаблПоле["КППСРП" + Ном];
				СтруктураДанныхТитульный.Удалить("КППСРП" + Ном);
			КонецЦикла;	
		ИначеЕсли ИмяПоказателя = "КодВал1" Тогда
			СтруктураДанныхТитульный.Вставить("КодВал");
			Для Ном = 1 По 3 Цикл
				СтруктураДанныхТитульный.КодВал = СокрЛП(СтруктураДанныхТитульный.КодВал) + ПоказателиТаблПоле["КодВал" + Ном];
				СтруктураДанныхТитульный.Удалить("КодВал" + Ном);
			КонецЦикла;
			
		ИначеЕсли ИмяПоказателя = "ДатаПодписиРук1" Тогда
			СтруктураДанныхТитульный.Вставить("ДатаПодписиРук");
			Для Ном = 1 По 8 Цикл
				СтруктураДанныхТитульный.ДатаПодписиРук = СокрЛП(СтруктураДанныхТитульный.ДатаПодписиРук) + ПоказателиТаблПоле["ДатаПодписиРук" + Ном];
				СтруктураДанныхТитульный.Удалить("ДатаПодписиРук" + Ном);
				Если Ном = 2 ИЛИ Ном = 4 Тогда
					СтруктураДанныхТитульный.ДатаПодписиРук = СокрЛП(СтруктураДанныхТитульный.ДатаПодписиРук) + ".";
				конецЕсли;

			КонецЦикла;	
			
		ИначеЕсли ИмяПоказателя = "ДатаПодписиОргУп1" Тогда
			СтруктураДанныхТитульный.Вставить("ДатаПодписиОргУп");
			Для Ном = 1 По 8 Цикл
				СтруктураДанныхТитульный.ДатаПодписиОргУп = СокрЛП(СтруктураДанныхТитульный.ДатаПодписиОргУп) + ПоказателиТаблПоле["ДатаПодписиОргУп" + Ном];
				СтруктураДанныхТитульный.Удалить("ДатаПодписиОргУп" + Ном);
				Если Ном = 2 ИЛИ Ном = 4 Тогда
					СтруктураДанныхТитульный.ДатаПодписиОргУп = СокрЛП(СтруктураДанныхТитульный.ДатаПодписиОргУп) + ".";
				конецЕсли;

			КонецЦикла;	
			
		КонецЕсли;
		
	КонецЦикла;
	
	ЗаполнитьДатуВЯчейках(ЭтаФорма);
	
КонецПроцедуры // ВосстановитьСведенияОбОрганизацииИзТитульногоЛистаСтарогоОбразца()

&НаСервере
Процедура ПоказатьПериод()

	НомерМесяца   = Формат(СтруктураРеквизитовФормы.мДатаКонцаПериодаОтчета, "ДФ=М");
	Если НомерМесяца = "12" Тогда
		НомерМесяца = "0";
	КонецЕсли;

	НомерКвартала = Формат(СтруктураРеквизитовФормы.мДатаКонцаПериодаОтчета, "ДФ=к");
	НомерГода     = Формат(СтруктураРеквизитовФормы.мДатаКонцаПериодаОтчета, "ДФ=гггг");

	// Отображаем на титульном листе отчетный период
	Если СтруктураРеквизитовФормы.мПериодичность = Перечисления.Периодичность.Месяц Тогда
		
		ТабличныйДокумент.Области.НалогПериод.Значение = "1";
		
		СтруктураРеквизитовФормы.СтрПериодОтчета = РегламентированнаяОтчетностьВызовСервера.ПредставлениеФинансовогоПериода(НачалоДня(СтруктураРеквизитовФормы.мДатаНачалаПериодаОтчета), КонецДня(СтруктураРеквизитовФормы.мДатаКонцаПериодаОтчета), "Ложь");
		
	Иначе

		Если НомерКвартала = "4" Тогда
			ТабличныйДокумент.Области.НалогПериод.Значение = "0";
		ИначеЕсли НомерКвартала = "3" Тогда
			ТабличныйДокумент.Области.НалогПериод.Значение = "9";
		ИначеЕсли НомерКвартала = "2" Тогда
			ТабличныйДокумент.Области.НалогПериод.Значение = "6";
		ИначеЕсли НомерКвартала = "1" Тогда
			ТабличныйДокумент.Области.НалогПериод.Значение = "3";
		КонецЕсли;
		
		СтруктураРеквизитовФормы.СтрПериодОтчета = ПредставлениеПериода( НачалоДня(СтруктураРеквизитовФормы.мДатаНачалаПериодаОтчета), КонецДня(СтруктураРеквизитовФормы.мДатаКонцаПериодаОтчета), "ФП = Истина" );

	КонецЕсли;

	ТабличныйДокумент.Области.ОтчетГод.Значение   = НомерГода;	
		
КонецПроцедуры // ПоказатьПериод()

&НаСервере
Процедура ЗаполнитьСведенияОбОрганизации(ВПрограммеИзмененаДатаПодписи = Ложь)
	
	Перем ОргСведения;
	
	Если (СтруктураРеквизитовФормы.Организация = Неопределено) Или (СтруктураРеквизитовФормы.Организация = РегламентированнаяОтчетностьКлиентСервер.ПустоеЗначениеТипа("СправочникСсылка.Организации")) Тогда
		Возврат
	КонецЕсли;
	
	Сведения = "";
	
	Если НЕ ВПрограммеИзмененаДатаПодписи Тогда
		Сведения = "ИННЮЛ, КППЮЛ, НаимЮЛПол, КодНО, НаимНО, ТипНП, ТелОрганизации, ";
	КонецЕсли;
	
	Сведения = Сведения + "ФИОРук";
	
	Результат = РегламентированнаяОтчетностьВызовСервера.ЗаполнитьСведенияОбОрганизацииНаСервере(ЭтаФорма, Сведения);
	
	Если (НЕ ВПрограммеИзмененаДатаПодписи) И (Результат <> Неопределено) Тогда
		
		ТабличныйДокумент.Области["КодИМНС"].Значение = Результат.КодНО;
		ТабличныйДокумент.Области.ИНН1.Значение    	  = Результат.ИННЮЛ;
		ТабличныйДокумент.Области.КПП1.Значение    	  = Результат.КППЮЛ;
			
	КонецЕсли;
	
	РегламентированнаяОтчетность.ПолучитьЗначениеОКВЭД(ДатаПодписи, СтруктураРеквизитовФормы);
	РегламентированнаяОтчетностьКлиентСервер.УстановитьЗначениеОКВЭД(ДатаПодписи,
																	 СтруктураРеквизитовФормы,
																	 ТабличныйДокумент.Области.ОКВЭД.Значение);
	
	РегламентированнаяОтчетностьКлиентСервер.ПроставитьСведенияОПредставителеОрганизации(ЭтаФорма, Ложь);
	
КонецПроцедуры

