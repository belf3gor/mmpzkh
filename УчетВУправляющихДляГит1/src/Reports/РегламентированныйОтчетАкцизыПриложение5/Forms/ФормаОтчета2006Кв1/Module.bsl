			   И Форма.СтруктураДанныхРаздел22.Свойство("П000220036007") И Форма.СтруктураДанныхРаздел22.Свойство("П000220005007") И Форма.СтруктураДанныхРаздел22.Свойство("П000220041007") Тогда				
				Форма.СтруктураДанныхРаздел22.Вставить("П000220051007", Макс(Форма.СтруктураДанныхРаздел22.П000220036007 - Форма.СтруктураДанныхРаздел22.П000220005007 + РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФорматуДелителя(Форма, Форма.СтруктураДанныхРаздел22.П000220041007), 0));
			КонецЕсли;
			
		КонецЕсли;
		
	ИначеЕсли ИмяТекущейСтраницыПанели = "Приложение1" Тогда

		ОбновитьСтруктуруМногострочногоРаздела(Форма, Форма.СтруктураРеквизитовФормы.мИдГруппы1);

		Форма.РасчетПриложения1НаСервере();
		
	ИначеЕсли ИмяТекущейСтраницыПанели = "Приложение2" Тогда

		ОбновитьСтруктуруМногострочногоРаздела(Форма, Форма.СтруктураРеквизитовФормы.мИдГруппы2);

		Форма.РасчетПриложения2НаСервере();
		
	ИначеЕсли ИмяТекущейСтраницыПанели = "Приложение3" Тогда
		
		ИндексЯчейки = 1;
		
		Если ЗначениеЗаполнено(ОбластьИмя) Тогда
			
			ИндексЯчейки = Сред(ОбластьИмя, СтрНайти(ОбластьИмя, "_") + 1);
			
		КонецЕсли;
		
		Форма.ТабличныйДокумент.Области["П000260001006_" + ИндексЯчейки].Значение = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФорматуДелителя(Форма, Форма.ТабличныйДокумент.Области["П000260001002_" + ИндексЯчейки].Значение)
		+ РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФорматуДелителя(Форма, Форма.ТабличныйДокумент.Области["П000260001003_" + ИндексЯчейки].Значение)
		+ РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФорматуДелителя(Форма, Форма.ТабличныйДокумент.Области["П000260001004_" + ИндексЯчейки].Значение);
		
	ИначеЕсли ИмяТекущейСтраницыПанели = "" Тогда
		
		// Выполняем расчет вычисляемых показателей отчета на всех страницах
		Расчет("Раздел21");
		Расчет("Раздел22");
		Расчет("Раздел23");
		Расчет("Приложение1");
		Расчет("Приложение2");
		Расчет("Приложение3");
		Расчет("Раздел1");	
				
	КонецЕсли;

КонецПроцедуры // Расчет()

