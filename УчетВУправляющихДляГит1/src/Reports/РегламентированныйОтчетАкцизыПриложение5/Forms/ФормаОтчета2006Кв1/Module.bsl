&НаКлиенте
Процедура ВвестиСтрокуЗавершение(Строка, ДополнительныеПараметры1) Экспорт
	
	НомерСтрокиВМногострочнойЧасти = ДополнительныеПараметры1.НомерСтрокиВМногострочнойЧасти;
	ПолеТабличногоДокументаРаздел1 = ДополнительныеПараметры1.ПолеТабличногоДокументаРаздел1;
	ТекущееЗначениеКБК = ?(Строка = Неопределено, ДополнительныеПараметры1.ТекущееЗначениеКБК, Строка);
	
	
	Если (Строка <> Неопределено) Тогда
		Если НомерСтрокиВМногострочнойЧасти = "1" Тогда
			ПолеТабличногоДокументаРаздел1.Области.КБКРаздел1_1.Значение = ТекущееЗначениеКБК;
		ИначеЕсли НомерСтрокиВМногострочнойЧасти = "2" Тогда
			ПолеТабличногоДокументаРаздел1.Области.КБКРаздел1_2.Значение = ТекущееЗначениеКБК;
		ИначеЕсли НомерСтрокиВМногострочнойЧасти = "3" Тогда
			ПолеТабличногоДокументаРаздел1.Области.КБКРаздел1_3.Значение = ТекущееЗначениеКБК;
		КонецЕсли;
		
		ПолеТабличногоДокументаРаздел1.Области["КБКРаздел1_" + НомерСтрокиВМногострочнойЧасти].Значение = ТекущееЗначениеКБК;
		Модифицированность = Истина;
		
	КонецЕсли
	
КонецПроцедуры

&НаКлиенте
Процедура ОткрытьФормуВыбораЗначенияИзТаблицыЗавершение(Результат1, ДополнительныеПараметры1) Экспорт
    
    НомерСтрокиВМногострочнойЧасти = ДополнительныеПараметры1.НомерСтрокиВМногострочнойЧасти;
    ПолеТабличногоДокументаРаздел1 = ДополнительныеПараметры1.ПолеТабличногоДокументаРаздел1;
    
    
    Результат = Результат1;
    
    Если Результат = Неопределено Тогда
        Возврат;
    КонецЕсли;
    
    Если НомерСтрокиВМногострочнойЧасти = "1" Тогда
        ПолеТабличногоДокументаРаздел1.Области.КБКРаздел1_1.Значение = Результат["Код"];
    ИначеЕсли НомерСтрокиВМногострочнойЧасти = "2" Тогда
        ПолеТабличногоДокументаРаздел1.Области.КБКРаздел1_2.Значение = Результат["Код"];
    ИначеЕсли НомерСтрокиВМногострочнойЧасти = "3" Тогда
        ПолеТабличногоДокументаРаздел1.Области.КБКРаздел1_3.Значение = Результат["Код"];
    КонецЕсли;
    
    ПолеТабличногоДокументаРаздел1.Области["КБКРаздел1_" + НомерСтрокиВМногострочнойЧасти].Значение = Результат["Код"];
    Модифицированность = Истина;

КонецПроцедуры

&НаКлиенте
Процедура ОткрытьФормуКонтактнойИнформацииЗавершение(Результат, Параметры) Экспорт
	
	ОбновитьАдресВТабличномДокументе(Результат, Параметры.РоссийскийАдрес);
	
КонецПроцедуры

&НаКлиенте
Процедура ОбновитьАдресВТабличномДокументе(Результат, РоссийскийАдрес)
	
	Если ТипЗнч(Результат) = Тип("Структура") Тогда
		
		// Обход ошибки платформы: в веб-клиенте, в результате выполнения процедуры "СформироватьАдрес"
		// общего модуля "РегламентированнаяОтчетностьВызовСервера", не изменяются значения ключей
		// соответствия "РоссийскийАдрес", передаваемого в качестве параметра.
		РоссийскийАдрес_ = РоссийскийАдрес;
		
		РегламентированнаяОтчетностьВызовСервера.СформироватьАдрес(Результат.КонтактнаяИнформация, РоссийскийАдрес_);
		
		ТабличныйДокумент.Области.ПочтИнд.Значение       = РоссийскийАдрес_["Индекс"];
		ТабличныйДокумент.Области.КодСубъекта.Значение   = РоссийскийАдрес_["КодРегиона"];
		ТабличныйДокумент.Области.П000230001201.Значение = РоссийскийАдрес_["Регион"];
		ТабличныйДокумент.Области.П000230001300.Значение = РоссийскийАдрес_["Район"];
		ТабличныйДокумент.Области.П000230001400.Значение = РоссийскийАдрес_["Город"];
		ТабличныйДокумент.Области.П000230001500.Значение = РоссийскийАдрес_["НаселенныйПункт"];
		ТабличныйДокумент.Области.П000230001600.Значение = РоссийскийАдрес_["Улица"];
		ТабличныйДокумент.Области.П000230001700.Значение = РоссийскийАдрес_["Дом"];
		ТабличныйДокумент.Области.П000230001800.Значение = РоссийскийАдрес_["Корпус"];
		ТабличныйДокумент.Области.П000230001900.Значение = РоссийскийАдрес_["Квартира"];
		
		Модифицированность = Истина;
		
	КонецЕсли;
	
КонецПроцедуры

&НаКлиенте
Процедура ТабличныйДокументПриИзмененииСодержимогоОбласти(Элемент, Область)
	
	Модифицированность = Истина;
	
	Если Элементы.РазделыОтчета.ТекущиеДанные.КолонкаРазделыОтчетаСокрНаим = "Титульный" Тогда
		Если Область.Имя = "ДатаПодписи" Тогда 
			ДатаПодписи = Область.Значение;
			ЗаполнитьСведенияОбОрганизации(Истина);
			СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация = Истина;
			ЗаполнитьДатуВЯчейкахНаКлиенте();
			СтруктураРеквизитовФормы.мВПрограммеИзмененаОрганизация = Ложь;
			Модифицированность = Истина;
		ИначеЕсли Область.Имя = "НомКоррект" Тогда 
			ОписаниеТипа = Новый ОписаниеТипов("Число");
			НомерКорректировки = ОписаниеТипа.ПривестиЗначение(Область.Значение);
			Область.Значение = Строка(НомерКорректировки);
			СтруктураРеквизитовФормы.ВидДокумента = ?(НомерКорректировки = 0, 0, 1);
			ТабличныйДокумент.Области["ВидДок"].Значение = ?(НомерКорректировки = 0, "1", "3");
			Модифицированность = Истина;
		КонецЕсли;
	КонецЕсли;
	
	Если Лев(Область.Имя, 11) = "ОКАТОРаздел" Тогда
		
		Если ЗначениеЗаполнено(Область.Значение) Тогда
			ДлинаЗначения = СтрДлина(СокрЛП(Область.Значение));
			Для Инд = ДлинаЗначения+1 По 11 Цикл
				Область.Значение = СокрЛП(Область.Значение) + "0";
			КонецЦикла;
		КонецЕсли;
		
		Если СтрДлина(СокрЛП(Область.Значение)) <> 0 И СтрДлина(СокрЛП(Область.Значение)) <> 11 Тогда
									
			Сообщение = Новый СообщениеПользователю;

			Сообщение.Текст = НСтр("ru='Длина значения показателя должна быть равна 11!'");

			Сообщение.Сообщить();
			
		КонецЕсли;
		
		ИмяТекОбласти = Лев(Область.Имя, 9);
			
	ИначеЕсли СтрНайти(Область.Имя, "КППРаздел23") > 0 Тогда
		
		Если (Область.Значение = "") Или (СтроковыеФункцииКлиентСервер.ТолькоЦифрыВСтроке(Область.Значение)) И (СтрДлина(Область.Значение) = 9) Тогда
		
		Иначе
			
			ПолеТабличногоДокументаРаздел23 = ТабличныйДокумент;
			
			ПолеТабличногоДокументаРаздел23.Области["КППРаздел23"].Значение = "";
			
			Сообщение = Новый СообщениеПользователю;
			
			Сообщение.Текст = НСтр("ru='Код КПП должен состоять из 9 цифр.'");
			
			Сообщение.Сообщить();
			
		КонецЕсли;
							
	КонецЕсли;	
	
	ИмяТекущейСтраницыПанели = Элементы.РазделыОтчета.ТекущиеДанные.КолонкаРазделыОтчетаСокрНаим;

	ОбластьИмя = Элемент.ТекущаяОбласть.Имя;

	Если ИмяТекущейСтраницыПанели = "Раздел21" Тогда
		
		РасчетНаКлиенте("Раздел21", ОбластьИмя);
					
	ИначеЕсли ИмяТекущейСтраницыПанели = "Раздел22" Тогда
			
		РасчетНаКлиенте("Раздел22", ОбластьИмя);
		                           	
	ИначеЕсли ИмяТекущейСтраницыПанели = "Приложение1" Тогда
        		
		Если СтрНайти(ОбластьИмя, "П000240010002") > 0 
		   Или СтрНайти(ОбластьИмя, "П000240010005") > 0 Тогда
			#Если ВебКлиент Тогда
				РасчетНаСервере("Приложение1", ОбластьИмя);
			#Иначе
				РасчетНаКлиенте("Приложение1", ОбластьИмя);
			#КонецЕсли
		КонецЕсли;

	ИначеЕсли ИмяТекущейСтраницыПанели = "Приложение2" Тогда

		Если СтрНайти(ОбластьИмя, "П000250010002") > 0 
		   Или СтрНайти(ОбластьИмя, "П000250010005") > 0 Тогда
			#Если ВебКлиент Тогда
				РасчетНаСервере("Приложение2", ОбластьИмя);
			#Иначе
				РасчетНаКлиенте("Приложение2", ОбластьИмя);
			#КонецЕсли
		КонецЕсли;

	ИначеЕсли ИмяТекущейСтраницыПанели = "Приложение3" Тогда
		
		Если (СтрНайти(ОбластьИмя, "П000260001002") > 0 ) Или (СтрНайти(ОбластьИмя, "П000260001003") > 0) Или (СтрНайти(ОбластьИмя, "П000260001004") > 0) Тогда
			РасчетНаКлиенте("Приложение3", ОбластьИмя);
		КонецЕсли;
		
    КонецЕсли;
	
КонецПроцедуры // ТабличныйДокументПриИзмененииСодержимогоОбласти()

&НаСервере
Процедура ОчиститьНаСервере() Экспорт
	
	СтруктураДанныхРаздел21.Очистить();
	СтруктураДанныхРаздел22.Очистить();
	СтруктураДанныхПриложение3.Очистить();
		
	ТаблицаСтраницыРаздела1.Очистить();
	ТаблицаСтраницыРаздела23.Очистить();
	ТаблицаСтраницыРаздела24.Очистить();
	ТаблицаСтраницыРаздела25.Очистить();
	
	ИнициализацияМногостраничныхРазделов();
	
	Для Каждого мнгРаздел Из мСтруктураМногострочныхРазделов Цикл
		ЭтаФорма[мнгРаздел.Ключ].Очистить();
	КонецЦикла;
	
	НачальныеОперацииСМногострочнымиБлоками();
	
	ЗаполнитьПустыеСтруктурыДанных();
	
	Если НЕ СтруктураРеквизитовФормы.НаимТекущегоРаздела = "Титульный"
	   И НЕ СтруктураРеквизитовФормы.НаимТекущегоРаздела = "ФизЛица" Тогда
		ОчиститьТабличноеПолеНаСервере(СтруктураРеквизитовФормы.НаимТекущегоРаздела);
	КонецЕсли;
		
	ВывестиМакетСтраницыВТабличныйДокументНаСервере(СтруктураРеквизитовФормы.НаимТекущегоРаздела, 1);
		
	СтруктураРеквизитовФормы.ВыводитьСтраницуВТаблДокНаСервере = Ложь;
	
	РегламентированнаяОтчетностьКлиентСервер.ОчиститьВариантыЗаполнения(ЭтаФорма);
	
КонецПроцедуры // ОчиститьНаСервере()

