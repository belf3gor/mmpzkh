&НаСервере
Функция ПроверитьКонтрольныеСоотношенияВОтчете() Экспорт
	
	СтруктураОшибок = Новый Структура();
	СтруктураРезультата = Новый Структура;
	СтруктураРезультата.Вставить("КоличествоНайденныхОшибок",    0);
	СтруктураРезультата.Вставить("КоличествоНайденныхБезОшибок", 0);
	СтруктураРезультата.Вставить("СтруктураОшибок", СтруктураОшибок);
	
	ОбщееЧислоОшибок = 0; // Счетчик учета ошибок.
	ОбщееЧислоБезОшибок = 0; // Счетчик учета проверенных соотношений, не являющиеся ошибками.
	
	ПоказателиОтчета = Новый Структура();
	
	ЗаполнитьПоказателиОтчетаНаСервере(ПоказателиОтчета);
	
	Показатели = ПоказателиОтчета.ПолеТабличногоДокументаФормаОтчета; 
	
	Нарушение = "Приказ Росстата от 24 октября 2011 г. N 435";
	
	//1
	
	Знак = ">=";
	
	Для СчетчикСтрок = 3 по 27 Цикл
		
		Если СчетчикСтрок = 4 ИЛИ СчетчикСтрок = 14 Тогда
			
			Продолжить;
			
		КонецЕсли;
		
		НомерСтроки = РегламентированнаяОтчетностьКлиентСервер.ДополнитьСтроку(СчетчикСтрок, 2, "0");
		
		КлючПоля_ЛеваяЧасть = "П01000" + НомерСтроки + "01";
		КлючПоля_ПраваяЧасть = "П01000" + НомерСтроки + "02";
		
		ТекстФормулы = "Стр. " + НомерСтроки + " гр. 1 " + Знак + " Стр. " + НомерСтроки + " гр. 2"; 
		Описание = "Стр. " + НомерСтроки + " гр. 1 (Всего)" + " " + Знак + " " + "Стр. " + НомерСтроки + "гр. 2 (Из нее просроченная)";
		
		ЛеваяЧасть  = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели[КлючПоля_ЛеваяЧасть]);
		
		ПраваяЧасть = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели[КлючПоля_ПраваяЧасть]);
		
		//Для сравнения будем использовать только положительные числа 
		ЛеваяЧасть = Макс(ЛеваяЧасть, - ЛеваяЧасть);
		
		ПраваяЧасть = Макс(ПраваяЧасть, - ПраваяЧасть);
		
		Расшифровка = Строка(ЛеваяЧасть) + " " + Знак + " " + Строка(ПраваяЧасть);
		
		Если ЛеваяЧасть >= ПраваяЧасть Тогда
			
			ЭтоОшибка = Ложь;
			ОбщееЧислоБезОшибок = ОбщееЧислоБезОшибок + 1;
			
		Иначе
			
			ЭтоОшибка = Истина;
			ОбщееЧислоОшибок = ОбщееЧислоОшибок + 1;
			
		КонецЕсли;
		
		РегламентированнаяОтчетность.ДобавитьЭлементВСтруктуруОшибок(СтруктураОшибок, ТекстФормулы, "", ОбщееЧислоОшибок, ОбщееЧислоБезОшибок, Расшифровка, Нарушение, Описание, ЭтоОшибка);
		
	КонецЦикла;
	
	//2
	
	Знак = "<=";
	
	ТекстФормулы = "стр. 05 <= стр. 03"; 
	Описание = "стр. 05 (Всего)" + " " + Знак + " " + "стр. 03 (Всего)";
	
	ЛеваяЧасть = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели["П010000501"]);
	
	ПраваяЧасть = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели["П010000301"]);
	
	Расшифровка = Строка(ЛеваяЧасть) + " " + Знак + " " + Строка(ПраваяЧасть);
	
	Если ЛеваяЧасть <= ПраваяЧасть Тогда
		
		ЭтоОшибка = Ложь;
		ОбщееЧислоБезОшибок = ОбщееЧислоБезОшибок + 1;
		
