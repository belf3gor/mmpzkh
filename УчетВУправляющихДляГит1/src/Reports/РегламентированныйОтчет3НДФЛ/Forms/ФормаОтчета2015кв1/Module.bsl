		// только пересчет итогов, выполняется в конце процедуры
	
	ИначеЕсли ИмяРаздела = "ЛистЗПродолжение3" Тогда
		
#Область Пересчет_ЛистЗПродолжение3

	ЛистЗПродолжение3 = Форма.СтруктураДанныхЛистЗПродолжение3;
	ЛистЗ = Форма.СтруктураДанныхЛистЗ;
	ЛистЗПродолжение = Форма.СтруктураДанныхЛистЗПродолжение;
	
	ЛистЗПродолжение3.П001500001409 = Макс(ЛистЗПродолжение3.П001500001407 - ЛистЗПродолжение3.П001500001408, 0);
	Форма.ТабличныйДокумент.Области["П001500001409"].Значение = ЛистЗПродолжение3.П001500001409;
	
	ЛистЗПродолжение3.П001500001410 = Макс(ЛистЗПродолжение3.П001500001408 - ЛистЗПродолжение3.П001500001407, 0);
	Форма.ТабличныйДокумент.Области["П001500001410"].Значение = ЛистЗПродолжение3.П001500001410;
	
