			Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИные, "ДохОперЦБОргНеОрг");
		КонецЕсли;
		
		// Если у узла заполнен вид опеации. то считаем его заполненным
		Если Узел <> Неопределено Тогда
			
			ЗаполнитьДаннымиУзел(Узел, СтраницаЛистаЗ.Данные[0].Значение);
			
			Узел = Неопределено;
			Если СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "1" Тогда
				Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИИС, "ДохОперЦБНеОрг");
			ИначеЕсли СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "2" Тогда
				Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИные, "ДохОперЦБНеОрг");
			КонецЕсли;
			
			Если Узел <> Неопределено Тогда
				ЗаполнитьДаннымиУзел(Узел, СтраницаЛистаЗ.Данные[0].Значение);
			КонецЕсли;
			
		КонецЕсли;
		
	КонецЦикла;
	
	// Продолжение 2 Листа З
	
	Для Каждого СтраницаЛистаЗ Из мДанныеОтчета.ЛистЗПродолжение2 Цикл
		
		Узел = Неопределено;
		Если СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "1" Тогда
			ПоказатьИтоговыйРазделИИС = Истина;
			Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИИС, "ДохПФИОргЦБ");
		ИначеЕсли СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "2" Тогда
			ПоказатьИтоговыйРазделИные = Истина;
			Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИные, "ДохПФИОргЦБ");
		КонецЕсли;
		
		Если Узел <> Неопределено Тогда
			
			ЗаполнитьДаннымиУзел(Узел, СтраницаЛистаЗ.Данные[0].Значение);
			
			Узел = Неопределено;
			Если СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "1" Тогда
				Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИИС, "ДохПФИОргНеЦБ");
			ИначеЕсли СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "2" Тогда
				Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИные, "ДохПФИОргНеЦБ");
			КонецЕсли;
			
			Если Узел <> Неопределено Тогда
				ЗаполнитьДаннымиУзел(Узел, СтраницаЛистаЗ.Данные[0].Значение);
			КонецЕсли;
			
			Узел = Неопределено;
			Если СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "1" Тогда
				Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИИС, "ДохПФИНеОрг");
			ИначеЕсли СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "2" Тогда
				Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИные, "ДохПФИНеОрг");
			КонецЕсли;
			
			Если Узел <> Неопределено Тогда
				ЗаполнитьДаннымиУзел(Узел, СтраницаЛистаЗ.Данные[0].Значение);
			КонецЕсли;
			
		КонецЕсли;
	КонецЦикла;
	
	// Продолжение 3 Листа З
	
	Для Каждого СтраницаЛистаЗ Из мДанныеОтчета.ЛистЗПродолжение3 Цикл
		
		Узел = Неопределено;
		Если СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "1" Тогда
			ПоказатьИтоговыйРазделИИС = Истина;
			Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИИС, "ДохРЕПО");
		ИначеЕсли СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "2" Тогда
			ПоказатьИтоговыйРазделИные = Истина;
			Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИные, "ДохРЕПО");
		КонецЕсли;
		
		Если Узел <> Неопределено Тогда
			ПоказатьИтоговыйРаздел = Истина;
			
			ЗаполнитьДаннымиУзел(Узел, СтраницаЛистаЗ.Данные[0].Значение);
			
			Узел = Неопределено;
			Если СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "1" Тогда
				Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИИС, "ДохЗаймЦБ");
			ИначеЕсли СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "2" Тогда
