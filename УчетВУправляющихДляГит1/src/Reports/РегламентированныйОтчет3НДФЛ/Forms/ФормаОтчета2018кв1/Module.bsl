// Перезаполняет страницы Раздела 2
//	Для каждой ставки в сводной таблице доходов создаем отдельную страницу раздела 2, в которой вычисляем 
//	результат (к доплате или к возмещению).
//	Полученный результат распределяем в колонке "РезультатРасчетов" таблицы "СводДоходов" по строкам с рассчитанной ставкой,
//	т.к. Раздел 2 должен заполняться уже не по ставкам, а по сочетанию КБК и ОКТМО
&НаКлиентеНаСервереБезКонтекста
Процедура ПодготовитьДанныеРаздела2(Форма)
	
	ТаблицаСтраницыРаздел2 = Форма.ТаблицаСтраницыРаздел2;
	
	ОбразецСтруктуры = Новый Структура(Новый ФиксированнаяСтруктура(ТаблицаСтраницыРаздел2[0].Данные[0].Значение));
	Для Каждого ЭлементСтруктуры Из ОбразецСтруктуры Цикл
		Если ТипЗнч(ЭлементСтруктуры.Значение) = Тип("Число") Тогда
			ОбразецСтруктуры[ЭлементСтруктуры.Ключ] = 0;
		Иначе
			ОбразецСтруктуры[ЭлементСтруктуры.Ключ] = "";
		КонецЕсли;
	КонецЦикла;
	
	ТаблицаСтраницыРаздел2.Очистить();
	
	ВидыДоходов = Новый Массив;
	ВидыДоходов.Добавить(ОбычныйДоход());
	ВидыДоходов.Добавить(ДоходыКонтролируемыхКомпаний());
	ВидыДоходов.Добавить(Дивиденды());
	
	Для Каждого ВидДохода Из ВидыДоходов Цикл
		
		Для Каждого СтрокаСтавки Из Форма.мСтавкиНалога Цикл
			
			Если ПустаяСтрока(СтрокаСтавки.Код) Тогда
				Продолжить;
			КонецЕсли;
			
			СтрокиДохода = Форма.СводДоходов.НайтиСтроки(Новый Структура("Ставка, ВидДохода", Число(СтрокаСтавки.Код), ВидДохода));
			
			Если СтрокиДохода.Количество() > 0 Тогда
				
				СтраницаРаздела = ТаблицаСтраницыРаздел2.Добавить();
				СтраницаРаздела.Представление               = СтрокаСтавки.Код + " %";
				СтраницаРаздела.АвтоматическоеПредставление = Истина;
				СтраницаРаздела.Данные.Добавить(Новый Структура(Новый ФиксированнаяСтруктура(ОбразецСтруктуры)));
				
				ЗаполнитьРасчетныеДанныеРаздела2(Форма, СтраницаРаздела, Число(СтрокаСтавки.Код), ВидДохода);
				
			КонецЕсли;
			
		КонецЦикла; 
			
	КонецЦикла;
	
	Если ТаблицаСтраницыРаздел2.Количество() = 0 Тогда
		// Добавляем пустую страницу со значениями по умолчанию.
		
		СтраницаРаздела = ТаблицаСтраницыРаздел2.Добавить();
		СтраницаРаздела.Представление               = "Новая страница";
		СтраницаРаздела.АктивнаяСтраница            = Истина;
		СтраницаРаздела.АвтоматическоеПредставление = Истина;
		
		СтрокиСтавка13 = Форма.мСтавкиНалога.НайтиСтроки(Новый Структура("Код", "13"));
		Если СтрокиСтавка13.Количество() > 0 Тогда
			Ставка13 = СтрокиСтавка13[0];
			СтраницаРаздела.Представление               = Ставка13.Код + " %";
			СтраницаРаздела.АвтоматическоеПредставление = Истина;
			СтраницаРаздела.Данные.Добавить(Новый Структура(Новый ФиксированнаяСтруктура(ОбразецСтруктуры)));
			ЗаполнитьРасчетныеДанныеРаздела2(Форма, СтраницаРаздела, Число(Ставка13.Код), ОбычныйДоход());
		Иначе
			СтраницаРаздела.Данные.Добавить(Новый Структура(Новый ФиксированнаяСтруктура(ОбразецСтруктуры)));
		КонецЕсли;
		
	Иначе
		
		ТаблицаСтраницыРаздел2[0].АктивнаяСтраница  = Истина;
		
	КонецЕсли; 
	
	
КонецПроцедуры

&НаКлиенте
Процедура СобратьДанныеВРаздел1()
	
	СобратьДанныеВРаздел1Сервер();
	
	// Обновляем дерево разделов
	СформироватьДеревоРазделовОтчета(ЭтотОбъект);
	
	// Выводим макет в табличный документ
	ВывестиМакетВТаблДокНаСервере("Раздел1", 1, , , Истина);
	
	// Активизируем строку раздела в дереве
	Для Каждого СтрокаРаздела Из РазделыОтчета.ПолучитьЭлементы() Цикл
		Если СтрокаРаздела.КолонкаРазделыОтчетаСокрНаим = "Раздел1" Тогда
			Элементы.РазделыОтчета.ТекущаяСтрока = СтрокаРаздела.ПолучитьИдентификатор();
			Прервать;
		КонецЕсли; 
	КонецЦикла; 
	
	Модифицированность = Истина;
	
КонецПроцедуры

&НаСервере
Процедура СобратьДанныеВРаздел1Сервер()
	
	// Итоги пересчитываем только в том случае, если не заполнена таблица свода доходов
	Если СводДоходов.Количество() = 0 Тогда
		РассчитатьИтогиЛиста(ЭтотОбъект, Неопределено);
		ЗаполнитьСводныеТаблицы(ЭтотОбъект);
	КонецЕсли;
	
	ОКТМОНАлогоплательщика = СтруктураРеквизитовФормы.Октмо;
	
	// Сначала распределим результаты расчета Раздела 2 по своду доходов
	Для Каждого СтраницаРаздела Из ТаблицаСтраницыРаздел2 Цикл
		
		Если СтраницаРаздела.Данные.Количество() = 0
			ИЛИ СтраницаРаздела.Данные[0].Значение.П000200001001 = 0
			ИЛИ СтраницаРаздела.Данные[0].Значение.П000200001010 
				+ СтраницаРаздела.Данные[0].Значение.П000200001051 = 0
			Тогда
			Продолжить;
		КонецЕсли;
		
		ДанныеСтраницы = СтраницаРаздела.Данные[0].Значение;
		
		ОстатокКУплате     = Число(ДанныеСтраницы.П000200001130);
		ОстатокКВозмещению = Число(ДанныеСтраницы.П000200001140);
		ПоследняяСтрока    = Неопределено;
		
		// Находим строки доходов по указанной ставке
		СтрокиДоходов = СводДоходов.НайтиСтроки(Новый Структура("Ставка", Число(СтраницаРаздела.Данные[0].Значение.П000200001001)));
		Для Каждого СтрокаДохода Из СтрокиДоходов Цикл
		
			Если ОстатокКУплате > 0 Тогда
				СтрокаДохода.ОКТМО             = ОКТМОНАлогоплательщика; // доплата налога всегда по месту регистрации налогоплательщика
				СтрокаДохода.РезультатРасчетов = Мин(ОстатокКУплате, СтрокаДохода.НачисленоНалога - СтрокаДохода.УдержаноНалога);
				ОстатокКУплате                 = Макс(0, ОстатокКУплате - СтрокаДохода.РезультатРасчетов);
			КонецЕсли;
			
			Если ОстатокКВозмещению > 0 Тогда
				НалогУплачен = СтрокаДохода.УплаченоНалога + СтрокаДохода.УдержаноНалога + СтрокаДохода.НалогПринимаемыйКУчету; // возмещается только то, что уплачено или удержано
				СтрокаДохода.РезультатРасчетов = - Мин(ОстатокКВозмещению, НалогУплачен); // пишем со знаком минус для получения итоговой суммы при сворачивании по КБК и ОКТМО
				ОстатокКВозмещению             = Макс(0, ОстатокКВозмещению + СтрокаДохода.РезультатРасчетов);
			КонецЕсли;
			
			Если СтрокаДохода.РезультатРасчетов <> 0 Тогда
				ПоследняяСтрока = СтрокаДохода;
			КонецЕсли;
			
		КонецЦикла;
		
		// Убеждаемся, что распределен весь результат расчетов
		Если ПоследняяСтрока <> Неопределено Тогда
			Если ОстатокКУплате > 0 Тогда
				ПоследняяСтрока.РезультатРасчетов = ПоследняяСтрока.РезультатРасчетов + ОстатокКУплате;
			КонецЕсли;
			Если ОстатокКВозмещению > 0 Тогда
				ПоследняяСтрока.РезультатРасчетов = ПоследняяСтрока.РезультатРасчетов - ОстатокКВозмещению;
			КонецЕсли;
		КонецЕсли;
		
	КонецЦикла;
	
	// Свернем таблицу доходов по КБК и ОКТМО, чтобы получить итоги расчетов
	ТаблицаРасчетов = СводДоходов.Выгрузить();
	ТаблицаРасчетов.Свернуть("КБК,ОКТМО", "РезультатРасчетов");
	
	// Сохраним образец структуры реквизитов для заполнения добавленный страниц
	ОбразецСтруктуры = Новый Структура(Новый ФиксированнаяСтруктура(ТаблицаСтраницыРаздел1[0].Данные[0].Значение));
	Для Каждого ЭлементСтруктуры Из ОбразецСтруктуры Цикл
		Если ТипЗнч(ЭлементСтруктуры.Значение) = Тип("Число") Тогда
			ОбразецСтруктуры[ЭлементСтруктуры.Ключ] = 0;
		Иначе
			ОбразецСтруктуры[ЭлементСтруктуры.Ключ] = "";
		КонецЕсли; 
	КонецЦикла; 
	
	// очистим страницы раздела
	ТаблицаСтраницыРаздел1.Очистить();
	
	// Для каждой пары КБК+ОКТМО создаем страницу раздела 1
	Для Каждого СтрокаРасчета Из ТаблицаРасчетов Цикл
		
		СтраницаРасчета = ТаблицаСтраницыРаздел1.Добавить();
		СтраницаРасчета.Представление               = "Страница N " + ТаблицаСтраницыРаздел1.Количество();
		СтраницаРасчета.АвтоматическоеПредставление = Истина;
		СтраницаРасчета.Данные.Добавить(Новый Структура(Новый ФиксированнаяСтруктура(ОбразецСтруктуры)));
		ДанныеСтраницыРасчета = СтраницаРасчета.Данные[0].Значение;
		
		ДанныеСтраницыРасчета.П000100001020 = СтрокаРасчета.КБК;
		
		Если ЗначениеЗаполнено(СтрокаРасчета.ОКТМО) Тогда
			ДанныеСтраницыРасчета.П000100001030 = СтрокаРасчета.ОКТМО;
		Иначе
			ДанныеСтраницыРасчета.П000100001030 = ОКТМОНАлогоплательщика;
		КонецЕсли;
	
		Если СтрокаРасчета.РезультатРасчетов > 0 Тогда
			
			ДанныеСтраницыРасчета.П000100001010 = "1";
			ДанныеСтраницыРасчета.П000100001040 = СтрокаРасчета.РезультатРасчетов;
			ДанныеСтраницыРасчета.П000100001050 = 0;
			
		ИначеЕсли СтрокаРасчета.РезультатРасчетов < 0 Тогда
			
			ДанныеСтраницыРасчета.П000100001010 = "2";
			ДанныеСтраницыРасчета.П000100001040 = 0;
			ДанныеСтраницыРасчета.П000100001050 = - СтрокаРасчета.РезультатРасчетов;
			
		Иначе
			
			ДанныеСтраницыРасчета.П000100001010 = "3";
			ДанныеСтраницыРасчета.П000100001040 = 0;
			ДанныеСтраницыРасчета.П000100001050 = 0;
			
		КонецЕсли;
		
	КонецЦикла;
	
	// Если не оказалось новых страниц раздела, добавим пустую
	Если ТаблицаСтраницыРаздел1.Количество() = 0 Тогда
		СтраницаРаздела = ТаблицаСтраницыРаздел1.Добавить();
		СтраницаРаздела.Представление               = "Новая страница";
		СтраницаРаздела.АктивнаяСтраница            = Истина;
		СтраницаРаздела.АвтоматическоеПредставление = Истина;
		
		ДанныеСтраницыРасчета = Новый Структура(Новый ФиксированнаяСтруктура(ОбразецСтруктуры));
		ДанныеСтраницыРасчета.П000100001030 = ОКТМОНАлогоплательщика;
		ДанныеСтраницыРасчета.П000100001010 = "3";
		ДанныеСтраницыРасчета.П000100001020 = КБКДоходовОтПредпринимательскойДеятельности();
		СтраницаРаздела.Данные.Добавить(ДанныеСтраницыРасчета);
		
	Иначе
		
		ТаблицаСтраницыРаздел1[0].АктивнаяСтраница  = Истина;
		
	КонецЕсли;
	
КонецПроцедуры

