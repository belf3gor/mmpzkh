	ПоказатьИтоговыйРазделИные = Ложь;
	
	// Лист З
	Для Каждого СтраницаЛистаЗ Из ДанныеРаздела Цикл
		
		Узел = Неопределено;
		Если СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "1" Тогда
			ПоказатьИтоговыйРазделИИС = Истина;
			Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИИС, "ДохОперЦБОрг");
		ИначеЕсли СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "2" Тогда
			ПоказатьИтоговыйРазделИные = Истина;
			Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИные, "ДохОперЦБОрг");
		КонецЕсли;
		
		Если ПоказатьИтоговыйРазделИИС ИЛИ ПоказатьИтоговыйРазделИные Тогда
			ЗаполнитьДаннымиУзел(Узел, СтраницаЛистаЗ.Данные[0].Значение);
		КонецЕсли;
		
	КонецЦикла;
	
	// Продолжение Листа З
	Для Каждого СтраницаЛистаЗ Из мДанныеОтчета.ЛистЗПродолжение Цикл
		
		Узел = Неопределено;
		Если СтраницаЛистаЗ.Данные[0].Значение.П001200001100 = "1" Тогда
			ПоказатьИтоговыйРазделИИС = Истина;
			Узел = ПолучитьПодчиненныйЭлемент(УзелЛистаЗСПродолжениямиИИС, "ДохОперЦБОргНеОрг");
