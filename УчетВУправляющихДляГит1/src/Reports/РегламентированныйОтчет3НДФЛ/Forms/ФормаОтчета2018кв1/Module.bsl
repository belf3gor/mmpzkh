&НаКлиентеНаСервереБезКонтекста
Процедура ЗаполнитьСводныеТаблицы(Форма)

	//Очищаем покатазети
	Форма.СводДоходов.Очистить();
	Форма.СводВычетов.Очистить();
	ОКТМОналогоплательщика = "";
	
	// Перебираем строки Листа А:
	//
	Для Каждого Лист Из Форма.ТаблицаСтраницыЛистА Цикл
		
		Если Лист.Данные.Количество() = 0 ИЛИ Лист.ДанныеДопСтрок.Количество() = 0 Тогда
			Продолжить;
		КонецЕсли; 
		
		Для Каждого СтрокаЛиста Из Лист.ДанныеДопСтрок[0].Значение Цикл
			
			Если НЕ ЗначениеЗаполнено(СтрокаЛиста.П000300001070) Тогда
				Продолжить;
			КонецЕсли; 
			
			СтрокаДохода = Форма.СводДоходов.Добавить();
			Если СтрокаЛиста.П000300001020 = ВидДоходаОтНалоговогоАгента() ИЛИ СтрокаЛиста.П000300001020 = ВидДоходаОтОперацийСЦБ()
				ИЛИ СтрокаЛиста.П000300001100 <> 0 Тогда
				СтрокаДохода.КБК = КБКДоходовОтНалоговогоАгента(); //доход от налогового агента
			Иначе
				СтрокаДохода.КБК = КБКДоходовССамостоятельнымИсчислением(); //доход с самостоятельным исчислением и уплатой налога
			КонецЕсли; 
			СтрокаДохода.ОКТМО            = СтрокаЛиста.П000300001050;
			СтрокаДохода.Ставка           = СтрокаЛиста.П000300001010;
			
			Если СтрокаЛиста.П000300001020 = ВидДоходаВВидеДивидендов() Тогда
				// с 2015-го года доходы в виде сумм прибыли контролируемых иностранных компаний должны считаться отдельно
				СтрокаДохода.ВидДохода = Дивиденды();
			Иначе
				СтрокаДохода.ВидДохода = ОбычныйДоход();
			КонецЕсли;
			
			СтрокаДохода.СуммаДохода      = СтрокаЛиста.П000300001070;
			СтрокаДохода.НачисленоНалога  = СтрокаЛиста.П000300001090;
			СтрокаДохода.УдержаноНалога   = СтрокаЛиста.П000300001100;
			
		КонецЦикла;
		
	КонецЦикла;
	
	// Перебираем строки Листа Б:
	//
	Для Каждого Лист Из Форма.ТаблицаСтраницыЛистБ Цикл
		
		Если Лист.Данные.Количество() = 0 ИЛИ Лист.ДанныеДопСтрок.Количество() = 0 Тогда
			Продолжить;
		КонецЕсли; 
		
		СтавкаНалога = Лист.Данные[0].Значение.П000400001001;
		
		Для Каждого СтрокаЛиста Из Лист.ДанныеДопСтрок[0].Значение Цикл
			
			Если НЕ ЗначениеЗаполнено(СтрокаЛиста.П000400001070) Тогда
				Продолжить;
			КонецЕсли;
			
			СтрокаДохода        = Форма.СводДоходов.Добавить();
			СтрокаДохода.КБК    = КБКДоходовССамостоятельнымИсчислением(); // доход с самостоятельным исчислением и уплатой налога
			СтрокаДохода.ОКТМО  = ОКТМОналогоплательщика;
			СтрокаДохода.Ставка = СтавкаНалога;
			
			Если Строка(СтрокаЛиста.П000400001031) = КодДоходаКонтролируемыхКомпаний() Тогда
				СтрокаДохода.ВидДохода = ДоходыКонтролируемыхКомпаний();
			Иначе
				СтрокаДохода.ВидДохода = ОбычныйДоход();
			КонецЕсли;
			
			СтрокаДохода.СуммаДохода     = СтрокаЛиста.П000400001070;
			СтрокаДохода.НачисленоНалога = СтрокаЛиста.П000400001120;
			
			СтрокаДохода.НалогПринимаемыйКУчету   = СтрокаЛиста.П000400001130;
			
			Если СтрокаЛиста.П000400001071 <> 0 Тогда
				
				СтрокаВычета                        = Форма.СводВычетов.Добавить();
				СтрокаВычета.ВидВычета              = "ЛистБ.071";
				СтрокаВычета.Ставка                 = СтавкаНалога;
				СтрокаВычета.СуммаВычетаДляРаздела2 = СтрокаЛиста.П000400001071;
				
				Если Строка(СтрокаЛиста.П000400001031) = КодДоходаКонтролируемыхКомпаний() Тогда
					СтрокаВычета.ВидДохода = ДоходыКонтролируемыхКомпаний();
				Иначе
					СтрокаВычета.ВидДохода = ОбычныйДоход();
				КонецЕсли;
				
			КонецЕсли;
			
			Если СтрокаЛиста.П000400001072 <> 0 Тогда
				
				СтрокаВычета                         = Форма.СводВычетов.Добавить();
				СтрокаВычета.ВидВычета               = "ЛистБ.072";
				СтрокаВычета.Ставка                  = СтавкаНалога;
				СтрокаВычета.СуммаВычетаДляРаздела2  = СтрокаЛиста.П000400001072;
				Если Строка(СтрокаЛиста.П000400001031) = КодДоходаКонтролируемыхКомпаний() Тогда
					СтрокаВычета.ВидДохода = ДоходыКонтролируемыхКомпаний();
				Иначе
					СтрокаВычета.ВидДохода = ОбычныйДоход();
