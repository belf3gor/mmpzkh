		СтрокаВычета.СуммаВычетаДляД1        = СтрокаВычета.СуммаВычетаДляРаздела2;
		СтрокаВычета.ВидДохода               = ОбычныйДоход();
		
		
		Если СтрокаЛиста.Свойство("П000500001140") Тогда
			// Итоговые показатели находятся только на последнем листе
			СтрокаДохода.УплаченоНалога   = СтрокаЛиста.П000500001140;
		КонецЕсли; 
		
	КонецЦикла;
	
	// - пункт 9 Листа Г
	Если Форма.СтруктураДанныхЛистГ.П000600001200 <> 0 Тогда
		СтрокаВычета                         = Форма.СводВычетов.Добавить();
