&НаСервере
Функция СтраницаПуста(ДанныеСтраницы, Показатели, ДеревоВыгрузки, РодМногостр = Ложь)
	
	Если ТипЗнч(Показатели) = Тип("Строка") Тогда
		ПоказателиРаздела = ПолучитьПоказателиРаздела(ДеревоВыгрузки, Показатели);
	Иначе
		ПоказателиРаздела = Показатели;
	КонецЕсли;
	
	ПоказателиНеанализируемыеНаЗаполненность = ПоказателиНеанализируемыеНаЗаполненность();
	
	Для Каждого Пок Из ПоказателиРаздела Цикл
		Если ПоказателиНеанализируемыеНаЗаполненность.Найти(Пок.Ключ) <> Неопределено Тогда
			Продолжить;
		КонецЕсли;
		Если Пок.Многострочность Или (РодМногостр И Пок.Родитель.Многострочность) Тогда
			ИндексСтроки = 1;
			ЗначениеПоказателя = Неопределено;
			Пока ДанныеСтраницы.Свойство(Пок.Ключ + "_" + Формат(ИндексСтроки, "ЧГ="), ЗначениеПоказателя) Цикл
				Если ПоказательЗаполнен(Пок, ЗначениеПоказателя) Тогда
					Возврат Ложь;
				КонецЕсли;
				ИндексСтроки = ИндексСтроки + 1;
			КонецЦикла;
		Иначе
			ЗначениеПоказателя = Неопределено;
			ДанныеСтраницы.Свойство(СтрЗаменить(Пок.Ключ,"_Ч",""), ЗначениеПоказателя);
			Если ПоказательЗаполнен(Пок, ЗначениеПоказателя) Тогда
				Возврат Ложь;
			КонецЕсли;
		КонецЕсли;
	КонецЦикла;
	Возврат Истина;
	
КонецФункции

&НаСервере
Функция РазделПуст(ДеревоВыгрузки, Раздел)
	
	ПоказателиРаздела = ПолучитьПоказателиРаздела(ДеревоВыгрузки, Раздел);
	
	ДанныеРаздела = мДанныеОтчета[Раздел];
	РазделПуст = Истина;
	Если ТипЗнч(ДанныеРаздела) <> Тип("Структура") Тогда
		Для Каждого Стр Из ДанныеРаздела Цикл
			Если НЕ СтраницаПуста(Стр.Данные[0].Значение, ПоказателиРаздела, ДеревоВыгрузки) Тогда
				Возврат Ложь;
			КонецЕсли;
		КонецЦикла;
	Иначе
		Если НЕ СтраницаПуста(ДанныеРаздела, ПоказателиРаздела, ДеревоВыгрузки) Тогда
			Возврат Ложь;
		КонецЕсли;
	КонецЕсли;
	
	Возврат Истина;
	
КонецФункции

&НаСервере
Процедура ЗаполнитьДанными(ДеревоВыгрузки, ПараметрыВыгрузки)
	
	ОбработатьУсловныеЭлементы(ПараметрыВыгрузки, ДеревоВыгрузки); // формируем точную структуру XML-макета
	ЗаполнитьДаннымиУзел(ДеревоВыгрузки, ПараметрыВыгрузки); // заполняем дерево данными
	
	ОтсечьНезаполненныеНеобязательныеУзлы(ДеревоВыгрузки);
	
КонецПроцедуры

&НаСервере
Процедура ОбработатьУсловныеЭлементы(Знач ПараметрыВыгрузки, Узел)
	
	Если ТипЗнч(Узел) <> Тип("ДеревоЗначений") Тогда
		Если Узел.Код = ПараметрыВыгрузки.ИмяКлючевогоУзлаСодержательнойЧасти Тогда
			Возврат;
		КонецЕсли;
		РегламентированнаяОтчетность.ДополнитьСтруктуруПараметров(мДанныеОтчета, ПараметрыВыгрузки, Узел);
	КонецЕсли;
	КоличествоСтрок = Узел.Строки.Количество();
	Для Инд = 1 По КоличествоСтрок Цикл
		ТекСтр = Узел.Строки.Получить(КоличествоСтрок - Инд);
		Если НЕ ПустаяСтрока(ТекСтр.Условие) Тогда
			Если НЕ РегламентированнаяОтчетность.УсловиеВыполнено(ЭтотОбъект, ПараметрыВыгрузки, ТекСтр.Условие) Тогда
				Узел.Строки.Удалить(ТекСтр);
				Продолжить;
			КонецЕсли;
		КонецЕсли;
		ОбработатьУсловныеЭлементы(ПараметрыВыгрузки, ТекСтр);
	КонецЦикла;
	
КонецПроцедуры

// Процедура рассчитывает итоги по указанному листу (если лист не указан, рассчитываются итоги на всех листах)
//  Если лист помечен активным и в табличном документе на форме есть соответствующие итогам области, их значения будут обновлены
&НаКлиентеНаСервереБезКонтекста
Процедура РассчитатьИтогиЛиста(Форма, ИмяЛиста = Неопределено)

	Если ИмяЛиста = "ЛистВ" Тогда
		
		Колонка110 = 0;  // общая сумма дохода
		Колонка120 = 0;  // общая сумма проф.вычета (факт или по норме)
		СтраницаИтогов = Неопределено;
		
		Для Каждого ЛистВ Из Форма.ТаблицаСтраницыЛистВ Цикл
			
			Если ЛистВ.Данные.Количество() = 0 Тогда
				Продолжить;
			КонецЕсли; 
			
			Колонка110 = Колонка110 + ЛистВ.Данные[0].Значение.П000500001030;
			Колонка120 = Колонка120 + ЛистВ.Данные[0].Значение.П000500001040
									+ ЛистВ.Данные[0].Значение.П000500001100;
			СтраницаИтогов = ЛистВ;
			
		КонецЦикла;
		
		Если СтраницаИтогов <> Неопределено Тогда
			
			ЛистВ.Данные[0].Значение.Вставить("П000500001110", Колонка110);
			ЛистВ.Данные[0].Значение.Вставить("П000500001120", Колонка120);
			
			Если СтраницаИтогов.АктивнаяСтраница И Форма.ТабличныйДокумент.Области.Найти("П000500001110") <> Неопределено Тогда
				Форма.ТабличныйДокумент.Области["П000500001110"].Значение = Колонка110;
				Форма.ТабличныйДокумент.Области["П000500001120"].Значение = Колонка120;
			КонецЕсли; 
			
		КонецЕсли; 
	
	ИначеЕсли ИмяЛиста = "ЛистГ" Тогда
		
		ПоказателиЛиста = Форма.СтруктураДанныхЛистГ;
		ПоказателиЛиста.П000600001200 = 
			ПоказателиЛиста.П000600001020 +
			ПоказателиЛиста.П000600001040 +
			ПоказателиЛиста.П000600001060 +
			ПоказателиЛиста.П000600001080 +
			ПоказателиЛиста.П000600001100 +
			ПоказателиЛиста.П000600001120 +
			ПоказателиЛиста.П000600001140 +
			ПоказателиЛиста.П000600001160 +
			ПоказателиЛиста.П000600001190;
			
		Если Форма.ТабличныйДокумент.Области.Найти("П000600001200") <> Неопределено Тогда
			Форма.ТабличныйДокумент.Области["П000600001200"].Значение = ПоказателиЛиста.П000600001200;
		КонецЕсли; 
		
	ИначеЕсли ИмяЛиста = "ЛистД1" Тогда
		
		// Формула определения суммы налоговой базы для определения имущественного вычета
		//ВсегоДоходПоСтавке13 (строка 010 Раздела 2) 
		// - пункт 10 Листа Г
		// - пункт 4 Листа Е1
		// - пункт 3 Листа Е2
		// - подпункты 11.3 Листа 3
		// - пункт 7.3 листа И
		// - подпункт 3.2 Листа В
		// - пункте 4 листа Д2
		// - подпункт 6.1 Листа Ж
		// - подпункт 2.5 Листа Д1
		// - подпункт 2.6 Листа Д1
		
		ЗаполнитьСводныеТаблицы(Форма);
		
		СуммаДохода           = СуммаДоходаДляИмущественногоВычета(Форма);
		
		СтраницаИтогов        = Неопределено;
		
		ВсегоНаПриобретение   = 0;
		ВсегоПоПроцентам      = 0;
		
		ОстатокВычетаНаПриобретение = 2000000;
		ОстатокВычетаПоПроцентам    = 3000000;
		
		// Вычет по нескольким объектам можно получить только по законодательству 2014 года.
		РассчитыватьПределПоПроцентам = Форма.ТаблицаСтраницыЛистД1.Количество() > 1;
		
		// Собираем информацию по вычетам по всем листам
		Для Каждого ЛистД1 Из Форма.ТаблицаСтраницыЛистД1 Цикл
			
			Если ЛистД1.Данные.Количество() = 0 Тогда
				Продолжить;
			КонецЕсли; 
			
			СтраницаИтогов  = ЛистД1;
			ПоказателиЛиста = ЛистД1.Данные[0].Значение;
			
			Если ПоказателиЛиста.П000700001120 > ОстатокВычетаНаПриобретение Тогда
				ПоказателиЛиста.П000700001120 = ОстатокВычетаНаПриобретение;
				Если ЛистД1.АктивнаяСтраница И Форма.ТабличныйДокумент.Области.Найти("П000700001120") <> Неопределено Тогда
					Форма.ТабличныйДокумент.Области["П000700001120"].Значение = ПоказателиЛиста.П000700001120;
				КонецЕсли; 
			КонецЕсли; 
			
			ВсегоНаПриобретение = ВсегоНаПриобретение + ПоказателиЛиста.П000700001120;
			ОстатокВычетаНаПриобретение = Макс(0, ОстатокВычетаНаПриобретение - ПоказателиЛиста.П000700001120);
			
			// Предел на вычет по процентам действует только для объектов недвижимости, приобретенных с 01.01.2014 
			Если НЕ РассчитыватьПределПоПроцентам Тогда
				РассчитыватьПределПоПроцентам = НЕ ЗначениеЗаполнено(ПоказателиЛиста.П000700001110) ИЛИ ПоказателиЛиста.П000700001110 >= 2014;
			КонецЕсли; 
			
			Если РассчитыватьПределПоПроцентам Тогда
				
				Если ПоказателиЛиста.П000700001130 > ОстатокВычетаПоПроцентам Тогда
					ПоказателиЛиста.П000700001130 = ОстатокВычетаПоПроцентам;
					Если ЛистД1.АктивнаяСтраница И Форма.ТабличныйДокумент.Области.Найти("П000700001130") <> Неопределено Тогда
						Форма.ТабличныйДокумент.Области["П000700001130"].Значение = ПоказателиЛиста.П000700001130;
					КонецЕсли; 
				КонецЕсли; 
				
				ВсегоПоПроцентам         = ВсегоПоПроцентам + ПоказателиЛиста.П000700001130;
				ОстатокВычетаПоПроцентам = ОстатокВычетаПоПроцентам - ПоказателиЛиста.П000700001130;
				
			КонецЕсли; 
			
		КонецЦикла; 
		
		ВсегоНаПриобретение = Мин(ВсегоНаПриобретение, 2000000);
		
		// Заполняем итоги
		Если СтраницаИтогов <> Неопределено Тогда
			
			ПоказателиЛиста = СтраницаИтогов.Данные[0].Значение;
			
			// Сумма дохода - налоговая база, вычисленная по формуле
			ПоказателиЛиста.Вставить("П000700001200", СуммаДохода);
			
			// Рассчитываем вычет на приобретение
			
			Если НЕ ПоказателиЛиста.Свойство("П000700001140") Тогда
				ПоказателиЛиста.Вставить("П000700001140", 0);
			КонецЕсли;
			
			Если НЕ ПоказателиЛиста.Свойство("П000700001180") Тогда
				ПоказателиЛиста.Вставить("П000700001180", 0);
			КонецЕсли;
			
			// Определение суммы вычета, доступного в текущем периоде
			ОстатокВычетаНаПриобретение = Макс(ВсегоНаПриобретение - ПоказателиЛиста.П000700001140 - ПоказателиЛиста.П000700001180, 0);
			
			// Расчет принимаемой к вычету суммы:
			ПринимаемыйВычетНаПриобретение = Мин(СуммаДохода, ОстатокВычетаНаПриобретение);
			ПоказательП000700001210 = 0;
			Если ПоказателиЛиста.Свойство("П000700001210", ПоказательП000700001210) Тогда
				ПринимаемыйВычетНаПриобретение = Мин(ПринимаемыйВычетНаПриобретение, ПоказательП000700001210);
			КонецЕсли;
			ПоказателиЛиста.Вставить("П000700001210", ПринимаемыйВычетНаПриобретение);
			
			ПоказателиЛиста.Вставить("П000700001230", Макс(ОстатокВычетаНаПриобретение - ПринимаемыйВычетНаПриобретение, 0)); // остаток на будущие периоды
			
			// уменьшаем сумму дохода на сумму принятого вычета
			СуммаДохода = СуммаДохода - ПринимаемыйВычетНаПриобретение;
			
			// Рассчитываем вычет по процентам
			
			Если НЕ ПоказателиЛиста.Свойство("П000700001150") Тогда
				ПоказателиЛиста.Вставить("П000700001150", 0);
			КонецЕсли;
			
			Если НЕ ПоказателиЛиста.Свойство("П000700001190") Тогда
				ПоказателиЛиста.Вставить("П000700001190", 0);
			КонецЕсли;
			
			// Определение суммы вычета, доступного в текущем периоде
			Если РассчитыватьПределПоПроцентам Тогда
				ОстатокВычетаПоПроцентам = Макс(ВсегоПоПроцентам - ПоказателиЛиста.П000700001150 - ПоказателиЛиста.П000700001190, 0);
			Иначе
				ОстатокВычетаПоПроцентам = Мин(ПоказателиЛиста.П000700001130, 3000000);
			КонецЕсли;
			
			// Расчет принимаемой к вычету суммы:
			ПринимаемыйВычетПоПроцентам = Мин(СуммаДохода, ОстатокВычетаПоПроцентам);
			ПоказательП000700001220 = 0;
			Если ПоказателиЛиста.Свойство("П000700001220", ПоказательП000700001220) Тогда
				ПринимаемыйВычетПоПроцентам = Мин(ПринимаемыйВычетПоПроцентам, ПоказательП000700001220);
			КонецЕсли;
			
			ПоказателиЛиста.Вставить("П000700001220", ПринимаемыйВычетПоПроцентам);
			
			ПоказателиЛиста.Вставить("П000700001240", Макс(ОстатокВычетаПоПроцентам - ПринимаемыйВычетПоПроцентам, 0)); // остаток на будущие периоды
			
			Если СтраницаИтогов.АктивнаяСтраница И Форма.ТабличныйДокумент.Области.Найти("П000700001200") <> Неопределено Тогда
				
				Форма.ТабличныйДокумент.Области["П000700001200"].Значение = ПоказателиЛиста.П000700001200; // Сумма дохода
				
				Форма.ТабличныйДокумент.Области["П000700001210"].Значение = ПоказателиЛиста.П000700001210; // Принимаемый вычет
				
				Форма.ТабличныйДокумент.Области["П000700001220"].Значение = ПоказателиЛиста.П000700001220; // Принимаемый вычет
			
				Форма.ТабличныйДокумент.Области["П000700001230"].Значение = ПоказателиЛиста.П000700001230; // Остаток на будущее
				Форма.ТабличныйДокумент.Области["П000700001240"].Значение = ПоказателиЛиста.П000700001240; // Остаток на будущее
				
			КонецЕсли; 
			
		КонецЕсли; 
			
	ИначеЕсли ИмяЛиста = "ЛистД2" Тогда
		
		// Расчет итогов Листа Д2
		ПоказателиЛиста = Форма.СтруктураДанныхЛистД2;
		
		ПоказателиЛиста.П000800001210 = ПоказателиЛиста.П000800001020
			+ ПоказателиЛиста.П000800001040
			+ ПоказателиЛиста.П000800001060
			+ ПоказателиЛиста.П000800001080
			+ ПоказателиЛиста.П000800001100
			+ ПоказателиЛиста.П000800001120
			+ ПоказателиЛиста.П000800001140
			+ ПоказателиЛиста.П000800001160
			+ ПоказателиЛиста.П000800001180
			+ ПоказателиЛиста.П000800001200;
			
		Если Форма.ТабличныйДокумент.Области.Найти("П000800001210") <> Неопределено Тогда
			Форма.ТабличныйДокумент.Области["П000800001210"].Значение = ПоказателиЛиста.П000800001210;
		КонецЕсли;
		
	ИначеЕсли ИмяЛиста = "ЛистЕ1" Тогда
		
		// Расчет итогов Листа Е1
		ПоказателиЛиста = Форма.СтруктураДанныхЛистЕ1;
		
		// Пункт 1.7
		ПоказателиЛиста.П000900001070 = 
			  ПоказателиЛиста.П000900001010
			+ ПоказателиЛиста.П000900001020
			+ ПоказателиЛиста.П000900001030 
			+ ПоказателиЛиста.П000900001040
			+ ПоказателиЛиста.П000900001050
			+ ПоказателиЛиста.П000900001060;
		
		// Пункт 2.4
		ПоказателиЛиста.П000900001110 =
			  ПоказателиЛиста.П000900001080
			+ ПоказателиЛиста.П000900001090
			+ ПоказателиЛиста.П000900001100; 
		
		// Пункт 3.6
		ПоказателиЛиста.П000900001170 = Мин(120000,
			  ПоказателиЛиста.П000900001120
			+ ПоказателиЛиста.П000900001130
			+ ПоказателиЛиста.П000900001140
			+ ПоказателиЛиста.П000900001150
			+ ПоказателиЛиста.П000900001160);
		
		// Пункт 3.7
		ПоказателиЛиста.П000900001180 = Макс(ПоказателиЛиста.П000900001110 + ПоказателиЛиста.П000900001170 - ПоказателиЛиста.П000900001171, 0);
		
		// Пункт 4
		ПоказателиЛиста.П000900001190 = ПоказателиЛиста.П000900001070 + ПоказателиЛиста.П000900001180;
		
		Если Форма.ТабличныйДокумент.Области.Найти("П000900001080") <> Неопределено Тогда
			
			Форма.ТабличныйДокумент.Области["П000900001070"].Значение = ПоказателиЛиста.П000900001070;
			Форма.ТабличныйДокумент.Области["П000900001110"].Значение = ПоказателиЛиста.П000900001110;
			Форма.ТабличныйДокумент.Области["П000900001170"].Значение = ПоказателиЛиста.П000900001170;
			Форма.ТабличныйДокумент.Области["П000900001180"].Значение = ПоказателиЛиста.П000900001180;
			Форма.ТабличныйДокумент.Области["П000900001190"].Значение = ПоказателиЛиста.П000900001190;
			
		КонецЕсли; 
		
	ИначеЕсли ИмяЛиста = "ЛистЕ2" Тогда
		
		// Расчет итогов Листа Е2
		ВсегоУплачено      = 0;  // сумма, принимаемая к вычету
		ВсегоПринимается   = 0;  // сумма, принимаемая к вычету
		СтраницаИтогов     = Неопределено;
		
		Для Каждого Лист Из Форма.ТаблицаСтраницыЛистЕ2 Цикл
			
			Если Лист.Данные.Количество() = 0 Тогда
				Продолжить;
			КонецЕсли; 
			
			СтраницаИтогов  = Лист;
			ПоказателиЛиста = Лист.Данные[0].Значение;
			
			ВсегоУплачено    = ВсегоУплачено + ПоказателиЛиста.П001000001060 + ПоказателиЛиста.П001000001080;
			ВсегоПринимается = ВсегоПринимается + ПоказателиЛиста.П001000001070 + ПоказателиЛиста.П001000001090;
			ПоказателиЛиста.Вставить("П001000001100", 0);
			
		КонецЦикла;
		
		Если СтраницаИтогов <> Неопределено Тогда
			
			ПоказателиЛиста = СтраницаИтогов.Данные[0].Значение;
			ПоказателиЛиста.Вставить("П001000001100", ВсегоПринимается);
			
			// Обновим связанный показатель на странице Е1
			Форма.СтруктураДанныхЛистЕ1.П000900001150 = ПоказателиЛиста.П001000001100;
			
			Если СтраницаИтогов.АктивнаяСтраница И Форма.ТабличныйДокумент.Области.Найти("П001000001100") <> Неопределено Тогда
				
				Форма.ТабличныйДокумент.Области["П001000001100"].Значение = ПоказателиЛиста.П001000001100;
				
			КонецЕсли;
			
		КонецЕсли; 
		
	ИначеЕсли ИмяЛиста = "ЛистЖ" Тогда
		
		Для Каждого СтраницаЛистЖ Из Форма.ТаблицаСтраницыЛистЖ Цикл
			
			ПоказателиЛиста = СтраницаЛистЖ.Данные[0].Значение;
			
			ПоказателиЛиста.П001100001020 = Мин(ПоказателиЛиста.П001100001010, ПоказателиЛиста.П001100001020);
			
			ПоказателиЛиста.П001100001040 = Мин(ПоказателиЛиста.П001100001030, ПоказателиЛиста.П001100001040);
			
			ПоказателиЛиста.П001100001060 = Мин(ПоказателиЛиста.П001100001050, ПоказателиЛиста.П001100001060);
			
			ПоказателиЛиста.П001100001080 = Мин(ПоказателиЛиста.П001100001070, ПоказателиЛиста.П001100001080);
			
			ПоказателиЛиста.П001100001100 = Мин(ПоказателиЛиста.П001100001090, ПоказателиЛиста.П001100001100);
			
			ПоказателиЛиста.П001100001120 = Мин(ПоказателиЛиста.П001100001110, ПоказателиЛиста.П001100001120);
			
			ПоказателиЛиста.П001100001140 = Мин(ПоказателиЛиста.П001100001130, ПоказателиЛиста.П001100001140);
			
			ПоказателиЛиста.П001100001150 = ПоказателиЛиста.П001100001020
								+ ПоказателиЛиста.П001100001040
								+ ПоказателиЛиста.П001100001060
								+ ПоказателиЛиста.П001100001080
								+ ПоказателиЛиста.П001100001100
								+ ПоказателиЛиста.П001100001120
								+ ПоказателиЛиста.П001100001140;
			
			Если СтраницаЛистЖ.АктивнаяСтраница И Форма.ТабличныйДокумент.Области.Найти("П001100001020") <> Неопределено Тогда
				
				Форма.ТабличныйДокумент.Области["П001100001020"].Значение = ПоказателиЛиста.П001100001020;
				Форма.ТабличныйДокумент.Области["П001100001040"].Значение = ПоказателиЛиста.П001100001040;
				Форма.ТабличныйДокумент.Области["П001100001060"].Значение = ПоказателиЛиста.П001100001060;
				Форма.ТабличныйДокумент.Области["П001100001080"].Значение = ПоказателиЛиста.П001100001080;
				Форма.ТабличныйДокумент.Области["П001100001100"].Значение = ПоказателиЛиста.П001100001100;
				Форма.ТабличныйДокумент.Области["П001100001120"].Значение = ПоказателиЛиста.П001100001120;
				Форма.ТабличныйДокумент.Области["П001100001140"].Значение = ПоказателиЛиста.П001100001140;
				Форма.ТабличныйДокумент.Области["П001100001150"].Значение = ПоказателиЛиста.П001100001150;
				
			КонецЕсли;
		
		КонецЦикла;
	
