			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000800001180", СуммаВычета);
		КонецЕсли;
		
	ИначеЕсли ИмяРаздела = "ЛистЕ1" Тогда
		
		Если ИмяОбласти13 = "П000900001010" // 3000 х кол-во месяцев
				И Форма.ТабличныйДокумент.Области["П000900001010"].Значение > 36000 Тогда 
				
			Форма.ТабличныйДокумент.Области["П000900001010"].Значение = 36000;
			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000900001010", Форма.ТабличныйДокумент.Области["П000900001010"].Значение);
			
		ИначеЕсли ИмяОбласти13 = "П000900001020" // 500 х кол-во месяцев
				И Форма.ТабличныйДокумент.Области["П000900001020"].Значение > 6000 Тогда 
				
			Форма.ТабличныйДокумент.Области["П000900001020"].Значение = 6000;
			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000900001020", Форма.ТабличныйДокумент.Области["П000900001020"].Значение);
			
		КонецЕсли;
		
	ИначеЕсли ИмяРаздела = "РасчетДоходаОтПродажиНедвижимости" Тогда
		
		НомерСтрокиСтрокой  = Сред(ИмяОбласти, 15);
		
		// При изменении суммы дохода исходя из цены договора или исходя из кадастровой стоимости
		Если ИмяОбласти13 = "П001900001030" ИЛИ ИмяОбласти13 = "П001900001040" Тогда
			
			П001900001050 = Макс(
				Форма.ТабличныйДокумент.Области["П001900001030_" + НомерСтрокиСтрокой].Значение,
				Форма.ТабличныйДокумент.Области["П001900001040_" + НомерСтрокиСтрокой].Значение);
			П001900001050 = Макс(П001900001050, 0);
			
			Форма.ТабличныйДокумент.Области["П001900001050_" + НомерСтрокиСтрокой].Значение = П001900001050;
			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П001900001050_" + НомерСтрокиСтрокой, П001900001050);
			
		КонецЕсли;
		
	ИначеЕсли ИмяРаздела = "Раздел2" Тогда
	
		// Пересчет выполняется при выводе листа или при изменении ставки
		
		ЗаполнитьСводныеТаблицы(Форма);
		
		ТаблицаСтраницыРаздел2 = Форма.ТаблицаСтраницыРаздел2;
		Для Каждого ЛистСтраницы Из ТаблицаСтраницыРаздел2 Цикл
			Если ЗначениеЗаполнено(ЛистСтраницы.Данные[0].Значение.П000200001001) Тогда
				РасчитатьИтогиСтраницыРаздела2(ЛистСтраницы.Данные[0].Значение);
			КонецЕсли;
			Если ЛистСтраницы.АктивнаяСтраница Тогда
				АктивнаяСтраница = ЛистСтраницы;
			КонецЕсли;
		КонецЦикла;
		
		Ставка = Форма.ТабличныйДокумент.Области.П000200001001.Значение;
		ВидДохода = Форма.ТабличныйДокумент.Области.П000200001002.Значение;
		ОчиститьЗначенияСтраницы(Форма, АктивнаяСтраница.Данные[0].Значение, "Раздел2", Ложь);
		ЗаполнитьРасчетныеДанныеРаздела2(Форма, АктивнаяСтраница, ?(ЗначениеЗаполнено(Ставка), Число(Ставка), 0), ВидДохода, Истина);
		
	КонецЕсли;
	
	// Пересчет итогов листа
	РассчитатьИтогиЛиста(Форма, ИмяРаздела);
	
	// Обновляем сводные таблицы
	ЗаполнитьСводныеТаблицы(Форма);
	
КонецПроцедуры

&НаКлиенте
Процедура СобратьДанныеВРаздел2()
	
	//Пересчитываем итоги всех листов
	РассчитатьИтогиЛиста(ЭтотОбъект, Неопределено);
	
	// Заполняем сводные таблицы итогов
	ЗаполнитьСводныеТаблицы(ЭтотОбъект);
	
	// Сначала в разделе 2 рассчитываются результаты в разрезе ставок
	ПодготовитьДанныеРаздела2(ЭтотОбъект);
	
	// Обновляем дерево разделов
	СформироватьДеревоРазделовОтчета(ЭтотОбъект);
	
	// Выводим макет в табличный документ
	ВывестиМакетВТаблДокНаСервере("Раздел2", 1, , , Истина);
	
	// Активизируем строку раздела в дереве
	Для Каждого СтрокаРаздела Из РазделыОтчета.ПолучитьЭлементы() Цикл
		Если СтрокаРаздела.КолонкаРазделыОтчетаСокрНаим = "Раздел2" Тогда
			Элементы.РазделыОтчета.ТекущаяСтрока = СтрокаРаздела.ПолучитьИдентификатор();
			Прервать;
		КонецЕсли;
	КонецЦикла;
	
	Модифицированность = Истина;
	
КонецПроцедуры

// Заполняет страницу расчетными данными по указанной ставке
&НаКлиентеНаСервереБезКонтекста
Процедура ЗаполнитьРасчетныеДанныеРаздела2(Форма, СтраницаРаздела, Ставка, ВидДохода, ОбновлятьТабличныйДокумент = Ложь)

	ДанныеСтраницы = СтраницаРаздела.Данные[0].Значение;
	
	ОКТМОНАлогоплательщика = Форма.СтруктураРеквизитовФормы.Октмо;
	
	// Устанавливаем ставку налога и вид дохода
	ДанныеСтраницы.П000200001001 = Число(Ставка);
	ДанныеСтраницы.П000200001002 = ВидДохода;
	
	Если ЗначениеЗаполнено(Ставка) Тогда
		
		Если ЗначениеЗаполнено(ВидДохода) Тогда
			
			// Находим строки доходов по указанной ставке
			СтрокиДоходов = Форма.СводДоходов.НайтиСтроки(Новый Структура("Ставка, ВидДохода", Число(Ставка), Число(ВидДохода)));
			
			Если СтрокиДоходов.Количество() > 0 Тогда
				
				// Заполняем суммами полученного дохода и уплаченных (удержанных) налогов
				Для Каждого СтрокаДохода Из СтрокиДоходов Цикл
					
					Если Не ОбновлятьТабличныйДокумент Тогда
						// поля, достуные для редактирования обновляем только при атоматическом заполнении (без обновления табличного документа)
					КонецЕсли;
					ДанныеСтраницы.П000200001080 = ДанныеСтраницы.П000200001080 + СтрокаДохода.УдержаноНалога;
					ДанныеСтраницы.П000200001100 = ДанныеСтраницы.П000200001100 + СтрокаДохода.УплаченоНалога;
					ДанныеСтраницы.П000200001120 = ДанныеСтраницы.П000200001120 + СтрокаДохода.НалогПринимаемыйКУчету;
					
					Если СтрокаДохода.ВидДохода = ДоходыКонтролируемыхКомпаний() Тогда
						ДанныеСтраницы.П000200001051 = ДанныеСтраницы.П000200001051 + СтрокаДохода.СуммаДохода;
					Иначе
						ДанныеСтраницы.П000200001010 = ДанныеСтраницы.П000200001010 + СтрокаДохода.СуммаДохода;
					КонецЕсли;
					
				КонецЦикла;
				
				
				// Заполняем суммами вычетов
				СтрокиВычетов = Форма.СводВычетов.НайтиСтроки(Новый Структура("Ставка, ВидДохода", Число(Ставка), Число(ВидДохода)));
				Для Каждого СтрокаВычета Из СтрокиВычетов Цикл
					
					Если СтрокаВычета.ВидВычета = "ЛистГ.200" 
						ИЛИ СтрокаВычета.ВидВычета = "ЛистГ.220"
						ИЛИ СтрокаВычета.ВидВычета = "ЛистБ.071"
						ИЛИ СтрокаВычета.ВидВычета = "ЛистБ.072"
						Тогда
						
						ДанныеСтраницы.П000200001020 = ДанныеСтраницы.П000200001020 + СтрокаВычета.СуммаВычетаДляРаздела2;
						
					ИначеЕсли СтрокаВычета.ВидВычета = "ЛистВ.120"
						ИЛИ СтрокаВычета.ВидВычета = "ЛистД1.180"
						ИЛИ СтрокаВычета.ВидВычета = "ЛистД1.190"
						ИЛИ СтрокаВычета.ВидВычета = "ЛистД1.210"
						ИЛИ СтрокаВычета.ВидВычета = "ЛистД1.220"
						ИЛИ СтрокаВычета.ВидВычета = "ЛистД2.210"
						ИЛИ СтрокаВычета.ВидВычета = "ЛистЕ1.190"
						ИЛИ СтрокаВычета.ВидВычета = "ЛистЕ1.171"
						ИЛИ СтрокаВычета.ВидВычета = "ЛистЕ2.110"
						ИЛИ СтрокаВычета.ВидВычета = "ЛистЖ.150"
						Тогда
						
						ДанныеСтраницы.П000200001040 = ДанныеСтраницы.П000200001040 + СтрокаВычета.СуммаВычетаДляРаздела2;
						
					ИначеЕсли СтрокаВычета.ВидВычета = "ЛистЗ.533" ИЛИ СтрокаВычета.ВидВычета = "ЛистИ.370" Тогда
						
						ДанныеСтраницы.П000200001050 = ДанныеСтраницы.П000200001050 + СтрокаВычета.СуммаВычетаДляРаздела2;
						
					КонецЕсли;
					
				КонецЦикла;
			КонецЕсли;
			
		КонецЕсли;
		
		// Итоги и результаты рассчитываем независимо от наличия доходов и вычетов, т.к. в любом случае могли быть налоговые платежи по патентам и т.п. 
		РасчитатьИтогиСтраницыРаздела2(ДанныеСтраницы);
	
	КонецЕсли;
	
	// Если страница активна, заполняем ячейки табличного документа
	ТабличныйДокумент = Форма.ТабличныйДокумент;
	Если ОбновлятьТабличныйДокумент И ТабличныйДокумент.Области.Найти("П000200001001") <> Неопределено Тогда
		Для Каждого КлючИЗначение Из ДанныеСтраницы Цикл
			ТабличныйДокумент.Области[КлючИЗначение.Ключ].Значение = КлючИЗначение.Значение;
		КонецЦикла;
	КонецЕсли;

КонецПроцедуры

&НаКлиентеНаСервереБезКонтекста
Процедура РасчитатьИтогиСтраницыРаздела2(ДанныеСтраницы)
	
	// 3. Общая сумма дохода, подлежащая налогообложению
	ДанныеСтраницы.П000200001030 = Макс(0, ДанныеСтраницы.П000200001010 - ДанныеСтраницы.П000200001020);
	
	// Проверяем: Общая сумма налоговых вычетов (строка 040) и сумма расходов, принимаемая в уменьшение полученных доходов (строка 050), 
	//	в совокупности не может превышать значение показателя строки 030
	СуммаРазницы = ДанныеСтраницы.П000200001040 + ДанныеСтраницы.П000200001050 - ДанныеСтраницы.П000200001030;
	Если СуммаРазницы > 0 Тогда
		
		// Сначала уменьшаем сумму вычета по листам З и И - убытки могут быть перенесены на следующие годы
		ДанныеСтраницы.П000200001050 = Макс(0, ДанныеСтраницы.П000200001050 - СуммаРазницы);
		СуммаРазницы = ДанныеСтраницы.П000200001040 + ДанныеСтраницы.П000200001050 - ДанныеСтраницы.П000200001030;
		
		Если СуммаРазницы > 0 Тогда
			// Если вычетов все равно больше, уменьшаем сумму вычетов по строке 040
			ДанныеСтраницы.П000200001040 = Макс(0, ДанныеСтраницы.П000200001040 - СуммаРазницы);
		КонецЕсли;
		
	КонецЕсли;
	
	// 6. Налоговая база для исчисления налога
	ДанныеСтраницы.П000200001060 = Макс(0, ДанныеСтраницы.П000200001030 + ДанныеСтраницы.П000200001051 
	- ДанныеСтраницы.П000200001040 - ДанныеСтраницы.П000200001050);
	
	// 7. Общая сумма налога, исчисленная к уплате
	ДанныеСтраницы.П000200001070 = Окр(ДанныеСтраницы.П000200001060 * ДанныеСтраницы.П000200001001 / 100, 0); // Начислено налога
	
	// 13. Сумма налога, исчисленная к уплате (доплате) в бюджет
	ДанныеСтраницы.П000200001121 = Макс(0,
	ДанныеСтраницы.П000200001070
	- ДанныеСтраницы.П000200001080
	- ДанныеСтраницы.П000200001090
	- ДанныеСтраницы.П000200001091
	- ДанныеСтраницы.П000200001100
	- ДанныеСтраницы.П000200001110
	- ДанныеСтраницы.П000200001120);
	
	// 13.3. Сумма налога, подлежащая уплате (доплате) в бюджет
	ДанныеСтраницы.П000200001130 = Макс(0, ДанныеСтраницы.П000200001121 - ДанныеСтраницы.П000200001122);
	
	// 14. Сумма налога, подлежащая возврату из бюджета
	
	МаксимумСтроки140 = ДанныеСтраницы.П000200001080 + ДанныеСтраницы.П000200001090 + ДанныеСтраницы.П000200001100;
	
	РасчетнаяСуммаСтроки140 = ДанныеСтраницы.П000200001080
	+ ДанныеСтраницы.П000200001090
	+ ДанныеСтраницы.П000200001091
	+ ДанныеСтраницы.П000200001100
	+ ДанныеСтраницы.П000200001110
	- ДанныеСтраницы.П000200001070;
	
	ДанныеСтраницы.П000200001140 = Макс(0, Мин(МаксимумСтроки140, РасчетнаяСуммаСтроки140));

КонецПроцедуры

