		СтрокаВычета.СуммаВычетаДляД1        = СтрокаВычета.СуммаВычетаДляРаздела2;
		СтрокаВычета.ВидДохода               = ОбычныйДоход();
	КонецЕсли;
	
	Для Каждого Лист Из Форма.ТаблицаСтраницыЛистЖ Цикл
		
		Если Лист.Данные.Количество() = 0 Тогда
			Продолжить;
		КонецЕсли; 
		
		СтрокаЛиста = Лист.Данные[0].Значение;
		
		// - подпункт 6.1 Листа Ж
		СтрокаВычета                         = Форма.СводВычетов.Добавить();
		СтрокаВычета.ВидВычета               = "ЛистЖ.150";
		СтрокаВычета.Ставка                  = 13;
		СтрокаВычета.СуммаВычетаДляРаздела2  = СтрокаЛиста.П001100001150;
		СтрокаВычета.СуммаВычетаДляД1        = СтрокаВычета.СуммаВычетаДляРаздела2;
		СтрокаВычета.ВидДохода               = ОбычныйДоход();
		
	КонецЦикла;
	
	Для Каждого Лист Из Форма.ТаблицаСтраницыЛистЗПродолжение4 Цикл
		
		Если Лист.Данные.Количество() = 0 Тогда
			Продолжить;
		КонецЕсли; 
			
		СтрокаЛиста = Лист.Данные[0].Значение;
			
		// - подпункты 11.3 Листа 3
		Если СтрокаЛиста.П001600001533 <> 0 Тогда
			
			СтрокаВычета                         = Форма.СводВычетов.Добавить();
			СтрокаВычета.ВидВычета               = "ЛистЗ.533";
			СтрокаВычета.Ставка                  = 13;
			СтрокаВычета.СуммаВычетаДляРаздела2  =СтрокаЛиста.П001600001533;
			СтрокаВычета.СуммаВычетаДляД1        = СтрокаВычета.СуммаВычетаДляРаздела2;
			СтрокаВычета.ВидДохода               = ОбычныйДоход();
			
		КонецЕсли;
		
	КонецЦикла;
	
	// - пункт 7.3 листа И
	Если Форма.СтруктураДанныхЛистИПродолжение.П001800001370 <> 0 Тогда
		СтрокаВычета                         = Форма.СводВычетов.Добавить();
		СтрокаВычета.ВидВычета               = "ЛистИ.370";
		СтрокаВычета.Ставка                  = 13;
		СтрокаВычета.СуммаВычетаДляРаздела2  = Форма.СтруктураДанныхЛистИПродолжение.П001800001370;
		СтрокаВычета.СуммаВычетаДляД1        = СтрокаВычета.СуммаВычетаДляРаздела2;
		СтрокаВычета.ВидДохода               = ОбычныйДоход();
	КонецЕсли;
	
КонецПроцедуры

&НаКлиентеНаСервереБезКонтекста
Функция СуммаДоходаДляИмущественногоВычета(Форма)

	Результат = 0;
	
	// Определяем сумму обычного дохода по ставке 13%
	СтрокиДохода = Форма.СводДоходов.НайтиСтроки(Новый Структура("Ставка, ВидДохода", 13, ОбычныйДоход()));
	Для Каждого СтрокаДохода Из СтрокиДохода Цикл
		Результат = Результат + СтрокаДохода.СуммаДохода;
	КонецЦикла;
	
	// Вычитаем суммы вычетов, принимаемых до имущественного вычета 
	Результат = Результат - Форма.СводВычетов.Итог("СуммаВычетаДляД1");

	Возврат Макс(Результат, 0);
	
КонецФункции

&НаКлиентеНаСервереБезКонтекста
Функция СуммаОблагаемогоДоходаПоСтавке(Форма, Ставка)

	// Вычисляет и возвращает сумму, соответствующую п. 3 раздела 2
	Результат = 0;
	
	СтрокиДохода = Форма.СводДоходов.НайтиСтроки(Новый Структура("Ставка", Ставка));
	Для Каждого СтрокаДохода Из СтрокиДохода Цикл
		Результат = Результат + СтрокаДохода.СуммаДохода;
	КонецЦикла; 
	
	Если Ставка = 13 Тогда
		
		Для Каждого СтрокаВычета Из Форма.СводВычетов Цикл
			Если Лев(СтрокаВычета.ВидВычета, 5) = "ЛистГ" Тогда
				Результат = Результат - СтрокаВычета.СуммаВычетаДляРаздела2;
			КонецЕсли; 
		КонецЦикла; 
		
	КонецЕсли; 
	
	Возврат Макс(Результат, 0);

КонецФункции

&НаКлиенте
Процедура РасчетНаКлиенте(ИмяСтраницыПанели = "", ОбластьИмя = "") Экспорт
	
	Расчет(ЭтотОбъект, ИмяСтраницыПанели, ОбластьИмя);
	
КонецПроцедуры

&НаСервере
Процедура РасчетНаСервере(ИмяСтраницыПанели = "", ОбластьИмя = "") Экспорт 
	
	Расчет(ЭтотОбъект, ИмяСтраницыПанели, ОбластьИмя);
	
КонецПроцедуры

// Выполняет провреку значений и пересчет зависимых показателей на заданной странице отчета
// Параметры:
//  Форма - управляемая форма - форма этого отчета
//	ИмяОбласти - Строка - имя раздела или листа декларации
//
&НаКлиентеНаСервереБезКонтекста
Процедура Расчет(Форма, ИмяРаздела = "", ИмяОбласти = "")
	
	Перем ТаблицаБлока, ТаблицаРаздела;
	
	Если (Форма.СтруктураРеквизитовФормы.мРежимПечати) Тогда
		Возврат; // служебный вызов в процессе печати
	КонецЕсли;
	
	Если (Форма.СтруктураРеквизитовФормы.ФлажокОтклАвтоРасчет) Тогда
		Возврат; // отключен режим авторасчета
	КонецЕсли;
	
	ИмяОбласти13 = Лев(ИмяОбласти, 13); // имя области без номера строки
	
	// Если пересчитывается текущий раздел, нужно обновить значения в табличном документе
	ОбновитьТабличныйДокумент = Форма.СтруктураРеквизитовФормы.НаимТекущегоРаздела = ИмяРаздела;
	
	// Проверки значений и пересчет связанных НЕ итоговых реквизитов
	Если ИмяРаздела = "ЛистА" Тогда
		
		НомерСтрокиСтрокой  = Сред(ИмяОбласти, 15);
		
		// При изменении ставки или суммы облагаемого дохода пересчитываем сумму налога
		Если ИмяОбласти13 = "П000300001010" ИЛИ ИмяОбласти13 = "П000300001080" Тогда
			
			П000300001090 = Окр(Форма.ТабличныйДокумент.Области["П000300001080_" + НомерСтрокиСтрокой].Значение
						* Форма.ТабличныйДокумент.Области["П000300001010_" + НомерСтрокиСтрокой].Значение / 100, 0); 
						
			Форма.ТабличныйДокумент.Области["П000300001090_" + НомерСтрокиСтрокой].Значение = П000300001090;
			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000300001090_" + НомерСтрокиСтрокой, П000300001090);
			
		КонецЕсли; 
		
	ИначеЕсли ИмяРаздела = "ЛистБ" Тогда
		
		НомерСтрокиСтрокой  = Сред(ИмяОбласти, 15);
		
		// Проверка необходимости пересчитать сумму дохода из валюты в рубли
		ПересчитыватьДоходВруб = ИмяОбласти13 = "П000400001030" // Код валюты
			Или ИмяОбласти13 = "П000400001040" // дата получения дохода
			Или ИмяОбласти13 = "П000400001050" // курс валюты на дату получения дохода
			Или ИмяОбласти13 = "П000400001060"; // сумма дохода в валюте
			
		Если ПересчитыватьДоходВруб Тогда
			
			Курс         = Форма.ТабличныйДокумент.Области["П000400001050_" + НомерСтрокиСтрокой].Значение;
			СуммаВВалюте = Форма.ТабличныйДокумент.Области["П000400001060_" + НомерСтрокиСтрокой].Значение;
			
			Если ЗначениеЗаполнено(Курс) И ЗначениеЗаполнено(СуммаВВалюте) Тогда
				ДоходВРублях = Окр(СуммаВВалюте * Курс, 2);
				Форма.ТабличныйДокумент.Области["П000400001070_" + НомерСтрокиСтрокой].Значение = ДоходВРублях;
				СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000400001070_" + НомерСтрокиСтрокой, ДоходВРублях);
			КонецЕсли; 
			
		КонецЕсли; 
		
		// Проверка необходимости пересчитать сумму налога из валюты в рубли
		ПересчитыватьНалогВРуб = ИмяОбласти13 = "П000400001030" // Код валюты
			Или ИмяОбласти13 = "П000400001080" // дата уплаты налога
			Или ИмяОбласти13 = "П000400001090" // курс валюты на дату уплаты налога
			Или ИмяОбласти13 = "П000400001100"; // сумма налога в валюте
			
		Если ПересчитыватьНалогВРуб Тогда
			
			Курс         = Форма.ТабличныйДокумент.Области["П000400001090_" + НомерСтрокиСтрокой].Значение;
			СуммаВВалюте = Форма.ТабличныйДокумент.Области["П000400001100_" + НомерСтрокиСтрокой].Значение;
			
			Если ЗначениеЗаполнено(Курс) И ЗначениеЗаполнено(СуммаВВалюте) Тогда
				НалогВРублях = Окр(СуммаВВалюте * Курс, 2); 
				Форма.ТабличныйДокумент.Области["П000400001110_" + НомерСтрокиСтрокой].Значение = НалогВРублях;
				СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000400001110_" + НомерСтрокиСтрокой, НалогВРублях);
			КонецЕсли; 
			
		КонецЕсли; 
		
		// Расчет суммы налога, начисленного в РФ.
		СтавкаНалога = Форма.ТабличныйДокумент.Области["П000400001001"].Значение;
		
		Если ИмяОбласти13 = "П000400001001" Тогда // Ставка налога на листе
			
			// Пересчитываем все суммы налогов на текущем листе
			АктивныеСтраницы = Форма.ТаблицаСтраницыЛистБ.НайтиСтроки(Новый Структура("АктивнаяСтраница", Истина));
			АктивнаяСтраница = ?(АктивныеСтраницы.Количество() = 0, Неопределено, АктивныеСтраницы[0]);
			
			Если АктивнаяСтраница <> Неопределено Тогда
				
				Для Каждого ЗначениеДобСтрок Из АктивнаяСтраница.ДанныеДопСтрок Цикл
					
					СчетчикСтрок = 0;
					Для Каждого ЭлементСтроки Из ЗначениеДобСтрок.Значение Цикл
						
						СчетчикСтрок = СчетчикСтрок + 1;
						НалогВРФ = Окр(ЭлементСтроки.П000400001070 * СтавкаНалога / 100, 2);
						Форма.ТабличныйДокумент.Области["П000400001120_" + СчетчикСтрок].Значение = НалогВРФ;
						СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000400001120_" + СчетчикСтрок, НалогВРФ);
						
						// Сумма принимаемого налога не может превышать сумму уплаченного
						Если ЭлементСтроки.П000400001130 > 0 Тогда
							ПринимаемаяСумма = Мин(ЭлементСтроки.П000400001120, ЭлементСтроки.П000400001110);
							Форма.ТабличныйДокумент.Области["П000400001130_" + СчетчикСтрок].Значение = ПринимаемаяСумма;
							СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000400001130_" + СчетчикСтрок, ПринимаемаяСумма);
						КонецЕсли; 
						
					КонецЦикла; 
					
				КонецЦикла; 
				
			КонецЕсли;
			
		ИначеЕсли ПересчитыватьДоходВруб ИЛИ ИмяОбласти13 = "П000400001070" Тогда
			
			НалогВРФ = Окр(Форма.ТабличныйДокумент.Области["П000400001070_" + НомерСтрокиСтрокой].Значение * СтавкаНалога / 100, 2);
			Форма.ТабличныйДокумент.Области["П000400001120_" + НомерСтрокиСтрокой].Значение = НалогВРФ;
			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000400001120_" + НомерСтрокиСтрокой, НалогВРФ);
			
		КонецЕсли; 

		Если (ПересчитыватьДоходВруб ИЛИ ПересчитыватьНалогВРуб ИЛИ ИмяОбласти13 = "П000400001130" ИЛИ ИмяОбласти13 = "П000400001120" ИЛИ ИмяОбласти13 = "П000400001070")
				И НомерСтрокиСтрокой <> "" И Форма.ТабличныйДокумент.Области["П000400001130_" + НомерСтрокиСтрокой].Значение > 0 Тогда
			
			Значение = Мин(Форма.ТабличныйДокумент.Области["П000400001130_" + НомерСтрокиСтрокой].Значение, Форма.ТабличныйДокумент.Области["П000400001120_" + НомерСтрокиСтрокой].Значение);
			
			Форма.ТабличныйДокумент.Области["П000400001130_" + НомерСтрокиСтрокой].Значение = Значение;
			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000400001130_" + НомерСтрокиСтрокой, Значение);
			
		КонецЕсли;
		
	ИначеЕсли ИмяРаздела = "ЛистВ" Тогда
		
		ОбновлятьФактическиеРасходы = ИмяОбласти13 = "П000500001050" // Матер. расходы
									Или ИмяОбласти13 = "П000500001060" // амморт. начилсения
									Или ИмяОбласти13 = "П000500001070" // выплаты физлицам
									Или ИмяОбласти13 = "П000500001090"; // прочие расходы
		
		Если ИмяОбласти13 = "П000500001040" И Форма.ТабличныйДокумент.Области["П000500001040"].Значение = 0 Тогда 
			
			// очищаем фактические расходы
			Форма.ТабличныйДокумент.Области["П000500001050"].Значение = 0;
			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000500001050", 0);
			Форма.ТабличныйДокумент.Области["П000500001060"].Значение = 0;
			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000500001060", 0);
			Форма.ТабличныйДокумент.Области["П000500001070"].Значение = 0;
			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000500001060", 0);
			Форма.ТабличныйДокумент.Области["П000500001080"].Значение = 0;
			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000500001060", 0);
			Форма.ТабличныйДокумент.Области["П000500001090"].Значение = 0;
			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000500001060", 0);
			ОбновлятьФактическиеРасходы = Истина;
			
		ИначеЕсли ИмяОбласти13 = "П000500001080" Тогда // Выплаты по трудовым договорам
			
			// Проверим сумму общих выплат физическим лицам
			Если Форма.ТабличныйДокумент.Области["П000500001070"].Значение < Форма.ТабличныйДокумент.Области["П000500001080"].Значение Тогда
				
				Форма.ТабличныйДокумент.Области["П000500001070"].Значение = Форма.ТабличныйДокумент.Области["П000500001080"].Значение;
				СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000500001070", Форма.ТабличныйДокумент.Области["П000500001070"].Значение);
				ОбновлятьФактическиеРасходы = Истина;
				
			КонецЕсли; 
			
		ИначеЕсли ИмяОбласти13 = "П000500001070" Тогда // все выплаты физическим лицам
			
			// Проверим сумму выплат по трудовым договорам
			Если Форма.ТабличныйДокумент.Области["П000500001070"].Значение < Форма.ТабличныйДокумент.Области["П000500001080"].Значение Тогда
				
				Форма.ТабличныйДокумент.Области["П000500001080"].Значение = Форма.ТабличныйДокумент.Области["П000500001070"].Значение;
				СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000500001080", Форма.ТабличныйДокумент.Области["П000500001080"].Значение);
				
			КонецЕсли; 
			
		ИначеЕсли ИмяОбласти13 = "П000500001160" И Форма.ТабличныйДокумент.Области["П000500001160"].Значение > Форма.ТабличныйДокумент.Области["П000500001030"].Значение Тогда // все выплаты физическим лицам
			
			// сумма вычета фермерского хозяйства
			Форма.ТабличныйДокумент.Области["П000500001160"].Значение 
					= Мин(Форма.ТабличныйДокумент.Области["П000500001160"].Значение, Форма.ТабличныйДокумент.Области["П000500001030"].Значение);
			
		КонецЕсли; 
		
		ВсегоФактРасходы = Форма.ТабличныйДокумент.Области["П000500001050"].Значение + Форма.ТабличныйДокумент.Области["П000500001060"].Значение
					+ Форма.ТабличныйДокумент.Области["П000500001070"].Значение + Форма.ТабличныйДокумент.Области["П000500001090"].Значение;
					
		Если ОбновлятьФактическиеРасходы ИЛИ 
				ИмяОбласти13 = "П000500001040" И Форма.ТабличныйДокумент.Области["П000500001040"].Значение < ВсегоФактРасходы Тогда

			Форма.ТабличныйДокумент.Области["П000500001040"].Значение = ВсегоФактРасходы;
			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000500001040", ВсегоФактРасходы);
			
			
		КонецЕсли; 
		
		ВсегоФактРасходы = Макс(ВсегоФактРасходы, Форма.ТабличныйДокумент.Области["П000500001040"].Значение);
		
		Если ВсегоФактРасходы = 0 Тогда
			
			// Если не указаны фактические расходы, то считаем расходы по норме
			Форма.ТабличныйДокумент.Области["П000500001100"].Значение = 
					Окр(Форма.ТабличныйДокумент.Области["П000500001030"].Значение * 0.2, 2);
			СохранитьЗначениеПоказателяВСтруктуре(Форма, ИмяРаздела, "П000500001100", Форма.ТабличныйДокумент.Области["П000500001100"].Значение);
			
