		Или ПодстрокаВИмениПоказателя13 = "П030103000101"
		Или ПодстрокаВИмениПоказателя13 = "П010203000101"
		Или ПодстрокаВИмениПоказателя13 = "П020203000101"
		Или ПодстрокаВИмениПоказателя13 = "П010103000102"
		Или ПодстрокаВИмениПоказателя13 = "П020103000102"
		Или ПодстрокаВИмениПоказателя13 = "П030103000102"
		Или ПодстрокаВИмениПоказателя13 = "П010203000102"
		Или ПодстрокаВИмениПоказателя13 = "П020203000102" Тогда
		ИмяПоказателяСодержащегоКод = Лев(Область.Имя, 11) + "02" + Сред(Область.Имя, 14);
		ИсходноеЗначение = ТабличныйДокумент.Области[ИмяПоказателяСодержащегоКод].Значение;
		ИсходноеЗначение = Формат(ИсходноеЗначение, "ЧГ=");
	ИначеЕсли ПодстрокаВИмениПоказателя13 = "П010103000103"
		Или ПодстрокаВИмениПоказателя13 = "П010203000103"
		Или ПодстрокаВИмениПоказателя13 = "П010103000104"
		Или ПодстрокаВИмениПоказателя13 = "П010203000104" Тогда
		ИмяПоказателяСодержащегоКод = Лев(Область.Имя, 11) + "04" + Сред(Область.Имя, 14);
		ИсходноеЗначение = ТабличныйДокумент.Области[ИмяПоказателяСодержащегоКод].Значение;
