	ИначеЕсли ПодстрокаВИмениПоказателя11 = "П0000020001"
		Или ПодстрокаВИмениПоказателя11 = "П0000020002" Тогда
		ИмяПоказателяСодержащегоКод = "П0000020002" + Сред(Область.Имя, 12);
		ИсходноеЗначение = ТабличныйДокумент.Области[ИмяПоказателяСодержащегоКод].Значение;
		ИсходноеЗначение = Формат(ИсходноеЗначение, "ЧГ=");
