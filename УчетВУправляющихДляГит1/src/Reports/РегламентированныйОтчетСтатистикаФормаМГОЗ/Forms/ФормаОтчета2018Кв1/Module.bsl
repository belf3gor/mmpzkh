					СтрЗаполнена = Истина;
					Прервать;
				КонецЕсли;
			КонецЦикла;
			Если Не СтрЗаполнена Тогда
				Продолжить;
			КонецЕсли;
			
			СтрВывода = СтрВывода + 1;
			Если СтрВывода > 99 Тогда 
				Прервать;
			КонецЕсли;
			ВывестиЗаголовок(Раздел2Пустой, ПечатнаяФорма, Макет, "Раздел2_Заголовок", "Раздел2_Шапка");
			ОблСтрока = Макет.ПолучитьОбласть("Раздел2_Строка");
			ОблСтрока.Области.QQQ0Б_2.Значение = Стр.Код;
			ОблСтрока.Области.QQQ0В_2.Значение = Стр.Номер + Формат(СтрВывода, "ЧЦ=2; ЧВН=; ЧГ=");
			Для Итер = 9 По 16 Цикл 
				ОблСтрока.Области["QQQ" + Формат(Итер, "ЧЦ=2; ЧВН=") + "_2"].Значение = СтруктураДанных[ИмяГруппы + Формат(Итер, "ЧЦ=2; ЧВН=") + "_" + ИндСтр];
