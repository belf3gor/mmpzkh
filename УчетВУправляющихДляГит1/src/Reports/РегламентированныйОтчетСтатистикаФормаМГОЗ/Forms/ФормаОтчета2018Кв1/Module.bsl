&НаКлиенте
Процедура ПоказатьБланк(Команда)
	
	Отказ = Ложь;
	
	ДополнительныеПараметры = Новый Структура();
	ДополнительныеПараметры.Вставить("Команда", Команда);
	ОписаниеОповещения = Новый ОписаниеОповещения("ПоказатьБланкЗавершение", ЭтотОбъект, ДополнительныеПараметры);
	
	ОнлайнСервисыРегламентированнойОтчетностиКлиент.ПередПечатьюРегламентированногоОтчета(ОписаниеОповещения, ЭтотОбъект, Отказ);
	
КонецПроцедуры

&НаКлиенте
Процедура ПоказатьБланкЗавершение(Отказ, ДополнительныеПараметры) Экспорт
	
	Если Отказ Тогда
		Возврат;
	КонецЕсли;

	Команда = ДополнительныеПараметры.Команда;
		
	Печать(Команда.Имя);
	
КонецПроцедуры

&НаКлиенте
Процедура Печать(ВидПечати, НеИзФормыОтчета = Ложь) Экспорт
	
	Состояние(СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(НСтр("ru='%1. Формируется печатная форма...'"), ЭтаФорма.Заголовок), , , БиблиотекаКартинок.Печать);
	
	Если НЕ ПечатьНаСервере(ВидПечати) Тогда
		Возврат;
	КонецЕсли;
	
	РегламентированнаяОтчетностьКлиент.ОткрытьФормуПредварительногоПросмотра(ЭтаФорма, ВидПечати, Неопределено, СтруктураРеквизитовФормы.СписокПечатаемыхЛистов);
	
	СтруктураРеквизитовФормы.мРежимПечати = Ложь;

КонецПроцедуры

&НаСервере
Процедура ЗаполнитьДаннымиТабличныйДокумент(ТабДок, ИмяМакета, ИмяОбласти, СтруктураДанных)
	
	Макет = ОбъектОтчета(ЭтаФорма.ИмяФормы).ПолучитьМакет(ИмяМакета);
	Если ЗначениеЗаполнено(ИмяОбласти) Тогда 
		ОбластьМакета = Макет.ПолучитьОбласть(ИмяОбласти);
		ТабДок.Вывести(ОбластьМакета);
	Иначе
		ТабДок.Вывести(Макет);
	КонецЕсли;
	
	Для Каждого ЭлСтруктуры Из СтруктураДанных Цикл
		РегламентированнаяОтчетность.УстановитьЗначениеИФорматВПоле(ТабДок, ЭлСтруктуры.Ключ, ЭлСтруктуры.Значение);
	КонецЦикла;
	
КонецПроцедуры

&НаСервере
Процедура ВывестиЗаголовок(РазделПустой, ПечатнаяФорма, Макет, Раздел_Заголовок, Раздел_Шапка)
	Если РазделПустой Тогда
		ПечатнаяФорма.Вывести(Макет.ПолучитьОбласть(Раздел_Заголовок));
		ПечатнаяФорма.Вывести(Макет.ПолучитьОбласть(Раздел_Шапка));
		РазделПустой = Ложь;
	КонецЕсли;
КонецПроцедуры

&НаСервере
Процедура ВывестиСтрокуСПроверкой(ПечатнаяФорма, Макет, Раздел_Шапка, ОблСтрока, МассивПроверки)
	МассивПроверки.Очистить();
	МассивПроверки.Добавить(ОблСтрока);
	Если Не ПечатнаяФорма.ПроверитьВывод(МассивПроверки) Тогда 
		СтруктураРеквизитовФормы.мПечатныеФормы.Добавить(ПечатнаяФорма, "Форма М-ГОЗ");
		ПечатнаяФорма = РегламентированнаяОтчетность.НоваяСтраница();
		Если ЗначениеЗаполнено(Раздел_Шапка) Тогда 
			ПечатнаяФорма.Вывести(Макет.ПолучитьОбласть(Раздел_Шапка));
		КонецЕсли;
	КонецЕсли;
	ПечатнаяФорма.Вывести(ОблСтрока);
КонецПроцедуры

&НаСервере
Процедура ПечатьРаздел1()
	Макет = ОбъектОтчета(ЭтаФорма.ИмяФормы).ПолучитьМакет("ФормаОтчета2018Кв1_Раздел12_Печать");
	ПечатнаяФорма = РегламентированнаяОтчетность.НоваяСтраница();
	
	Раздел1Пустой = Истина;
	МассивПроверки = Новый Массив;
	
	Для Каждого Стр Из РегламентированнаяОтчетностьКлиентСервер.НайтиСвойстваПоказателя(ЭтаФорма, "Заказчики").ТаблицаЗначений Цикл
		СтруктураДанных = ЭтотОбъект["СтруктураДанныхРаздел12" + Стр.Код];
		
		ВывестиЗаголовок(Раздел1Пустой, ПечатнаяФорма, Макет, "Раздел1_Заголовок", "Раздел1_Шапка");
		ОблСтрока = Макет.ПолучитьОбласть("Раздел1_Строка");
		ОблСтрока.Области.QQQ0А_1.Значение = Стр.Название;
		ОблСтрока.Области.QQQ0Б_1.Значение = Стр.Код;
		ОблСтрока.Области.QQQ0В_1.Значение = Стр.Номер;
		ОблСтрока.Области.QQQ07_1.Значение = СтруктураДанных["П000" + Стр.Код + "07"];
		ОблСтрока.Области.QQQ08_1.Значение = СтруктураДанных["П000" + Стр.Код + "08"];
		ВывестиСтрокуСПроверкой(ПечатнаяФорма, Макет, "Раздел1_Шапка", ОблСтрока, МассивПроверки);
		
		Инд = 0; СтрВывода = 0;
		Пока Истина Цикл 
			Инд = Инд + 1;
			ИндСтр = Формат(Инд, "ЧГ=");
			ИмяГруппы = "П000" + Стр.Код;
			Если Не СтруктураДанных.Свойство(ИмяГруппы + "01_" + ИндСтр) Тогда 
				Прервать;
			КонецЕсли;
			
			СтрЗаполнена = Ложь;
			Для Итер = 1 По 8 Цикл 
				Если ЗначениеЗаполнено(СтруктураДанных[ИмяГруппы + "0" + Итер + "_" + ИндСтр]) Тогда 
					СтрЗаполнена = Истина;
					Прервать;
				КонецЕсли;
			КонецЦикла;
			Если Не СтрЗаполнена Тогда
				Продолжить;
			КонецЕсли;
			
			СтрВывода = СтрВывода + 1;
			Если СтрВывода > 99 Тогда 
				Прервать;
			КонецЕсли;
			ВывестиЗаголовок(Раздел1Пустой, ПечатнаяФорма, Макет, "Раздел1_Заголовок", "Раздел1_Шапка");
			ОблСтрока = Макет.ПолучитьОбласть("Раздел1_Строка");
			ОблСтрока.Области.QQQ0Б_1.Значение = Стр.Код;
			ОблСтрока.Области.QQQ0В_1.Значение = Стр.Номер + Формат(СтрВывода, "ЧЦ=2; ЧВН=; ЧГ=");
			Для Итер = 1 По 8 Цикл 
				ОблСтрока.Области["QQQ0" + Итер + "_1"].Значение = СтруктураДанных[ИмяГруппы + "0" + Итер + "_" + ИндСтр];
			КонецЦикла;
			ВывестиСтрокуСПроверкой(ПечатнаяФорма, Макет, "Раздел1_Шапка", ОблСтрока, МассивПроверки);
		КонецЦикла;
	КонецЦикла;
	ВывестиСтрокуСПроверкой(ПечатнаяФорма, Макет, "", Макет.ПолучитьОбласть("Раздел1_Подвал"), МассивПроверки);
	СтруктураРеквизитовФормы.мПечатныеФормы.Добавить(ПечатнаяФорма, "Форма М-ГОЗ");
	ПечатнаяФорма = РегламентированнаяОтчетность.НоваяСтраница();
КонецПроцедуры

