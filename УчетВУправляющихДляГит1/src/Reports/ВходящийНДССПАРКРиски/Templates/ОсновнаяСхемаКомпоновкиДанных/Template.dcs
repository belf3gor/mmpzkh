			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КонтрагентДляОбновленияИндексов</dataPath>
			<field>КонтрагентДляОбновленияИндексов</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
