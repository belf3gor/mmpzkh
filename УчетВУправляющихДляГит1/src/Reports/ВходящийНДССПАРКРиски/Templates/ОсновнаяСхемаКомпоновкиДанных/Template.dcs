		<dataSource>ОсновнойНабор</dataSource>
		<query>ВЫБРАТЬ
	НДСПокупки.Организация КАК Организация,
	НДСПокупки.Поставщик КАК Контрагент,
	"Заявлен к вычету" КАК Раздел,
	НДСПокупки.СчетФактура КАК СчетФактура,
	НДСПокупки.СуммаБезНДСОборот КАК СуммаЗакупкиБезНДС,
	НДСПокупки.НДСОборот КАК СуммаНДС
ПОМЕСТИТЬ ВТ_НДС
{ВЫБРАТЬ
	Организация.*,
	Контрагент.*,
	Раздел,
	СчетФактура.*}
ИЗ
	РегистрНакопления.НДСЗаписиКнигиПокупок.Обороты({(&amp;НачалоПериода)}, {(&amp;КонецПериода)}, , НЕ ВидЦенности В (&amp;ВидыЦенностейАвансы) {(Организация).* КАК Организация, (Поставщик).* КАК Контрагент}) КАК НДСПокупки

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	НДСПредъявленный.Организация,
	НДСПредъявленный.Поставщик,
	"Предъявлен поставщиком",
	НДСПредъявленный.СчетФактура,
	НДСПредъявленный.СуммаБезНДСОборот,
	НДСПредъявленный.НДСОборот
ИЗ
	РегистрНакопления.НДСПредъявленный.Обороты({(&amp;НачалоПериода)}, {(&amp;КонецПериода)}, , НЕ ВидЦенности В (&amp;ВидыЦенностейАвансы) {(Организация).* КАК Организация, (Поставщик).* КАК Контрагент}) КАК НДСПредъявленный

ИНДЕКСИРОВАТЬ ПО
	Контрагент
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
	Рег.Контрагент КАК Контрагент,
	Рег.ИНН КАК ИНН,
	Рег.ИННКорректный КАК ИННКорректный,
	Рег.СвояОрганизация КАК СвояОрганизация
ПОМЕСТИТЬ ВТ_Контрагенты
ИЗ
	РегистрСведений.СвойстваКонтрагентовСПАРКРиски КАК Рег
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_НДС КАК ВТ_НДС
		ПО Рег.Контрагент = ВТ_НДС.Контрагент
ГДЕ
	Рег.ИННКорректный

ИНДЕКСИРОВАТЬ ПО
	Контрагент
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	МАКСИМУМ(СправкиСПАРКРиски.Дата) КАК Дата,
	СправкиСПАРКРиски.Контрагент КАК Контрагент
ПОМЕСТИТЬ ВТ_АктуальныеДатыСправок
ИЗ
	Документ.СправкиСПАРКРиски КАК СправкиСПАРКРиски
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_НДС КАК ВТ_НДС
		ПО СправкиСПАРКРиски.Контрагент = ВТ_НДС.Контрагент
ГДЕ
	НЕ СправкиСПАРКРиски.ПометкаУдаления

СГРУППИРОВАТЬ ПО
	СправкиСПАРКРиски.Контрагент

ИНДЕКСИРОВАТЬ ПО
	Контрагент
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СправкиСПАРКРиски.Дата КАК Дата,
	МАКСИМУМ(СправкиСПАРКРиски.Ссылка) КАК Ссылка,
	СправкиСПАРКРиски.Контрагент КАК Контрагент
ПОМЕСТИТЬ ВТ_Справки
ИЗ
	Документ.СправкиСПАРКРиски КАК СправкиСПАРКРиски
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_АктуальныеДатыСправок КАК ВТ_АктуальныеДатыСправок
		ПО СправкиСПАРКРиски.Дата = ВТ_АктуальныеДатыСправок.Дата
			И СправкиСПАРКРиски.Контрагент = ВТ_АктуальныеДатыСправок.Контрагент
ГДЕ
	НЕ СправкиСПАРКРиски.ПометкаУдаления

СГРУППИРОВАТЬ ПО
	СправкиСПАРКРиски.Дата,
	СправкиСПАРКРиски.Контрагент
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_НДС.Организация КАК Организация,
	ВТ_НДС.Контрагент КАК Контрагент,
	ВТ_НДС.Раздел КАК Раздел,
	ВТ_НДС.СуммаЗакупкиБезНДС КАК СуммаЗакупкиБезНДС,
	ВТ_НДС.СуммаНДС КАК СуммаНДС,
	ВТ_Контрагенты.ИНН КАК ИНН,
	ВТ_Контрагенты.ИННКорректный КАК ЭтоКорректныйИНН,
	ВТ_Контрагенты.СвояОрганизация КАК СвояОрганизация,
	ЕСТЬNULL(РегИндексы.ИНН, "") КАК ИННИндексы,
	ЕСТЬNULL(РегИндексы.ДатаОбновления, &amp;ПустаяДата) КАК ДатаОбновления,
	ВЫБОР
		КОГДА РегИндексы.ИндексДолжнойОсмотрительности ЕСТЬ NULL
			ТОГДА -1
		ИНАЧЕ РегИндексы.ИндексДолжнойОсмотрительности
	КОНЕЦ КАК ИндексДолжнойОсмотрительностиЧисло,
	ВЫБОР
		КОГДА РегИндексы.ИндексДолжнойОсмотрительности &gt;= 0
				И РегИндексы.ИндексДолжнойОсмотрительности &lt;= 40
			ТОГДА ЗНАЧЕНИЕ(Перечисление.ГрадацияИндексовСПАРКРиски.Низкий)
		КОГДА РегИндексы.ИндексДолжнойОсмотрительности &gt;= 41
				И РегИндексы.ИндексДолжнойОсмотрительности &lt;= 71
			ТОГДА ЗНАЧЕНИЕ(Перечисление.ГрадацияИндексовСПАРКРиски.Средний)
		КОГДА РегИндексы.ИндексДолжнойОсмотрительности &gt;= 72
				И РегИндексы.ИндексДолжнойОсмотрительности &lt;= 100
			ТОГДА ЗНАЧЕНИЕ(Перечисление.ГрадацияИндексовСПАРКРиски.Высокий)
		ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.ГрадацияИндексовСПАРКРиски.ПустаяСсылка)
	КОНЕЦ КАК ИндексДолжнойОсмотрительностиГрадация,
	ВТ_Справки.Дата КАК ДатаСправки,
	ВТ_Справки.Ссылка КАК Справка,
	ВТ_НДС.СчетФактура КАК СчетФактура
{ВЫБРАТЬ
	Организация.*,
	Контрагент.*,
	Раздел,
	ДатаСправки,
	Справка,
	СвояОрганизация,
	СчетФактура.*}
ИЗ
	ВТ_НДС КАК ВТ_НДС
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Контрагенты КАК ВТ_Контрагенты
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ИндексыСПАРКРиски КАК РегИндексы
			ПО ВТ_Контрагенты.Контрагент = РегИндексы.Контрагент
		ПО ВТ_НДС.Контрагент = ВТ_Контрагенты.Контрагент
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Справки КАК ВТ_Справки
		ПО ВТ_НДС.Контрагент = ВТ_Справки.Контрагент
{ГДЕ
	ВТ_Контрагенты.Контрагент.* КАК КонтрагентДляОбновленияИндексов}</query>
	</dataSet>
	<calculatedField>
		<dataPath>Заголовок</dataPath>
		<expression>""</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Заголовок</v8:content>
			</v8:item>
		</title>
	</calculatedField>
	<calculatedField>
		<dataPath>СуммаПокупок</dataPath>
		<expression>СуммаНДС+СуммаЗакупкиБезНДС</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Сумма покупок</v8:content>
			</v8:item>
		</title>
	</calculatedField>
	<totalField>
		<dataPath>СуммаНДС</dataPath>
		<expression>Сумма(СуммаНДС)</expression>
	</totalField>
	<totalField>
		<dataPath>СуммаЗакупкиБезНДС</dataPath>
		<expression>Сумма(СуммаЗакупкиБезНДС)</expression>
	</totalField>
	<totalField>
		<dataPath>ИндексДолжнойОсмотрительностиГрадация</dataPath>
		<expression>Максимум(ИндексДолжнойОсмотрительностиГрадация)</expression>
	</totalField>
	<totalField>
		<dataPath>ИндексДолжнойОсмотрительностиЧисло</dataPath>
		<expression>Максимум(ИндексДолжнойОсмотрительностиЧисло)</expression>
	</totalField>
	<totalField>
		<dataPath>ДатаСправки</dataPath>
		<expression>Максимум(ДатаСправки)</expression>
	</totalField>
	<totalField>
		<dataPath>Справка</dataPath>
		<expression>Максимум(Справка)</expression>
	</totalField>
	<totalField>
		<dataPath>СуммаПокупок</dataPath>
		<expression>Сумма(СуммаПокупок)</expression>
	</totalField>
	<parameter>
		<name>СрокГодности</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Срок годности</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:dateTime</v8:Type>
			<v8:DateQualifiers>
				<v8:DateFractions>DateTime</v8:DateFractions>
			</v8:DateQualifiers>
		</valueType>
		<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
		<useRestriction>true</useRestriction>
	</parameter>
	<parameter>
		<name>СервисНеПодключен</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Сервис не подключен</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:boolean</v8:Type>
		</valueType>
		<value xsi:type="xs:boolean">false</value>
		<useRestriction>true</useRestriction>
	</parameter>
	<parameter>
		<name>НачалоПериода</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Начало периода</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:dateTime</v8:Type>
			<v8:DateQualifiers>
				<v8:DateFractions>DateTime</v8:DateFractions>
			</v8:DateQualifiers>
		</valueType>
		<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
		<useRestriction>false</useRestriction>
	</parameter>
	<parameter>
		<name>КонецПериода</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Конец периода</v8:content>
			</v8:item>
		</title>
