 //Выполняет расчет вычисляемых показателей отчета
// (ячеек, выделенных зеленым цветом).
//
&НаКлиентеНаСервереБезКонтекста
Процедура Расчет(Форма, ИмяСтраницыПанели = "", ОбластьИмя = "")
	
	Если (Форма.СтруктураРеквизитовФормы.ФлажокОтклАвтоРасчет = Истина) Тогда
		Возврат; // отключен режим авторасчета
	КонецЕсли;
	
	Области = Форма.ПолеТабличногоДокументаФормаОтчета.Области;
	
	Для Стр = 2 По 13 Цикл
		Если Стр = 5 Тогда 
			Продолжить;
		КонецЕсли;
		
		Если Стр = 9 Тогда 
			СтрСтр = Формат(Стр, "ЧЦ=2; ЧВН=; ЧГ=");
			Области["П001000903"].Значение = Области["П001000904"].Значение
											+Области["П001000905"].Значение;
		Иначе
			СтрСтр = Формат(Стр, "ЧЦ=2; ЧВН=; ЧГ=");
			Области["П00100" + СтрСтр + "03"].Значение = Области["П00100" + СтрСтр + "04"].Значение
											+Области["П00100" + СтрСтр + "05"].Значение
											+Области["П00100" + СтрСтр + "06"].Значение;
		КонецЕсли;
	КонецЦикла;
	
	Области.П003002803.Значение = Области.П003002903.Значение
								+Области.П003003003.Значение
								+Области.П003003103.Значение;
	
	Для Графа = 1 По 7 Цикл
		Значение = Области["П00100030" + (Графа+2)].Значение
					+Области["П00100040" + (Графа+2)].Значение
					+Области["П00100060" + (Графа+2)].Значение
					+Области["П00100100" + (Графа+2)].Значение
					+Области["П00100120" + (Графа+2)].Значение
					+Области["П00100130" + (Графа+2)].Значение;
		
		Если Графа <> 6 Тогда 
			Значение = Значение + Области["П00100020" + (Графа+2)].Значение;
		КонецЕсли;
		
		Области["П00100010" + (Графа+2)].Значение = Значение;
	КонецЦикла;
	
	Строка = 0;
	Пока Истина Цикл
		Строка = Строка + 1;
		Если Области.Найти("П004000003210_" + Строка) = Неопределено Тогда 
			Прервать;
		КонецЕсли;
		
		Области["П004000003210_" + Строка].Значение =
					+Области["П004000003211_" + Строка].Значение
					+Области["П004000003212_" + Строка].Значение
					+Области["П004000003213_" + Строка].Значение;
		
		Области["П004000003205_" + Строка].Значение =
					Области["П004000003206_" + Строка].Значение
					+Области["П004000003208_" + Строка].Значение
					+Области["П004000003209_" + Строка].Значение
					+Области["П004000003210_" + Строка].Значение
					+Области["П004000003214_" + Строка].Значение
					+Области["П004000003215_" + Строка].Значение
					+Области["П004000003217_" + Строка].Значение;
	КонецЦикла;
	
	Для Колонка = 1 По 17 Цикл 
		Значение = 0;
		КолонкаСтр = Формат(Колонка+3, "ЧЦ=2; ЧВН=; ЧГ=");
		Строка = 0;
		Пока Истина Цикл 
			Строка = Строка + 1;
			Если Области.Найти("П0040000032" + КолонкаСтр + "_" + Строка) = Неопределено Тогда 
				Прервать;
			КонецЕсли;
			
			Значение = Значение + Области["П0040000032" + КолонкаСтр + "_" + Строка].Значение;
		КонецЦикла;
		
		Области["П0040032" + КолонкаСтр].Значение = Значение;
	КонецЦикла;
	
КонецПроцедуры 

