&НаКлиентеНаСервереБезКонтекста
Процедура ПронумероватьСтрокиДляПечати(Форма)
	Форма.Модифицированность = Истина;
	Для Каждого КЗ Из Форма.мСтруктураМногострочныхРазделов Цикл 
		ОднаСтрока = (Неопределено = Форма.ПолеТабличногоДокументаФормаОтчета.Области.Найти("УдалитьСтроку" + КЗ.Ключ + "01_2"));
		Стр = 0;
		Пока Истина Цикл 
			Стр = Стр + 1;
			СтрСтр = Формат(Стр, "ЧГ=");
			ОблНомер = Форма.ПолеТабличногоДокументаФормаОтчета.Области.Найти(КЗ.Ключ + "01_" + СтрСтр);
			Если ОблНомер = Неопределено Тогда 
				Прервать;
			КонецЕсли;
			ОблНомер.Значение = СтрСтр;
			
			ОблУС = Форма.ПолеТабличногоДокументаФормаОтчета.Области.Найти("УдалитьСтроку" + КЗ.Ключ + "01_" + СтрСтр);
			Если ОблУС <> Неопределено Тогда 
				Если ОднаСтрока Тогда 
					ОблУС.Текст = "";
					ОблУС.Гиперссылка = Ложь;
				Иначе
					ОблУС.Текст = "х";
					ОблУС.Гиперссылка = Истина;
				КонецЕсли;
			КонецЕсли;
		КонецЦикла;
	КонецЦикла;
КонецПроцедуры

&НаКлиенте
Процедура ДобавитьСтроку(Команда)
	#Если ВебКлиент Тогда
		ДобавитьСтрокуНаСервере("ФормаОтчета");
	#Иначе
		ДобавитьСтрокуНаКлиенте("ФормаОтчета");
	#КонецЕсли
	
	ПронумероватьСтрокиДляПечати(ЭтотОбъект);
КонецПроцедуры

