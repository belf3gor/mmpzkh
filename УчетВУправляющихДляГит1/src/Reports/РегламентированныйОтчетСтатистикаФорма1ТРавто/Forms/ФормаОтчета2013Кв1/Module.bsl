&НаСервере
Процедура РасчетНаСервере(ИмяСтраницыПанели = "", ОбластьИмя = "") Экспорт

	Расчет(ЭтаФорма, ИмяСтраницыПанели, ОбластьИмя);

КонецПроцедуры

&НаКлиентеНаСервереБезКонтекста
Процедура Расчет(Форма, ИмяТекущейСтраницыПанели = "", ОбластьИмя = "")
	
	// Раздел 1.
	// Стр100 равна сумме строк с 101 по 107.
	Для НомКол = 3 По 5 Цикл
		ТекСумма = 0;
		Постфикс = "0" + Формат(НомКол, "ЧЦ=1; ЧВН=; ЧГ=");
		Для НомСтр = 1 По 7 Цикл
			ТекСумма = ТекСумма + Форма.ПолеТабличногоДокументаФормаОтчета.Области["П000001010" + Формат(НомСтр, "ЧЦ=1; ЧВН=; ЧГ=") + Постфикс].Значение;
		КонецЦикла;
		Форма.ПолеТабличногоДокументаФормаОтчета.Области["П00000101000" + Формат(НомКол, "ЧЦ=1; ЧВН=; ЧГ=")].Значение = ТекСумма;
	КонецЦикла;
	
	// Стр130 равна сумме строк с 131 по 133.
	Для НомКол = 3 По 5 Цикл
		ТекСумма = 0;
		Постфикс = "0" + Формат(НомКол, "ЧЦ=1; ЧВН=; ЧГ=");
		Для НомСтр = 1 По 3 Цикл
			ТекСумма = ТекСумма + Форма.ПолеТабличногоДокументаФормаОтчета.Области["П000001013" + Формат(НомСтр, "ЧЦ=1; ЧВН=; ЧГ=") + Постфикс].Значение;
		КонецЦикла;
		Форма.ПолеТабличногоДокументаФормаОтчета.Области["П00000101300" + Формат(НомКол, "ЧЦ=1; ЧВН=; ЧГ=")].Значение = ТекСумма;
	КонецЦикла;
	
	// Стр150 равна сумме строк с 151 по 153.
	Для НомКол = 3 По 4 Цикл
		ТекСумма = 0;
		Постфикс = "0" + Формат(НомКол, "ЧЦ=1; ЧВН=; ЧГ=");
		Для НомСтр = 1 По 3 Цикл
			ТекСумма = ТекСумма + Форма.ПолеТабличногоДокументаФормаОтчета.Области["П000001015" + Формат(НомСтр, "ЧЦ=1; ЧВН=; ЧГ=") + Постфикс].Значение;
		КонецЦикла;
		Форма.ПолеТабличногоДокументаФормаОтчета.Области["П00000101500" + Формат(НомКол, "ЧЦ=1; ЧВН=; ЧГ=")].Значение = ТекСумма;
	КонецЦикла;
	
	// Раздел 2.2
	// Стр250 равна сумме строк с 251 по 263.
	Для НомКол = 3 По 4 Цикл
		ТекСумма = 0;
		Постфикс = "0" + Формат(НомКол, "ЧЦ=1; ЧВН=; ЧГ=");
		Для НомСтр = 51 По 63 Цикл
			ТекСумма = ТекСумма + Форма.ПолеТабличногоДокументаФормаОтчета.Области["П00020202" + Формат(НомСтр, "ЧЦ=2; ЧВН=; ЧГ=") + Постфикс].Значение;
		КонецЦикла;
		Форма.ПолеТабличногоДокументаФормаОтчета.Области["П00020202500" + Формат(НомКол, "ЧЦ=1; ЧВН=; ЧГ=")].Значение = ТекСумма;
	КонецЦикла;

	
КонецПроцедуры

