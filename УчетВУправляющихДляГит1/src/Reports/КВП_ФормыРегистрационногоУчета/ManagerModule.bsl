	СписокФормОтчета.Добавить("Ф10",    "Ф10 - Поквартирная карточка");
	СписокФормОтчета.Добавить("Ф11",    "Ф11 - Домовая (поквартирная) книга");
	СписокФормОтчета.Добавить("Ф12_П",
										"Ф12П - Листок статистического учета прибытия (к документам о регистрации по новому месту жительства)");
	СписокФормОтчета.Добавить("Ф12_В",
										"Ф12В - Листок статистического учета выбытия (к документам о снятии с регистрационного учета)");
	СписокФормОтчета.Добавить("Ф14",    "Ф14 - Уведомление в орган регистрационного учета");
	СписокФормОтчета.Добавить("Ф15",	"Ф15 - Уведомление собственнику (нанимателю) жилого помещения");
	СписокФормОтчета.Добавить("Ф16",    "Ф16 - Справка о составе семьи");
	СписокФормОтчета.Добавить("Ф17",    "Ф17 - Выписка из домовой книги");
	СписокФормОтчета.Добавить("Ф7_СПБ", "Ф7_СПБ - Характеристика жилого помещения (СПБ)");
	СписокФормОтчета.Добавить("Ф9_СПБ", "Ф9_СПБ - Справка о регистрации (СПБ)");

	СписокФормОтчета.Добавить("С_С",    "С_С - Справка в суд");
	СписокФормОтчета.Добавить("С_Р",    "С_Р - Справка о регистрации по месту жительства");
	СписокФормОтчета.Добавить("С_ЗП",   "С_ЗП - Справка об отсутствии паспорта в связи с заменой");
	СписокФормОтчета.Добавить("С_ПР",   "С_ПР - Справка об отсутствии паспорта (на регистрации)");
	СписокФормОтчета.Добавить("З_С",    "З_С - Заявление собственников для регистрации");
	СписокФормОтчета.Добавить("РФЛ1",   "РФЛ1 - Сведения о регистрации гражданина РФ по месту жительства");
