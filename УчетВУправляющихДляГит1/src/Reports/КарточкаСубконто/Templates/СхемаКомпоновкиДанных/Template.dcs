		<parameter xmlns:dcsat="http://v8.1c.ru/8.1/data-composition-system/area-template" xsi:type="dcsat:ExpressionAreaTemplateParameter">
			<dcsat:name>ПериодГруппировки</dcsat:name>
			<dcsat:expression>НСтр("ru = 'Обороты за '") + ВЫБОР   
КОГДА &amp;Периодичность = 6 
   ТОГДА Формат(ПериодГруппировки, "ДФ=dd.MM.yy")  
   КОГДА &amp;Периодичность = 7
   ТОГДА Формат(НачалоПериода(ПериодГруппировки, "Неделя"), "ДФ=dd.MM.yy")  + " - " + Формат(КонецПериода(ПериодГруппировки, "Неделя"), "ДФ=dd.MM.yy") 
   КОГДА &amp;Периодичность = 8
   ТОГДА Формат(НачалоПериода(ПериодГруппировки, "Декада"), "ДФ=dd.MM.yy")  + " - " + Формат(КонецПериода(ПериодГруппировки, "Декада"), "ДФ=dd.MM.yy") 
   КОГДА &amp;Периодичность = 9
   ТОГДА Формат(ПериодГруппировки, "ДФ = 'MMMM yy'")
   КОГДА &amp;Периодичность = 10
   ТОГДА 
      ВЫБОР
         КОГДА Квартал(ПериодГруппировки) = 1 
         ТОГДА Формат(ПериодГруппировки, "ДФ = '""1 кв."" yy'")
         КОГДА Квартал(ПериодГруппировки) = 2 
         ТОГДА Формат(ПериодГруппировки, "ДФ = '""2 кв."" yy'")
         КОГДА Квартал(ПериодГруппировки) = 3 
         ТОГДА Формат(ПериодГруппировки, "ДФ = '""3 кв."" yy'")
         КОГДА Квартал(ПериодГруппировки) = 4 
         ТОГДА Формат(ПериодГруппировки, "ДФ = '""4 кв."" yy'")
      КОНЕЦ
   КОГДА &amp;Периодичность = 11
   ТОГДА 
      ВЫБОР
         КОГДА Квартал(ПериодГруппировки) &lt; 2 
         ТОГДА Формат(ПериодГруппировки, "ДФ = '""1 полугодие"" yy'")
         ИНАЧЕ Формат(ПериодГруппировки, "ДФ = '""2 полугодие"" yy'")
      КОНЕЦ
   КОГДА &amp;Периодичность = 12
   ТОГДА Формат(ПериодГруппировки, "ДФ = 'yyyy'")
КОНЕЦ</dcsat:expression>
		</parameter>
