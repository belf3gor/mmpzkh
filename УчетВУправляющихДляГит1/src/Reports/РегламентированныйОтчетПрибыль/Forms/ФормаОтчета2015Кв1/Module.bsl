&НаСервере
Процедура ПроверитьКС_1(СтруктураРезультата)
	
	Если НЕ КодНайден(мДанныеОтчета.Титульный.ПоМесту, "214,215,216,225,226,228,229,231,234,235,236,237") Тогда
		Возврат;
	КонецЕсли;
	
	ОбщееЧислоОшибок    = СтруктураРезультата.КоличествоНайденныхОшибок;
	ОбщееЧислоБезОшибок = СтруктураРезультата.КоличествоНайденныхБезОшибок;
	СтруктураОшибок     = СтруктураРезультата.СтруктураОшибок;
	
	ТекстФормулы    = НСтр("ru='Проверка Подраздела 1.1 Раздела 1 (КС 1)'");
	ОписаниеКС      = НСтр("ru='Сумма налога к доплате в федеральный бюджет (строка 040) должна быть равна сумме налога на прибыль к доплате по строке 270 Листа 02 за минусом суммы налога к уменьшению по строке 280 Листа 02'");
	
	СчетчикФормулДляРасшифровок = СчетчикФормулДляРасшифровок + 1;
	ЗарегистрироватьФормулуДляРасшифровки(ОписаниеКС);
	
	Нарушение       = НСтр("ru='п.п.1-3 ст.286 НК РФ'");
	ОписаниеОшибки1 = НСтр("ru='Завышена сумма налога на прибыль к доплате'");
	ОписаниеОшибки2 = НСтр("ru='Занижена сумма налога на прибыль к доплате'");
	
	Р1_1Ст40Гр03 = ПоказательПоСтроке("Р1_1Ст40Гр03");
	Л02Ст270Гр03 = ПоказательПоСтроке("Л02Ст270Гр03");
	Л02Ст280Гр03 = ПоказательПоСтроке("Л02Ст280Гр03");
	Л02Ст1Гр00 = ПоказательПоСтроке("Л02Ст1Гр00");
	Раздел1_1 = мДанныеОтчета.Раздел1_1;
	Лист02 = мДанныеОтчета.Лист02;
	
	СуммаСтр040 = 0;
	СуммаСтр040Стр = "";
	Для Каждого СтраницаР1_1 Из Раздел1_1 Цикл
		ДанныеСтраницыР1_1 = СтраницаР1_1.Данные[0].Значение;
		НомерСтраницыР1_1Стр = СтрокаЧГ0(Раздел1_1.Индекс(СтраницаР1_1) + 1);
		
		ВставитьЗначениеПоказателя(ДанныеСтраницыР1_1, Р1_1Ст40Гр03);
		ВнестиПоказательВРасшифровку(Р1_1Ст40Гр03, "Раздел1_1", НомерСтраницыР1_1Стр);
		СуммаСтр040 = СуммаСтр040 + Р1_1Ст40Гр03.Значение;
		СуммаСтр040Стр = СуммаСтр040Стр + Р1_1Ст40Гр03.ЗначениеСтр + " + ";
		
	КонецЦикла;
	
	СуммаЛ02 = 0;
	СуммаЛ02Стр = "";
	Для Каждого СтраницаЛ02 Из Лист02 Цикл
		ДанныеСтраницыЛ02 = СтраницаЛ02.Данные[0].Значение;
		НомерСтраницыЛ02Стр = СтрокаЧГ0(Лист02.Индекс(СтраницаЛ02) + 1);
		
		ВставитьЗначениеПоказателя(ДанныеСтраницыЛ02, Л02Ст1Гр00);
		Если НЕ КодНайден(Л02Ст1Гр00.Значение, "1,3,4") Тогда
			Продолжить;
		КонецЕсли;
		
		ВставитьЗначениеПоказателя(ДанныеСтраницыЛ02, Л02Ст270Гр03);
		ВставитьЗначениеПоказателя(ДанныеСтраницыЛ02, Л02Ст280Гр03);
		
		ВнестиПоказательВРасшифровку(Л02Ст270Гр03, "Лист02", НомерСтраницыЛ02Стр);
		ВнестиПоказательВРасшифровку(Л02Ст280Гр03, "Лист02", НомерСтраницыЛ02Стр);
		СуммаЛ02 = СуммаЛ02 + Л02Ст270Гр03.Значение - Л02Ст280Гр03.Значение;
		СуммаЛ02Стр = СуммаЛ02Стр + Л02Ст270Гр03.ЗначениеСтр + " - " + Л02Ст280Гр03.ЗначениеСтр + " + ";
		
	КонецЦикла;
	
	КС = ПоказателиКС(СуммаСтр040, СуммаЛ02);
	Расшифровка = РасшифровкаВСтр(СуммаСтр040Стр) + " Итог [" + КС.ЛеваяЧастьСтр + "] = " + РасшифровкаВСтр(СуммаЛ02Стр)
		+ " Итог [" + КС.ПраваяЧастьСтр + "]";
	
	УсловиеВыполнено = (КС.ПраваяЧасть >= 0);
	Если НЕ УсловиеВыполнено ИЛИ (УсловиеВыполнено И КС.ЛеваяЧасть = КС.ПраваяЧасть) Тогда
		ЭтоОшибка = Ложь;
		ОбщееЧислоБезОшибок = ОбщееЧислоБезОшибок + 1;
		
	Иначе
		ЭтоОшибка = Истина;
		ОбщееЧислоОшибок = ОбщееЧислоОшибок + 1;
		
	КонецЕсли;
	
	Если ЭтоОшибка = Ложь Тогда
		ОписаниеОшибки = "";
		
	ИначеЕсли КС.ЛеваяЧасть > КС.ПраваяЧасть Тогда
		ОписаниеОшибки = ОписаниеОшибки1;
		
	ИначеЕсли КС.ЛеваяЧасть < КС.ПраваяЧасть Тогда
		ОписаниеОшибки = ОписаниеОшибки2;
	
	КонецЕсли;
	
	ДобавитьЭлементВСтруктуруОшибок(СтруктураОшибок, ТекстФормулы, ОписаниеКС, ОбщееЧислоОшибок, ОбщееЧислоБезОшибок,
		Расшифровка, ЭтоОшибка, ОписаниеОшибки, Нарушение);
	
	СтруктураРезультата.Вставить("КоличествоНайденныхОшибок", ОбщееЧислоОшибок);
	СтруктураРезультата.Вставить("КоличествоНайденныхБезОшибок", ОбщееЧислоБезОшибок);
	СтруктураРезультата.Вставить("СтруктураОшибок", СтруктураОшибок);
	
