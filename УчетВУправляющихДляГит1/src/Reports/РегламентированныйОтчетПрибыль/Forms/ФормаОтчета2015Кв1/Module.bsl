&НаКлиентеНаСервереБезКонтекста
Процедура ЗаполнитьПериодОтчета(Форма)
	
	Если Форма.ТабличныйДокумент.Области.Период.Значение <> "50" Тогда
		
		ПоМесту = РегламентированнаяОтчетностьКлиентСервер.ВернутьЗначениеПараметраСЛиста(Форма, "Титульный", "ПоМесту");
		
		Если ЗначениеЗаполнено(ПоМесту) Тогда
			
			ЭтоКонсолидированнаяГруппа = (ПоМесту = "218");
			
			Если ЭтоКонсолидированнаяГруппа Тогда
				КодОтчетногоПериода
					= Форма.СтруктураРеквизитовФормы.СоответствиеНомераМесяцаКодуОтчетногоПериода_КГН[
					Месяц(Форма.СтруктураРеквизитовФормы.мДатаКонцаПериодаОтчета)];
			Иначе
				КодОтчетногоПериода
					= Форма.СтруктураРеквизитовФормы.СоответствиеНомераМесяцаКодуОтчетногоПериода_НеКГН[
					Месяц(Форма.СтруктураРеквизитовФормы.мДатаКонцаПериодаОтчета)];
			КонецЕсли;
			
		Иначе
			
			КодОтчетногоПериода = "";
			
		КонецЕсли;
		
		Форма.ТабличныйДокумент.Области.Период.Значение = КодОтчетногоПериода;
		
	КонецЕсли;
	
	Форма.ТабличныйДокумент.Области.ОтчетГод.Значение
		= Формат(Год(Форма.СтруктураРеквизитовФормы.мДатаКонцаПериодаОтчета), "ЧГ=");
	
