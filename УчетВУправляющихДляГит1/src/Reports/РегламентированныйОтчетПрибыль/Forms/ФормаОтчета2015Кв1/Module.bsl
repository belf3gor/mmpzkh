						ПризнакНПЛ = СокрЛП(Форма.ТабличныйДокумент.Области.П000250000100.Значение);
						ПризнакРС  = СокрЛП(Форма.ТабличныйДокумент.Области.П000250001000.Значение);
						КПП_Л02_5  = СокрЛП(Форма.ТабличныйДокумент.Области.П000250002001.Значение);
						ЗначСтр121 = Форма.ТабличныйДокумент.Области.П000250012103.Значение;
					Иначе
						СтраницаДанные = ТаблицаРаздела[Инд].Данные[0].Значение;
						ПризнакНПЛ = СокрЛП(СтраницаДанные.П000250000100);
						ПризнакРС  = СокрЛП(СтраницаДанные.П000250001000);
						КПП_Л02_5  = СокрЛП(СтраницаДанные.П000250002001);
						ЗначСтр121 = СтраницаДанные.П000250012103;
					КонецЕсли;
					
					Если НЕ (ЗначениеЗаполнено(ПризнакНПЛ)
						И СтрНайти(ВозможныеПризнакиНПЛ, ПризнакНПЛ) > 0) Тогда
						Продолжить;
					КонецЕсли;
					
					Если НЕ (ЗначениеЗаполнено(ПризнакРС)
						И СтрНайти(ВозможныеПризнакиРС, ПризнакРС) > 0) Тогда
						Продолжить;
					КонецЕсли;
					
					Если ПризнакРС = "4" И НЕ (КПП_Л02_5 = КПП_ГолОрг) Тогда
