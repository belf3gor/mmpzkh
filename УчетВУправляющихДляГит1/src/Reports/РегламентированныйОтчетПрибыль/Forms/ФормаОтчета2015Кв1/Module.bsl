	Если КодНайден(мДанныеОтчета.Титульный.ПоМесту, "220") Тогда
		Возврат;
	КонецЕсли;
	
	ОбщееЧислоОшибок    = СтруктураРезультата.КоличествоНайденныхОшибок;
	ОбщееЧислоБезОшибок = СтруктураРезультата.КоличествоНайденныхБезОшибок;
	СтруктураОшибок     = СтруктураРезультата.СтруктураОшибок;
	
	ТекстФормулы    = НСтр("ru='Лист 02 (КС 78)'");
	ОписаниеКС      = НСтр("ru='Сумма ежемесячных авансовых платежей в бюджет субъекта РФ (строка 310 Листа 02) должна быть равна сумме величин по строке 120 всех Приложений 5 к Листу 02'");
	
	ОписаниеОшибки  = НСтр("ru='Возможно неправильное распределение суммы ежемесячного авансового платежа'");
	Нарушение  = НСтр("ru='п.2 ст. 286 НК РФ и ст.288 НК РФ'");
	
	Л02Ст1Гр00 = ПоказательПоСтроке("Л02Ст1Гр00");
	Л02Ст310Гр03 = ПоказательПоСтроке("Л02Ст310Гр03");
	Л02_5Ст1Гр00 = ПоказательПоСтроке("Л02_5Ст1Гр00");
	Л02_5Ст120Гр03 = ПоказательПоСтроке("Л02_5Ст120Гр03");
	Лист02 = мДанныеОтчета.Лист02;
	Лист02_5 = мДанныеОтчета.Лист02_5;
	
	Если НЕ РазделЗаполненПоПараметрам(Лист02_5, Новый Структура("П000250000100, П000250001000")) Тогда
		Возврат;
		
	КонецЕсли;
	
	//МассивПН = Новый Массив;
	//Для Каждого СтраницаЛ02 Из Лист02 Цикл
	//	ДанныеСтраницыЛ02 = СтраницаЛ02.Данные[0].Значение;
	//	
	//	ВставитьЗначениеПоказателя(ДанныеСтраницыЛ02, Л02Ст1Гр00);
	//	Если Л02Ст1Гр00.Значение <> "" Тогда
	//		МассивПН.Добавить(Л02Ст1Гр00.Значение);
	//		
	//	КонецЕсли;
	//	
	//КонецЦикла;
	
	МассивПН = Новый Массив;
	СобратьМассивПоказателейСЛистов(МассивПН, Лист02_5, Л02_5Ст1Гр00.Имя);
	
	Для Каждого ПН Из МассивПН Цикл
	
		СчетчикФормулДляРасшифровок = СчетчикФормулДляРасшифровок + 1;
		ЗарегистрироватьФормулуДляРасшифровки(ОписаниеКС);
		
		СуммаЛ02 = 0;
		СуммаЛ02Стр = "";
		ПНЛ02 = 0;
		Для Каждого СтраницаЛ02 Из Лист02 Цикл
			ДанныеСтраницыЛ02 = СтраницаЛ02.Данные[0].Значение;
			НомерСтраницыЛ02Стр = СтрокаЧГ0(Лист02.Индекс(СтраницаЛ02) + 1);
			
			ВставитьЗначениеПоказателя(ДанныеСтраницыЛ02, Л02Ст1Гр00);
