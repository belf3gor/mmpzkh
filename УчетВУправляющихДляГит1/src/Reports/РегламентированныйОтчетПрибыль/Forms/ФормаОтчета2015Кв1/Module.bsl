					ПризнакРС  = СокрЛП(СтраницаДанные.П000250001000);
					КПП_Л02_5  = СокрЛП(СтраницаДанные.П000250002001);
				КонецЕсли;
				
				Если НЕ (ЗначениеЗаполнено(ПризнакНПЛ)
					И СтрНайти(ВозможныеПризнакиНПЛ, ПризнакНПЛ) > 0) Тогда
					Продолжить;
				КонецЕсли;
				
				Если НЕ (ЗначениеЗаполнено(ПризнакРС)
					И СтрНайти(ВозможныеПризнакиРС, ПризнакРС) > 0) Тогда
					Продолжить;
				КонецЕсли;
				
				Если ПризнакРС = "4" И НЕ (КПП_Л02_5 = КПП_ГолОрг) Тогда
					Продолжить;
				КонецЕсли;
				
				ЕстьЗапПоУслП5кЛ02 = Истина;
				
				Прервать;
				
			КонецЦикла;
		КонецЕсли;
		
		Если ЕстьЗапПоУслП5кЛ02 Тогда
			
			ИмяРаздела = "Лист02";
			
			ВозможныеПризнакиНПЛ = "1|3|4";
			
			Форма.мСтруктураМногостраничныхРазделов.Свойство(ИмяРаздела, ТаблицаРаздела);
			
			Если ТаблицаРаздела <> Неопределено Тогда
				ТаблицаРаздела = Форма[ТаблицаРаздела];
				Для Инд = 0 По ТаблицаРаздела.Количество() - 1 Цикл
					
					Если ТаблицаРаздела[Инд].АктивнаяСтраница
						И Форма.СтруктураРеквизитовФормы.НаимТекущегоРаздела = ИмяРаздела Тогда
						ПризнакНПЛ = СокрЛП(Форма.ТабличныйДокумент.Области.П000200000100.Значение);
						ЗначСтр330 = Форма.ТабличныйДокумент.Области.П000200033003.Значение;
					Иначе
						СтраницаДанные = ТаблицаРаздела[Инд].Данные[0].Значение;
						ПризнакНПЛ = СокрЛП(СтраницаДанные.П000200000100);
