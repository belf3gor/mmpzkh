	Раздел.Вставить("П00050", "Листа 05");
	Раздел.Вставить("П00060", "Листа 06");
	Раздел.Вставить("П00070", "Листа 07");
	Раздел.Вставить("П00001", "Приложения 1");
	Раздел.Вставить("П00002", "Приложения 2");
	
	ПодстрокаРаздела = Раздел[Лев(ИмяПоказателя, 6)];
	ПодстрокаСтроки = Сред(ИмяПоказателя, 9, 3);
