		ПоМесту = СокрЛП(Форма.СтруктураДанныхТитульный.ПоМесту);
		КПП_ГолОрг = СокрЛП(Форма.СтруктураДанныхТитульный.КПП1);
	КонецЕсли;
	ОКТМО_ГолОрг = Форма.СтруктураРеквизитовФормы.ОКТМОДляРазделов;
	
	ЭтоКонсолидированнаяГруппа   = (ПоМесту = "218");
	ЭтоОбособленноеПодразделение = (ПоМесту = "220" ИЛИ ПоМесту = "223");
	
	Если НЕ ЭтоКонсолидированнаяГруппа И НЕ ЭтоОбособленноеПодразделение Тогда
		
		ЕстьЗапПоУслП5кЛ02 = Ложь;
		
		ИмяРаздела = "Лист02_5";
		
		ВозможныеПризнакиНПЛ = "1|3";
		ВозможныеПризнакиРС  = "1|4";
		
		Форма.мСтруктураМногостраничныхРазделов.Свойство(ИмяРаздела, ТаблицаРаздела);
		
