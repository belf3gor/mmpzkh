																    + Форма.ТабличныйДокумент.Области.П0000400070005.Значение;
		КонецЕсли;

		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П0000400071003, ОбластьИмя) > 0 Тогда
			
			Форма.ТабличныйДокумент.Области.П0000400071003.Значение = Форма.ТабличныйДокумент.Области.П0000400071004.Значение
																    + Форма.ТабличныйДокумент.Области.П0000400071005.Значение;
		КонецЕсли;

		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П0000400072003, ОбластьИмя) > 0 Тогда
			
			Форма.ТабличныйДокумент.Области.П0000400072003.Значение = Форма.ТабличныйДокумент.Области.П0000400072004.Значение
																    + Форма.ТабличныйДокумент.Области.П0000400072005.Значение;
		КонецЕсли;

		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П0000400073003, ОбластьИмя) > 0 Тогда
			
			Форма.ТабличныйДокумент.Области.П0000400073003.Значение = Форма.ТабличныйДокумент.Области.П0000400073004.Значение
																    + Форма.ТабличныйДокумент.Области.П0000400073005.Значение;
		КонецЕсли;

		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П0000400074003, ОбластьИмя) > 0 Тогда
			
			Форма.ТабличныйДокумент.Области.П0000400074003.Значение = Форма.ТабличныйДокумент.Области.П0000400074004.Значение
																    + Форма.ТабличныйДокумент.Области.П0000400074005.Значение;
		КонецЕсли;

		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П0000400075003, ОбластьИмя) > 0 Тогда
			
			Форма.ТабличныйДокумент.Области.П0000400075003.Значение = Форма.ТабличныйДокумент.Области.П0000400075004.Значение
																    + Форма.ТабличныйДокумент.Области.П0000400075005.Значение;
		КонецЕсли;

		Если НЕ ЗначениеЗаполнено(ОбластьИмя) ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета.П0000400076003, ОбластьИмя) > 0 Тогда
			
			Форма.ТабличныйДокумент.Области.П0000400076003.Значение = Форма.ТабличныйДокумент.Области.П0000400076004.Значение;
			
		КонецЕсли;

		
	ИначеЕсли ИмяТекущейСтраницыПанели = "" Тогда
		
