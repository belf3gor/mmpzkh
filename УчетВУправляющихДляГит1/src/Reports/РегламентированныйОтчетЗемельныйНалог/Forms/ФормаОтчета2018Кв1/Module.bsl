&НаСервере
Процедура	ПроверитьКС_1_32_код(Настройки)
	
	Запрос = Новый Запрос;
	Запрос.МенеджерВременныхТаблиц = мДанныеОтчета.ДанныеКС.ТаблицаДанныхКС;
	Запрос.УстановитьПараметр("КиРавноНулю", Настройки.КиРавноНулю);
	Запрос.Текст = 
	
	"ВЫБРАТЬ
	|	ВТ_ТаблицаР2.НомерКадастра КАК НомерКадастра
	|ПОМЕСТИТЬ ВТ_ТаблицаР2_Отбор
	|ИЗ
	|	ВТ_ТаблицаР2 КАК ВТ_ТаблицаР2
	|
	|СГРУППИРОВАТЬ ПО
	|	ВТ_ТаблицаР2.НомерКадастра
	|
	|ИМЕЮЩИЕ
	|	МИНИМУМ(ВТ_ТаблицаР2.С040) = 1 И
	|	МАКСИМУМ(ВТ_ТаблицаР2.С040) = 2 И
	|	ВЫБОР
	|		КОГДА &КиРавноНулю
	|			ТОГДА МИНИМУМ(ВТ_ТаблицаР2.С145) = 0
	|		ИНАЧЕ НЕ МАКСИМУМ(ВТ_ТаблицаР2.С145) = 0
	|	КОНЕЦ
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ВТ_ТаблицаР2.НомерКадастра КАК Р2КНЗУ,
	|	ВТ_ТаблицаР2.НомерСтраницы КАК НомерСтраницы,
	|	ВТ_ТаблицаР2.С040 КАК Р2С040,
	|	ВТ_ТаблицаР2.С110 КАК Р2С110,
	|	ВТ_ТаблицаР2.С120 КАК Р2С120,
	|	ВТ_ТаблицаР2.С140 КАК Р2С140,
	|	ВТ_ТаблицаР2.С145 КАК Р2С145,
	|	ВТ_ТаблицаР2.С150 КАК Р2С150
	|ИЗ
	|	ВТ_ТаблицаР2 КАК ВТ_ТаблицаР2
	|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_ТаблицаР2_Отбор КАК ВТ_ТаблицаР2_Отбор
	|		ПО ВТ_ТаблицаР2.НомерКадастра = ВТ_ТаблицаР2_Отбор.НомерКадастра
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтраницы
	|ИТОГИ
	|	МИНИМУМ(НомерСтраницы)
	|ПО
	|	Р2КНЗУ
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|УНИЧТОЖИТЬ ВТ_ТаблицаР2_Отбор";
	
	РезультатЗапроса = Запрос.Выполнить();
	ВыборкаК = РезультатЗапроса.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	
	Пока ВыборкаК.Следующий() Цикл
		Выборка = ВыборкаК.Выбрать();
		Пока Выборка.Следующий() Цикл
			
			Настройки.Вставить("ДопСведенияПараметры", 
				Новый Структура("НомерС, НомерК", Выборка.НомерСтраницы, Выборка.Р2КНЗУ));
			ЗарегистрироватьФормулу(Настройки);
		
			Показатели = ЗаполнитьПоказатели("Р2КНЗУ, Р2С040, Р2С110, Р2С120, Р2С140, Р2С145, Р2С150", Выборка,, Истина);
			РасшифроватьПоказатели(Показатели, Настройки, "Раздел2", Выборка.НомерСтраницы);
			
			Коэффициент = ?(Выборка.Р2С040 = 1, 2, 4);
			РасчетнаяСумма1	= Округлить(Выборка.Р2С110 * Выборка.Р2С120 * Выборка.Р2С140 * Коэффициент / 100);
			РасчетнаяСумма2	= Округлить(Выборка.Р2С110 * Выборка.Р2С120 * Выборка.Р2С140 * Выборка.Р2С145 * Коэффициент / 100);
			РасчетнаяСумма	= ?(Настройки.КиРавноНулю, РасчетнаяСумма1, РасчетнаяСумма2);
			Настройки.ЭтоОшибка		= НЕ Выборка.Р2С150 = РасчетнаяСумма;
			Настройки.Расшифровка	= СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(Настройки.ШаблонРасшифровки, 
				Выборка.Р2С150, Выборка.Р2С110, Выборка.Р2С120, Выборка.Р2С140, Коэффициент, РасчетнаяСумма, Выборка.Р2С145);
			ДобавитьЭлементВСтруктуруОшибок(Настройки);
		
		КонецЦикла;
	КонецЦикла;
	
КонецПроцедуры

