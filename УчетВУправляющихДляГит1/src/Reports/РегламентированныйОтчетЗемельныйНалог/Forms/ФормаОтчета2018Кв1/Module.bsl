Процедура	ПроверитьКС_1_30_код(Настройки)
	
	Запрос = Новый Запрос;
	Запрос.МенеджерВременныхТаблиц = мДанныеОтчета.ДанныеКС.ТаблицаДанныхКС;
	Запрос.УстановитьПараметр("Коэффициент", Настройки.Коэффициент);
	Запрос.УстановитьПараметр("КиРавноНулю", Настройки.КиРавноНулю);
	Запрос.Текст = 
	
	"ВЫБРАТЬ
	|	ВТ_ТаблицаР2.НомерКадастра КАК НомерКадастра
	|ПОМЕСТИТЬ ВТ_ТаблицаР2_Отбор
	|ИЗ
	|	ВТ_ТаблицаР2 КАК ВТ_ТаблицаР2
	|
	|СГРУППИРОВАТЬ ПО
	|	ВТ_ТаблицаР2.НомерКадастра
	|
	|ИМЕЮЩИЕ
	|	НЕ(МИНИМУМ(ВТ_ТаблицаР2.С040) = 1
	|			И МАКСИМУМ(ВТ_ТаблицаР2.С040) = 2) И
	|	ВЫБОР
	|		КОГДА &КиРавноНулю
	|			ТОГДА МИНИМУМ(ВТ_ТаблицаР2.С145) = 0
	|		ИНАЧЕ НЕ МАКСИМУМ(ВТ_ТаблицаР2.С145) = 0
	|	КОНЕЦ
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ВТ_ТаблицаР2.НомерКадастра КАК Р2КНЗУ,
	|	ВТ_ТаблицаР2.НомерСтраницы КАК НомерСтраницы,
	|	ВТ_ТаблицаР2.С040 КАК Р2С040,
