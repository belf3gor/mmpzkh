	Для НомерСтраницы = 1 По КоличествоСтраниц Цикл
		
		Раздел2 = мДанныеОтчета.Раздел2[НомерСтраницы - 1].Данные;
		Показатели = ЗаполнитьПоказатели("Р2КНЗУ, Р2С040, Р2С110, Р2С120, Р2С140, Р2С145, Р2С150", Раздел2);
		НоваяСтрока = ТаблицаР2.Добавить();
		НоваяСтрока.НомерКадастра = Показатели.Р2КНЗУ.Значение;
		НоваяСтрока.НомерСтраницы = "" + НомерСтраницы;
		НоваяСтрока.С040 = Показатели.Р2С040.Значение;
		НоваяСтрока.С110 = Показатели.Р2С110.Значение;
		НоваяСтрока.С120 = Показатели.Р2С120.Значение;
		НоваяСтрока.С140 = Показатели.Р2С140.Значение;
		НоваяСтрока.С145 = Показатели.Р2С145.Значение;
		НоваяСтрока.С150 = Показатели.Р2С150.Значение;
		
	КонецЦикла;
	
	ТаблицаДанныхКС = Новый МенеджерВременныхТаблиц;
	
	Запрос = Новый Запрос;
	Запрос.МенеджерВременныхТаблиц = ТаблицаДанныхКС;
	Запрос.УстановитьПараметр("ТаблицаР2", ТаблицаР2);
	
	Запрос.Текст =
	
	"ВЫБРАТЬ
	|	ТаблицаР2.НомерКадастра КАК НомерКадастра,
	|	ТаблицаР2.НомерСтраницы КАК НомерСтраницы,
	|	ТаблицаР2.С040 КАК С040,
	|	ТаблицаР2.С110 КАК С110,
	|	ТаблицаР2.С120 КАК С120,
	|	ТаблицаР2.С140 КАК С140,
	|	ТаблицаР2.С145 КАК С145,
	|	ТаблицаР2.С150 КАК С150
	|ПОМЕСТИТЬ ВТ_ТаблицаР2
	|ИЗ
	|	&ТаблицаР2 КАК ТаблицаР2";

	Запрос.Выполнить();
	мДанныеОтчета.ДанныеКС.Вставить("ТаблицаДанныхКС", ТаблицаДанныхКС);
	
