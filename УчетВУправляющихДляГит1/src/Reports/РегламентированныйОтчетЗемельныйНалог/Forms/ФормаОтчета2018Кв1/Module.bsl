		РасшифроватьПоказатели(Показатели, Настройки, "Раздел2", НомерСтраницы);
		РасчетнаяСумма = Округлить(Показатели.Р2С150.Значение * Показатели.Р2С220.Значение / 100);
		ЭтоОшибка = Не Показатели.Р2С230.Значение = РасчетнаяСумма;
		Настройки.Расшифровка = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(Настройки.ШаблонРасшифровки, 
			Показатели.Р2С230.Значение, Показатели.Р2С150.Значение, Показатели.Р2С220.Значение, РасчетнаяСумма);
		Настройки.ЭтоОшибка = ЭтоОшибка;
		ДобавитьЭлементВСтруктуруОшибок(Настройки);
		
	КонецЦикла;
	
	СтруктураРезультата = ОбработатьРезультат(Настройки, СтруктураРезультата);
	
КонецПроцедуры

