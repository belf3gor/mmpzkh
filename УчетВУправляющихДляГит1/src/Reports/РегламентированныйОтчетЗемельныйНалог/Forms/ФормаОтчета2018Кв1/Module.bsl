&НаСервере
Процедура	ПроверитьКС_1_01(СтруктураРезультата)
	
	// Формула КС: Р1С021 = сумме строк Р2С250 по сочетанию КБК - ОКТМО
	
	Заголовки = Новый Структура;
	Заголовки.Вставить("ТекстФормулы"		, НСтр("ru='Проверка КС 1.1'"));
	Заголовки.Вставить("Нарушение"			, НСтр("ru='НК РФ ст.387'"));
	Заголовки.Вставить("ОписаниеКС"			, 
		НСтр("ru='Исчисленная сумма налога, подлежащая уплате в бюджет за налоговый период (строка 021 Раздела 1) '") +
		НСтр("ru='должна быть равна сумме всех значений строк 250 Раздела 2 по соответствующим кодам ОКТМО и КБК'"));
	Заголовки.Вставить("ШаблонДопСведений"	, НСтр("ru='КБК %1, ОКТМО %2'"));
	Заголовки.Вставить("ШаблонРасшифровки"	, НСтр("ru='%1 = %2'"));
	Настройки = ОбработатьНастройки(		, Заголовки, СтруктураРезультата);
	Настройки.ОписаниеОшибок.Добавить(
		НСтр("ru='Завышена исчисленная сумма земельного налога, подлежащая уплате в бюджет за налоговый период'"));
	Настройки.ОписаниеОшибок.Добавить(
