		НСтр("ru='Сумма налоговой льготы (строка 230 Раздела 2) должна быть равна произведению значения строки 150 '") +
		НСтр("ru='Раздела 2 х процент, на который уменьшается исчисленная сумма налога (строка 220а) / 100'"));
	Заголовки.Вставить("ШаблонДопСведений"	, НСтр("ru='Раздел 2, Страница %1'"));
	Заголовки.Вставить("ШаблонРасшифровки"	, НСтр("ru='%1 = (%2 х %3) / 100 Итог [%4]'"));
	Заголовки.Вставить("ОписаниеОшибки"		, НСтр("ru='Неправильно указана сумма налоговой льготы'"));
	Настройки = ОбработатьНастройки(		, Заголовки, СтруктураРезультата);
	КоличествоСтраниц = мДанныеОтчета.Раздел2.Количество();
	
