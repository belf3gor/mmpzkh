&НаСервере
Процедура ПроверитьКС_1_01(СтруктураРезультата)
	
	Заголовки = Новый Структура;
	Заголовки.Вставить("ТекстФормулы"		, НСтр("ru='Проверка Раздела 1 (КС 1.1)'"));
	Заголовки.Вставить("Нарушение"			, НСтр("ru='ст. 45 НК РФ'"));
	Заголовки.Вставить("ОписаниеКС"			, 
		НСтр("ru='Сумма авансового платежа по налогу, подлежащая уплате в бюджет (стр.030 Раздела 1) должна '") +
		НСтр("ru='быть равна (стр.180 Раздела 2 - стр.200 Раздела 2) + (стр.090 Раздела 3 - стр.110 Раздела 3) '") +
		НСтр("ru='по соответствующим ОКТМО и КБК'"));
	Заголовки.Вставить("ШаблонДопСведений"	, НСтр("ru='ОКТМО %1, КБК %2'"));
	Заголовки.Вставить("ШаблонРасшифровки"	, НСтр("ru='%1 Итог [%2] = (%3) + (%4) Итог [%5], ОКТМО %6, КБК %7'"));
	Настройки = ОбработатьНастройки(		, Заголовки, СтруктураРезультата);
	Настройки.ОписаниеОшибок.Добавить(НСтр("ru='Завышена сумма авансового платежа по налогу'"));
	Настройки.ОписаниеОшибок.Добавить(НСтр("ru='Занижена сумма авансового платежа по налогу'"));
	
	Запрос = Новый Запрос;
	Запрос.МенеджерВременныхТаблиц = мДанныеОтчета.ДанныеКС.ВТ;
	Запрос.Текст =
	
	"ВЫБРАТЬ
	|	ВТ_Раздел1.Р01С020 + ВТ_Раздел1.Р01С010 КАК Ключ,
	|	ВТ_Раздел1.НомерСтраницы КАК НомерСтраницы,
	|	ВТ_Раздел1.НомерСтроки КАК НомерСтроки,
	|	ВТ_Раздел1.Р01С010 КАК Р01С010,
	|	ВТ_Раздел1.Р01С020 КАК Р01С020,
	|	ВТ_Раздел1.Р01С030 КАК Р01С030
	|ИЗ
	|	ВТ_Раздел1 КАК ВТ_Раздел1
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтраницы,
	|	НомерСтроки
	|ИТОГИ
	|	МИНИМУМ(НомерСтраницы),
	|	МИНИМУМ(НомерСтроки),
	|	МИНИМУМ(Р01С010),
	|	МИНИМУМ(Р01С020),
	|	СУММА(Р01С030)
	|ПО
	|	Ключ
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ВТ_Раздел2.Р02С011 + ВТ_Раздел2.Р02С010 КАК Ключ,
	|	ВТ_Раздел2.НомерСтраницы КАК НомерСтраницы,
	|	ВТ_Раздел2.Р02С180 КАК Р02С180,
	|	-ВТ_Раздел2.Р02С200 КАК Р02С200
	|ИЗ
	|	ВТ_Раздел2 КАК ВТ_Раздел2
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтраницы
	|ИТОГИ
	|	МИНИМУМ(НомерСтраницы),
	|	СУММА(Р02С180),
	|	СУММА(Р02С200)
	|ПО
	|	Ключ
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ВТ_Раздел3.Р03С011 + ВТ_Раздел3.Р03С010 КАК Ключ,
	|	ВТ_Раздел3.НомерСтраницы КАК НомерСтраницы,
	|	ВТ_Раздел3.Р03С015 КАК Р03С015,
	|	ВТ_Раздел3.Р03С090 КАК Р03С090,
	|	-ВТ_Раздел3.Р03С110 КАК Р03С110
	|ИЗ
	|	ВТ_Раздел3 КАК ВТ_Раздел3
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтраницы
	|ИТОГИ
	|	МИНИМУМ(НомерСтраницы),
	|	СУММА(Р03С090),
	|	СУММА(Р03С110)
	|ПО
	|	Ключ";
	
	Результат = Запрос.ВыполнитьПакет();
	ВыборкаР1 = Результат[0].Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	ВыборкаР2 = Результат[1].Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	ВыборкаР3 = Результат[2].Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	
	Пока ВыборкаР1.Следующий() Цикл
		Настройки.Вставить("ДопСведенияПараметры", Новый Структура("ОКТМО, КБК", ВыборкаР1.Р01С010, ВыборкаР1.Р01С020));
		ЗарегистрироватьФормулу(Настройки);
		СуммаР1Стр = "";
		СуммаР2Стр = "";
		СуммаР3Стр = "";
		СуммаР2 = 0;
		СуммаР3 = 0;
		Счетчик = 1;
			
		Выборка = ВыборкаР1.Выбрать();
		Пока Выборка.Следующий() Цикл
			Показатели = ЗаполнитьПоказатели("Р01С030", Выборка, Выборка.НомерСтроки, Истина);
			Дополнение = "Страница " + Выборка.НомерСтраницы + ", строка " + Выборка.НомерСтроки;
			ДобавитьИнфоКПредставлениюПоказателя(Показатели.Р01С030, Дополнение);
			РасшифроватьПоказатели(Показатели, Настройки, "Раздел1", Выборка.НомерСтраницы);
			Если Счетчик <= 5 И ЗначениеЗаполнено(Выборка.Р01С030) Тогда
				СуммаР1Стр = СуммаР1Стр + ?(Счетчик = 5, "<...>", Выборка.Р01С030) + " + ";
				Счетчик = Счетчик + 1;
			КонецЕсли;
		КонецЦикла;
		
		Если ВыборкаР2.НайтиСледующий(ВыборкаР1.Ключ, "Ключ") Тогда
			Счетчик = 1;
			СуммаР2 = ВыборкаР2.Р02С180 + ВыборкаР2.Р02С200;
			Выборка = ВыборкаР2.Выбрать();
			Пока Выборка.Следующий() Цикл
				Показатели = ЗаполнитьПоказатели("Р02С180, Р02С200", Выборка,, Истина);
				Дополнение = "Страница " + Выборка.НомерСтраницы;
				ДобавитьИнфоКПредставлениюПоказателя(Показатели.Р02С180, Дополнение);
				ДобавитьИнфоКПредставлениюПоказателя(Показатели.Р02С200, Дополнение);
				РасшифроватьПоказатели(Показатели, Настройки, "Раздел2", Выборка.НомерСтраницы);
				Если Счетчик <= 3 И ЗначениеЗаполнено(Выборка.Р02С180 + Выборка.Р02С200 * -1) Тогда
					СуммаР2Стр = СуммаР2Стр + ?(Счетчик = 3, "<...>", "" + Выборка.Р02С180 + " - " + Выборка.Р02С200 * -1) + " + ";
					Счетчик = Счетчик + 1;
				КонецЕсли;
			КонецЦикла;
		КонецЕсли; 
		
		Если ВыборкаР3.НайтиСледующий(ВыборкаР1.Ключ, "Ключ") Тогда
			Счетчик = 1;
			СуммаР3 = ВыборкаР3.Р03С090 + ВыборкаР3.Р03С110;
			Выборка = ВыборкаР3.Выбрать();
			Пока Выборка.Следующий() Цикл
				Показатели = ЗаполнитьПоказатели("Р03С090, Р03С110", Выборка,, Истина);
				Дополнение = "Страница " + Выборка.НомерСтраницы + ", кадастровый номер " + Выборка.Р03С015;
				ДобавитьИнфоКПредставлениюПоказателя(Показатели.Р03С090, Дополнение);
				ДобавитьИнфоКПредставлениюПоказателя(Показатели.Р03С110, Дополнение);
				РасшифроватьПоказатели(Показатели, Настройки, "Раздел3", Выборка.НомерСтраницы);
				Если Счетчик <= 3 И ЗначениеЗаполнено(Выборка.Р03С090 + Выборка.Р03С110 * -1) Тогда
					СуммаР3Стр = СуммаР3Стр + ?(Счетчик = 3, "<...>", "" + Выборка.Р03С090 + " - " + Выборка.Р03С110 * -1) + " + ";
					Счетчик = Счетчик + 1;
				КонецЕсли;
			КонецЦикла;
		КонецЕсли; 
		
		Настройки.ЭтоОшибка		= НЕ ВыборкаР1.Р01С030 = СуммаР2 + СуммаР3;
		Настройки.ОписаниеОшибки = ?(ВыборкаР1.Р01С030 > СуммаР2 + СуммаР3, Настройки.ОписаниеОшибок[0], 
			Настройки.ОписаниеОшибок[1]);
		Настройки.Расшифровка	= СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(Настройки.ШаблонРасшифровки, 
			РасшифровкаВСтр(СуммаР1Стр), ВыборкаР1.Р01С030, РасшифровкаВСтр(СуммаР2Стр), РасшифровкаВСтр(СуммаР3Стр), 
			СуммаР2 + СуммаР3, ВыборкаР1.Р01С010, ВыборкаР1.Р01С020);
		ДополнитьСтруктуруОшибок(Настройки);
		ВыборкаР2.Сбросить();
		ВыборкаР3.Сбросить();
	КонецЦикла;
	
	СтруктураРезультата = ОбработатьРезультат(Настройки, СтруктураРезультата);
	
КонецПроцедуры

