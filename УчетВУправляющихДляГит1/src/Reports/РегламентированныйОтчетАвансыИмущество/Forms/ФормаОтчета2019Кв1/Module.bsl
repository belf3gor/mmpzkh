	КонецЕсли;
	
	Заголовки = Новый Структура;
	Заголовки.Вставить("ТекстФормулы"		, НСтр("ru='Проверка Раздела 2 (КС 1.2)'"));
	Заголовки.Вставить("Нарушение"			, НСтр("ru='п. 4 ст. 376 НК РФ'"));
