&НаСервере
Процедура ПроверитьКС_1_24_код(Настройки)
	
	Настройки.ОписаниеОшибок.Добавить("ru='Завышена сумма авансового платежа'");
	Настройки.ОписаниеОшибок.Добавить("ru='Занижена сумма авансового платежа'");
	
	Запрос = Новый Запрос;
	Запрос.МенеджерВременныхТаблиц = мДанныеОтчета.ДанныеКС.ВТ;
	Запрос.Текст =
	
	"ВЫБРАТЬ
	|	ВТ_Раздел3.НомерСтраницы КАК НомерСтраницы,
	|	ВТ_Раздел3.Р03С020 КАК Р03С020,
	|	ВТ_Раздел3.Р03С025 КАК Р03С025,
	|	ВТ_Раздел3.Р03С030 КАК Р03С030,
	|	ВТ_Раздел3.Р03С031 КАК Р03С031,
	|	ВТ_Раздел3.Р03С050 КАК Р03С050,
	|	ВТ_Раздел3.Р03С051 КАК Р03С051,
	|	ВТ_Раздел3.Р03С070 КАК Р03С070,
	|	ВТ_Раздел3.Р03С080 КАК Р03С080,
	|	ВТ_Раздел3.Р03С085 КАК Р03С085,
	|	ВТ_Раздел3.Р03С090 КАК Р03С090
	|ИЗ
	|	ВТ_Раздел3 КАК ВТ_Раздел3
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтраницы";
	
	Результат = Запрос.Выполнить();
	Выборка = Результат.Выбрать();
	
	Если Настройки.НомерКС = 24 Тогда
		Исключения = "Р03С080, Р03С085";
	ИначеЕсли Настройки.НомерКС = 23 Тогда
		Исключения = "Р03С080";
	ИначеЕсли Настройки.НомерКС = 21 Тогда
		Исключения = "Р03С085";
	Иначе
		Исключения = "";
	КонецЕсли; 
	
	Пока Выборка.Следующий() Цикл
		Показатели = ЗаполнитьПоказатели(
			"Р03С020, Р03С025, Р03С030, Р03С031, Р03С050, Р03С051, Р03С070, Р03С080, Р03С085, Р03С090", Выборка,, Истина);
			
		Если Настройки.НомерКС = 22 И НЕ (Выборка.Р03С080 <> 0 И Выборка.Р03С080 <> 1 
			И Выборка.Р03С085 <> 0 И Выборка.Р03С085 <> 1) Тогда
			Продолжить;
		ИначеЕсли Настройки.НомерКС = 21 И НЕ (Выборка.Р03С080 <> 0 И Выборка.Р03С080 <> 1 
			И (Выборка.Р03С085 = 0 ИЛИ Выборка.Р03С085 = 1)) Тогда
			Продолжить;
		ИначеЕсли Настройки.НомерКС = 23 И НЕ ((Выборка.Р03С080 = 0 ИЛИ Выборка.Р03С080 = 1) 
			И Выборка.Р03С085 <> 0 И Выборка.Р03С085 <> 1) Тогда
			Продолжить;
		ИначеЕсли Настройки.НомерКС = 24 И НЕ ((Выборка.Р03С080 = 0 ИЛИ Выборка.Р03С080 = 1) 
			И (Выборка.Р03С085 = 0 ИЛИ Выборка.Р03С085 = 1)) Тогда
			Продолжить;
		КонецЕсли;
		
		Настройки.Вставить("ДопСведенияПараметры", Новый Структура("Страница", Выборка.НомерСтраницы));
		ЗарегистрироватьФормулу(Настройки);
		РасшифроватьПоказатели(Показатели, Настройки, "Раздел3", Выборка.НомерСтраницы,, Исключения);
		Разность = Макс(Выборка.Р03С020 - Выборка.Р03С025, 0);
		Стр030 = ?(Выборка.Р03С031 = 0, 1, Выборка.Р03С030 / Выборка.Р03С031);
		Стр050 = ?(Выборка.Р03С051 = 0, 1, Выборка.Р03С050 / Выборка.Р03С051);
		Стр080 = ?(Выборка.Р03С080 = 0, 1, Выборка.Р03С080);
		Стр085 = ?(Выборка.Р03С085 = 0, 1, Выборка.Р03С085);
		РасчетнаяСумма = Окр(0.25 * Разность * Стр030 * Стр050 * Выборка.Р03С070 * Стр080 * Стр085 / 100, 4);
		РасчетнаяСуммаОкр = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, РасчетнаяСумма);
		Настройки.ЭтоОшибка = НЕ Выборка.Р03С090 = РасчетнаяСуммаОкр;
		Настройки.ОписаниеОшибки = ?(Выборка.Р03С090 > РасчетнаяСуммаОкр, Настройки.ОписаниеОшибок[0], 
			Настройки.ОписаниеОшибок[1]);
		Стр030Стр = ПредставлениеДроби(Показатели.Р03С030, Показатели.Р03С031);
		Стр050Стр = ПредставлениеДроби(Показатели.Р03С050, Показатели.Р03С051);
		Настройки.Расшифровка = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(Настройки.ШаблонРасшифровки, 
			Показатели.Р03С090.ЗначениеСтр, Показатели.Р03С020.ЗначениеСтр, Показатели.Р03С025.ЗначениеСтр, Стр030Стр, 
			Стр050Стр, Показатели.Р03С070.ЗначениеСтр, Показатели.Р03С080.ЗначениеСтр, Показатели.Р03С085.ЗначениеСтр, 
			РасчетнаяСумма);
		ДополнитьСтруктуруОшибок(Настройки);
	КонецЦикла;
	
КонецПроцедуры

