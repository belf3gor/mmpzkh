	Структ.Вставить("П000020014003", "П000020002004;П000020004004;П000020004004;П000020005004;П000020006004;П000020007004;П000020008004;П000020009004;П000020010004;П000020011004");
	Структ.Вставить("П000020018003", "П000020000101;П000020002003;П000020003003;П000020004003;П000020005003;П000020006003;П000020007003;П000020008003;П000020009003;П000020010003;П000020011003;П000020002004;П000020004004;П000020004004;П000020005004;П000020006004;П000020007004;П000020008004;П000020009004;П000020010004;П000020011004;П000020012003;П000020014003;П000020015003_Числ;П000020015003_Знам;П000020017003;П000020017503");
	СтруктураЗависимыхЯчеекДляАвторасчета.Вставить("Раздел2", Структ);
	
