	Если Раздел1Заполнен Тогда
		ПроверитьКС_2_01_доп(СтруктураРезультата);
	КонецЕсли;
		
	Если Раздел2Заполнен Тогда
		ПроверитьКС_2_02_доп(СтруктураРезультата);
