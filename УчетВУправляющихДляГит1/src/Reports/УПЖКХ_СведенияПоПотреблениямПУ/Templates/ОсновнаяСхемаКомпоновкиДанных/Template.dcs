////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	врПоказанияИтоговыеПоМесяцам.ДневноеПоказание КАК ДневноеПоказание,
	врПоказанияИтоговыеПоМесяцам.НочноеПоказание КАК НочноеПоказание,
	врПоказанияИтоговыеПоМесяцам.ПиковоеПоказание КАК ПиковоеПоказание,
	врПоказанияИтоговыеПоМесяцам.Дата КАК Дата,
	врПоказанияИтоговыеПоМесяцам.МоментВремени КАК МоментВремени,
	врПоказанияИтоговыеПоМесяцам.Счетчик КАК Счетчик,
	врПоказанияИтоговыеПоМесяцам.Месяц КАК Месяц,
	МАКСИМУМ(врПоказанияИтоговыеПоМесяцам2.Месяц) КАК МесяцПредыдущий
ПОМЕСТИТЬ ПоказанияИтоговыеСПредыдущимМесяцем
ИЗ
	врПоказанияИтоговыеПоМесяцам КАК врПоказанияИтоговыеПоМесяцам
		ЛЕВОЕ СОЕДИНЕНИЕ врПоказанияИтоговыеПоМесяцам КАК врПоказанияИтоговыеПоМесяцам2
		ПО (врПоказанияИтоговыеПоМесяцам2.Счетчик = врПоказанияИтоговыеПоМесяцам.Счетчик)
			И (врПоказанияИтоговыеПоМесяцам2.Месяц &lt; врПоказанияИтоговыеПоМесяцам.Месяц)

СГРУППИРОВАТЬ ПО
	врПоказанияИтоговыеПоМесяцам.ДневноеПоказание,
	врПоказанияИтоговыеПоМесяцам.НочноеПоказание,
	врПоказанияИтоговыеПоМесяцам.ПиковоеПоказание,
	врПоказанияИтоговыеПоМесяцам.Дата,
	врПоказанияИтоговыеПоМесяцам.МоментВремени,
	врПоказанияИтоговыеПоМесяцам.Счетчик,
	врПоказанияИтоговыеПоМесяцам.Месяц
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПоказанияИтоговыеСПредыдущимМесяцем.ДневноеПоказание КАК ДневноеПоказание,
	ПоказанияИтоговыеСПредыдущимМесяцем.НочноеПоказание КАК НочноеПоказание,
	ПоказанияИтоговыеСПредыдущимМесяцем.ПиковоеПоказание КАК ПиковоеПоказание,
	ПоказанияИтоговыеСПредыдущимМесяцем.Дата КАК Дата,
	ПоказанияИтоговыеСПредыдущимМесяцем.МоментВремени КАК МоментВремени,
	ПоказанияИтоговыеСПредыдущимМесяцем.Счетчик КАК Счетчик,
	ПоказанияИтоговыеСПредыдущимМесяцем.Месяц КАК Месяц,
	врПоказанияИтоговыеПоМесяцам.Дата КАК ДатаПред,
	врПоказанияИтоговыеПоМесяцам.ДневноеПоказание КАК ДневноеПоказаниеПред,
	врПоказанияИтоговыеПоМесяцам.НочноеПоказание КАК НочноеПоказаниеПред,
	врПоказанияИтоговыеПоМесяцам.ПиковоеПоказание КАК ПиковоеПоказаниеПред
ПОМЕСТИТЬ ПоказанияИтоговыеПоМесяцам
ИЗ
	ПоказанияИтоговыеСПредыдущимМесяцем КАК ПоказанияИтоговыеСПредыдущимМесяцем
		ЛЕВОЕ СОЕДИНЕНИЕ врПоказанияИтоговыеПоМесяцам КАК врПоказанияИтоговыеПоМесяцам
		ПО (врПоказанияИтоговыеПоМесяцам.Счетчик = ПоказанияИтоговыеСПредыдущимМесяцем.Счетчик)
			И (врПоказанияИтоговыеПоМесяцам.Месяц = ПоказанияИтоговыеСПредыдущимМесяцем.МесяцПредыдущий)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	врПУ.ВидОбъекта КАК ВидОбъекта,
	врПУ.УровеньИерархии КАК УровеньИерархии,
	врПУ.Здание КАК Здание,
	врПУ.Паркинг КАК Паркинг,
	врПУ.Подъезд КАК Подъезд,
	врПУ.ПредставлениеОбъекта КАК ПредставлениеОбъекта,
	врПУ.Объект КАК Объект,
	врПУ.ЛицевойСчет КАК ЛицевойСчет,
	врПУ.НомСчетчика КАК НомСчетчика,
	врПУ.ВидУслуги КАК ВидУслуги,
	ВЫБОР
		КОГДА врПУ.ВидТарифа = ЗНАЧЕНИЕ(Перечисление.КВП_ВидыТарифов.Дневной)
			ТОГДА ЕСТЬNULL(ПоказанияСчетчиков.ДневноеПоказание, 0)
		КОГДА врПУ.ВидТарифа = ЗНАЧЕНИЕ(Перечисление.КВП_ВидыТарифов.Ночной)
			ТОГДА ЕСТЬNULL(ПоказанияСчетчиков.НочноеПоказание, 0)
		КОГДА врПУ.ВидТарифа = ЗНАЧЕНИЕ(Перечисление.КВП_ВидыТарифов.Пиковый)
			ТОГДА ЕСТЬNULL(ПоказанияСчетчиков.ПиковоеПоказание, 0)
		ИНАЧЕ 0
	КОНЕЦ КАК ПокТек,
	ПоказанияСчетчиков.Дата КАК ДатаТек,
	ВЫБОР
		КОГДА врПУ.ВидТарифа = ЗНАЧЕНИЕ(Перечисление.КВП_ВидыТарифов.Дневной)
			ТОГДА ЕСТЬNULL(ПоказанияСчетчиков.ДневноеПоказаниеПред, ЕСТЬNULL(врПУ.ПокПред, 0))
		КОГДА врПУ.ВидТарифа = ЗНАЧЕНИЕ(Перечисление.КВП_ВидыТарифов.Ночной)
			ТОГДА ЕСТЬNULL(ПоказанияСчетчиков.НочноеПоказаниеПред, ЕСТЬNULL(врПУ.ПокПред, 0))
		КОГДА врПУ.ВидТарифа = ЗНАЧЕНИЕ(Перечисление.КВП_ВидыТарифов.Пиковый)
			ТОГДА ЕСТЬNULL(ПоказанияСчетчиков.ПиковоеПоказаниеПред, ЕСТЬNULL(врПУ.ПокПред, 0))
		ИНАЧЕ 0
	КОНЕЦ КАК ПокПред,
	ЕСТЬNULL(ПоказанияСчетчиков.ДатаПред, врПУ.ДатаПред) КАК ДатаПред,
	врПУ.КодКвартиры КАК КодКвартиры,
	врПУ.Квартира КАК Квартира,
	врПУ.ПоказаниеСч КАК ПоказаниеСч,
	врПУ.ВидТарифа КАК ВидПоказания,
	врПУ.Счетчик КАК Счетчик,
	врПУ.ПоказаниеСч2 КАК ПоказаниеСч2,
	врПУ.КоличествоПУ КАК КоличествоПУ,
	ВЫБОР
		КОГДА врПУ.ВидТарифа = ЗНАЧЕНИЕ(Перечисление.КВП_ВидыТарифов.Дневной)
			ТОГДА ЕСТЬNULL(ПоказанияСчетчиков.ДневноеПоказание, 0)
		КОГДА врПУ.ВидТарифа = ЗНАЧЕНИЕ(Перечисление.КВП_ВидыТарифов.Ночной)
			ТОГДА ЕСТЬNULL(ПоказанияСчетчиков.НочноеПоказание, 0)
		КОГДА врПУ.ВидТарифа = ЗНАЧЕНИЕ(Перечисление.КВП_ВидыТарифов.Пиковый)
			ТОГДА ЕСТЬNULL(ПоказанияСчетчиков.ПиковоеПоказание, 0)
		ИНАЧЕ 0
	КОНЕЦ КАК ПоказаниеВМесяце,
	ПоказанияСчетчиков.Месяц КАК Месяц,
	ПоказанияСчетчиков.Дата КАК ДатаПоказания,
	врПУ.ДатаПоверки КАК ДатаПоверки
ПОМЕСТИТЬ ВрТаблицаПУСПоказаниямиПоМесяцам
ИЗ
	врПУ КАК врПУ
		ЛЕВОЕ СОЕДИНЕНИЕ ПоказанияИтоговыеПоМесяцам КАК ПоказанияСчетчиков
		ПО врПУ.Счетчик = ПоказанияСчетчиков.Счетчик
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	врПУ.ВидОбъекта КАК ВидОбъекта,
	врПУ.УровеньИерархии КАК УровеньИерархии,
	врПУ.Здание КАК Здание,
	врПУ.Паркинг КАК Паркинг,
	врПУ.Подъезд КАК Подъезд,
	врПУ.ПредставлениеОбъекта КАК ПредставлениеОбъекта,
	врПУ.Объект КАК Объект,
	врПУ.ЛицевойСчет КАК ЛицевойСчет,
	врПУ.НомСчетчика КАК НомСчетчика,
	врПУ.ВидУслуги КАК ВидУслуги,
	врПУ.ПокТек КАК ПокТек,
	врПУ.ДатаТек КАК ДатаТек,
	врПУ.ПокПред КАК ПокПред,
	врПУ.ДатаПред КАК ДатаПред,
	врПУ.КодКвартиры КАК КодКвартиры,
	врПУ.Квартира КАК Квартира,
	врПУ.ПоказаниеСч КАК ПоказаниеСч,
	врПУ.ВидПоказания КАК ВидПоказания,
	ЕСТЬNULL(УПЖКХ_РасчетПриборовУчетаОбороты.ПоказаниеПриход, 0) / ВЫБОР
		КОГДА врПУ.Счетчик.Коэффициент = 0
			ТОГДА 1
		ИНАЧЕ врПУ.Счетчик.Коэффициент
	КОНЕЦ КАК Расход,
	ЕСТЬNULL(УПЖКХ_РасчетПриборовУчетаОбороты.ПоказаниеПриход, 0) КАК УчетноеКоличество,
	врПУ.Счетчик КАК Счетчик,
	врПУ.ПоказаниеСч2 КАК ПоказаниеСч2,
	врПУ.КоличествоПУ КАК КоличествоПУ,
	врПУ.ПоказаниеВМесяце КАК ПоказаниеВМесяце,
	врПУ.Месяц КАК Месяц,
	врПУ.ДатаПоказания КАК ДатаПоказания,
	врПУ.ДатаПоверки КАК ДатаПоверки
ИЗ
	ВрТаблицаПУСПоказаниямиПоМесяцам КАК врПУ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.УПЖКХ_РасчетПриборовУчета.Обороты(&amp;НачалоПериода, &amp;КонецПериода, Месяц, {(ВидПоказания).* КАК ВидПоказания}) КАК УПЖКХ_РасчетПриборовУчетаОбороты
