// Функция РАСЧЕТ ()
// Выполняет расчет вычисляемых показателей на заданной странице отчета
// (ячеек, выделенных зеленым цветом). 
// В случае неуказания листа, ничего происходить не будет
// Параметры:
//  ИмяСтраницыПанели - строка, имя страницы основной панели формы.
//
&НаКлиентеНаСервереБезКонтекста
Процедура Расчет(Форма, ИмяТекущейСтраницыПанели = "", ОбластьИмя = "")
	
	Если ИмяТекущейСтраницыПанели = "Разделы1_4" Тогда
		ОбластиТабДок = Форма.ТабличныйДокумент.Области;
		ОбластиТабДок["П030306_6"].Значение = ОбластиТабДок["П030307_6"].Значение;
		ОбластиТабДок["П030306_7"].Значение = ОбластиТабДок["П030307_7"].Значение;
		ОбластиТабДок["П030302_6"].Значение = ОбластиТабДок["П030303_6"].Значение;
		ОбластиТабДок["П030302_7"].Значение = ОбластиТабДок["П030303_7"].Значение;
		ОбластиТабДок["П030302_3"].Значение = ОбластиТабДок["П030303_3"].Значение+ОбластиТабДок["П030304_3"].Значение;
		ОбластиТабДок["П030302_4"].Значение = ОбластиТабДок["П030303_4"].Значение+ОбластиТабДок["П030304_4"].Значение;
		ОбластиТабДок["П030302_5"].Значение = ОбластиТабДок["П030303_5"].Значение+ОбластиТабДок["П030304_5"].Значение;
	ИначеЕсли ИмяТекущейСтраницыПанели = "Разделы5_7" Тогда
		ОбластиТабДок = Форма.ТабличныйДокумент.Области;
		Для Гр = 3 По 4 Цикл 
			ЗначСумма = 0;
			Для Стр = 5 По 14 Цикл 
				Если Стр = 9 Тогда 
					Продолжить;
				КонецЕсли;
				
				ЗначСумма = ЗначСумма + ОбластиТабДок["П0505"+Формат(Стр, "ЧЦ=2; ЧВН=; ЧГ=;")+"0"+Гр].Значение;
			КонецЦикла;
			
			ОбластиТабДок["П0505040"+Гр].Значение = ЗначСумма;
			ОбластиТабДок["П0505030"+Гр].Значение = ЗначСумма+ОбластиТабДок["П0505150"+Гр].Значение+ОбластиТабДок["П0505160"+Гр].Значение;
		КонецЦикла;
	ИначеЕсли ИмяТекущейСтраницыПанели = "Разделы8_10" Тогда
		ОбластиТабДок = Форма.ТабличныйДокумент.Области;
		Для Стр = 8 По 11 Цикл
			Начало = "П0909" + Формат(Стр, "ЧЦ=2; ЧВН=; ЧГ=;") + "_";
			ОбластиТабДок[Начало + "4"].Значение = ОбластиТабДок[Начало + "5"].Значение
			+?(Стр = 11, 0, ОбластиТабДок[Начало + "6"].Значение);
		КонецЦикла;
		
		Для Гр = 3 По 8 Цикл 
			Сумма = 0;
			Для Стр = 2 По 7 Цикл 
				Сумма = Сумма + ОбластиТабДок["П10100"+Стр+"_"+Гр].Значение;
			КонецЦикла;
			ОбластиТабДок["П101001_"+Гр].Значение = Сумма;
		КонецЦикла;
	ИначеЕсли ИмяТекущейСтраницыПанели = "Разделы11_12" Тогда
		ОбластиТабДок = Форма.ТабличныйДокумент.Области;
		Для Стр = 13 По 26 Цикл
			Если Стр > 15 И Стр < 24 Тогда 
				Продолжить;
			КонецЕсли;
			
			Если СокрЛП(ОбластиТабДок["П1111"+Стр+"_1"].Значение) = "" Тогда
				ОбластиТабДок["П1111"+Стр+"_2"].Значение = 0;
			Иначе
				ОбластиТабДок["П1111"+Стр+"_2"].Значение = 1;
			КонецЕсли;
		КонецЦикла;
	КонецЕсли;
	
КонецПроцедуры

