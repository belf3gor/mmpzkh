			Список.Добавить(2,"2 - 256-511 Кбит/сек");
			Список.Добавить(3,"3 - 512 Кбит/сек - 1.9 Мбит/сек");
			Список.Добавить(4,"4 - 2.0-30.0 Мбит/сек");
			Список.Добавить(5,"5 - 30.1-100.0 Мбит/сек");
			Список.Добавить(6,"6 - выше 100 Мбит/сек");
			
			НайденнаяСтрока = Список.НайтиПоЗначению(Область.Значение);
			
			ДополнительныеПараметры = Новый Структура("Область, Элемент", Область, Элемент);
			ОписаниеОповещения = Новый ОписаниеОповещения("ТабличныйДокументВыборЗавершение", ЭтотОбъект, ДополнительныеПараметры);
						
			// обход ошибок платформы: в тонком клиенте не работает ПоказатьВыборИзСписка (ПоказатьВыборИзМеню не работает в веб-клиенте 
			// и в событии ПриИзмененииСодержимогоОбласти табличного документа - где мы его и хотим использовать прежде всего)
			# Если ВебКлиент Тогда
			
				Если НайденнаяСтрока = Неопределено Тогда
					ПоказатьВыборИзСписка(ОписаниеОповещения, Список, Элемент);
				Иначе
					ПоказатьВыборИзСписка(ОписаниеОповещения, Список, Элемент, НайденнаяСтрока);
				КонецЕсли;
				
			#Иначе
				
				Список.ПоказатьВыборЭлемента(ОписаниеОповещения, "Выберите значение", Элемент);
					
			#КонецЕсли
			
			Возврат;
			
		ИначеЕсли ОбластьИмя = "П103030703" Или ОбластьИмя = "П103030803" Тогда
			
			СтандартнаяОбработка = Ложь;
			
			Список = Новый СписокЗначений;
			Список.Добавить(0,"Пустое значение      ");
			Список.Добавить(1,"1 - ниже 256 Кбит/сек");
			Список.Добавить(7,"7 - выше 256 Кбит/сек");
			Список.Добавить(8,"8 - этот вид доступа не используется");
			
			НайденнаяСтрока = Список.НайтиПоЗначению(Область.Значение);
			
			ДополнительныеПараметры = Новый Структура("Область, Элемент", Область, Элемент);
			ОписаниеОповещения = Новый ОписаниеОповещения("ТабличныйДокументВыборЗавершение", ЭтотОбъект, ДополнительныеПараметры);
						
			// обход ошибок платформы: в тонком клиенте не работает ПоказатьВыборИзСписка (ПоказатьВыборИзМеню не работает в веб-клиенте 
			// и в событии ПриИзмененииСодержимогоОбласти табличного документа - где мы его и хотим использовать прежде всего)
			# Если ВебКлиент Тогда
			
				Если НайденнаяСтрока = Неопределено Тогда
					ПоказатьВыборИзСписка(ОписаниеОповещения, Список, Элемент);
				Иначе
					ПоказатьВыборИзСписка(ОписаниеОповещения, Список, Элемент, НайденнаяСтрока);
				КонецЕсли;
				
			#Иначе
				
				Список.ПоказатьВыборЭлемента(ОписаниеОповещения, "Выберите значение", Элемент);
					
			#КонецЕсли
			
			Возврат;
			
		ИначеЕсли ОбластьИмя = "П104042803" Тогда
			
			СтандартнаяОбработка = Ложь;
			
			Список = Новый СписокЗначений;
			Список.Добавить(0,"Пустое значение");
			Список.Добавить(1,"1 - полностью удовлетворены");
			Список.Добавить(2,"2 - частично удовлетворены");
			Список.Добавить(3,"3 - не удовлетворены");
			Список.Добавить(4,"4 - затрудняюсь ответить");
			
			НайденнаяСтрока = Список.НайтиПоЗначению(Область.Значение);
			
			ДополнительныеПараметры = Новый Структура("Область, Элемент", Область, Элемент);
			ОписаниеОповещения = Новый ОписаниеОповещения("ТабличныйДокументВыборЗавершение", ЭтотОбъект, ДополнительныеПараметры);
						
			// обход ошибок платформы: в тонком клиенте не работает ПоказатьВыборИзСписка (ПоказатьВыборИзМеню не работает в веб-клиенте 
			// и в событии ПриИзмененииСодержимогоОбласти табличного документа - где мы его и хотим использовать прежде всего)
			# Если ВебКлиент Тогда
			
				Если НайденнаяСтрока = Неопределено Тогда
					ПоказатьВыборИзСписка(ОписаниеОповещения, Список, Элемент);
				Иначе
					ПоказатьВыборИзСписка(ОписаниеОповещения, Список, Элемент, НайденнаяСтрока);
