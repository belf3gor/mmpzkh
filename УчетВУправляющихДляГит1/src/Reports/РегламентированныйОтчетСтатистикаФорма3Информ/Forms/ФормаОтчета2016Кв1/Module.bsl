			#Иначе
				
				Список.ПоказатьВыборЭлемента(ОписаниеОповещения, "Выберите значение", Элемент);
					
			#КонецЕсли
			
			Возврат;
			
		КонецЕсли;
		
	КонецЕсли;
	
	Если НЕ СтандартнаяОбработка И Модифицированность Тогда
		
		ТабличныйДокументПриИзмененииСодержимогоОбласти(Элемент, Область);
		
