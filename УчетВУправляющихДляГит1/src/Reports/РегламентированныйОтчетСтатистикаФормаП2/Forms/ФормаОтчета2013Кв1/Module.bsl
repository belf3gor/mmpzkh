	КонецЕсли;
	
	РегламентированнаяОтчетность.ДобавитьЭлементВСтруктуруОшибок(СтруктураОшибок, ТекстФормулы, "", ОбщееЧислоОшибок, ОбщееЧислоБезОшибок, Расшифровка, Нарушение, Описание, ЭтоОшибка);
	
	Знак = "=";
	ТекстФормулы = "(стр. 31 + стр. 34) гр. 2 = стр. 13 гр. 1";
	Описание     = "(стр. 31 + стр. 34) гр. 2 <> стр. 13 гр. 1";
	
	ЛеваяЧасть_1  = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели["П030003106"]);
