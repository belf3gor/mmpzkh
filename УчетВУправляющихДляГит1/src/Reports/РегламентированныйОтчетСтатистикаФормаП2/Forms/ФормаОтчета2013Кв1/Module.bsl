	РегламентированнаяОтчетность.ДобавитьЭлементВСтруктуруОшибок(СтруктураОшибок, ТекстФормулы, "", ОбщееЧислоОшибок, ОбщееЧислоБезОшибок, Расшифровка, Нарушение, Описание, ЭтоОшибка);
	
	
	Знак = "=";
	ТекстФормулы = "(стр. 31 + стр. 34) гр. 3 = (стр. 15 + стр. 19 + стр. 21) гр. 1";
	Описание     = "(стр. 31 + стр. 34) гр. 3 <> (стр. 15 + стр. 19 + стр. 21) гр. 1";
	
	ЛеваяЧасть_1  = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели["П030005007"]);
	ЛеваяЧасть_2  = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели["П030003207"]);
	ПраваяЧасть_1 = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели["П020002403"]);
	ПраваяЧасть_2 = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели["П020002503"]);
	ПраваяЧасть_3 = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели["П020002603"]);
	Расшифровка = Строка(ЛеваяЧасть_1) + " + " + Строка(ЛеваяЧасть_2) + " " + Знак + " " + Строка(ПраваяЧасть_1) + " + " + Строка(ПраваяЧасть_2) + " + " + Строка(ПраваяЧасть_3);
	
	Если ЛеваяЧасть_1 + ЛеваяЧасть_2 = ПраваяЧасть_1 + ПраваяЧасть_2 + ПраваяЧасть_3 Тогда
		ЭтоОшибка = Ложь;
		ОбщееЧислоБезОшибок = ОбщееЧислоБезОшибок + 1;
