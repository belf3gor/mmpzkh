		ЭтоОшибка = Истина;
		ОбщееЧислоОшибок = ОбщееЧислоОшибок + 1;
	КонецЕсли;
	
	РегламентированнаяОтчетность.ДобавитьЭлементВСтруктуруОшибок(СтруктураОшибок, ТекстФормулы, "", ОбщееЧислоОшибок, ОбщееЧислоБезОшибок, Расшифровка, Нарушение, Описание, ЭтоОшибка);
	
	Знак = ">=";
	Для Стр = 1 По 6 Цикл
		Если Стр = 5 Тогда 
			Продолжить;
		КонецЕсли;
		
		ТекстФормулы = "гр. 3 >= гр. 4 по стр. " + Стр;
		Описание     = "гр. 3 < гр. 4 по стр. " + Стр;
		ЛеваяЧасть  = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели["П0200020" + Стр + "5"]);
