		Расшифровка = Строка(ЛеваяЧасть) + " " + Знак + " " + Строка(ПраваяЧасть);
		
		Если ЛеваяЧасть >= ПраваяЧасть Тогда
			ЭтоОшибка = Ложь;
			ОбщееЧислоБезОшибок = ОбщееЧислоБезОшибок + 1;
		Иначе
			ЭтоОшибка = Истина;
			ОбщееЧислоОшибок = ОбщееЧислоОшибок + 1;
		КонецЕсли;
		
		РегламентированнаяОтчетность.ДобавитьЭлементВСтруктуруОшибок(СтруктураОшибок, ТекстФормулы, "", ОбщееЧислоОшибок, ОбщееЧислоБезОшибок, Расшифровка, Нарушение, Описание, ЭтоОшибка);
	КонецЦикла;
	
	Знак = ">=";
	Для Стр = 10 По 12 Цикл
		ТекстФормулы = "гр. 3 >= гр. 4 по стр. " + Стр;
		Описание     = "гр. 3 < гр. 4 по стр. " + Стр;
		ЛеваяЧасть  = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели["П0200022" + (Стр - 9) + "5"]);
		ПраваяЧасть  = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели["П0200022" + (Стр - 9) + "6"]);
		Расшифровка = Строка(ЛеваяЧасть) + " " + Знак + " " + Строка(ПраваяЧасть);
		
		Если ЛеваяЧасть >= ПраваяЧасть Тогда
			ЭтоОшибка = Ложь;
			ОбщееЧислоБезОшибок = ОбщееЧислоБезОшибок + 1;
		Иначе
			ЭтоОшибка = Истина;
			ОбщееЧислоОшибок = ОбщееЧислоОшибок + 1;
		КонецЕсли;
		
		РегламентированнаяОтчетность.ДобавитьЭлементВСтруктуруОшибок(СтруктураОшибок, ТекстФормулы, "", ОбщееЧислоОшибок, ОбщееЧислоБезОшибок, Расшифровка, Нарушение, Описание, ЭтоОшибка);
	КонецЦикла;
	
	СтрокаДляПроверки = "П02000201;П02000202;П02000203;П02000204;П02000205;П02000206;П02000207;П02000208;П02000209;"
						+ "П02000221;П02000222;П02000223;П02000230;П02000231;П02000240;П02000241;"
						+ "П02000171;П02000242;П02000250;П02000251;П02000260;";
	Знак = ">=";
	Стр = 0;
	
	Пока Истина Цикл
		ИндексТЗ = СтрНайти(СтрокаДляПроверки, ";");
		Если ИндексТЗ = 0 Тогда 
			Прервать;
		КонецЕсли;
		ТекНачало = Лев(СтрокаДляПроверки, ИндексТЗ - 1);
		СтрокаДляПроверки = Прав(СтрокаДляПроверки, СтрДлина(СтрокаДляПроверки) - 1 - СтрДлина(ТекНачало));
		Стр = Стр + 1;
		
		ТекстФормулы = "гр. 1 >= гр. 2 по стр. " + Стр;
		Описание     = "гр. 1 < гр. 2 по стр. " + Стр;
		ЛеваяЧасть  = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели[ТекНачало + "3"]);
		ПраваяЧасть  = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Показатели[ТекНачало + "4"]);
		Расшифровка = Строка(ЛеваяЧасть) + " " + Знак + " " + Строка(ПраваяЧасть);
		
		Если ЛеваяЧасть >= ПраваяЧасть Тогда
			ЭтоОшибка = Ложь;
			ОбщееЧислоБезОшибок = ОбщееЧислоБезОшибок + 1;
		Иначе
