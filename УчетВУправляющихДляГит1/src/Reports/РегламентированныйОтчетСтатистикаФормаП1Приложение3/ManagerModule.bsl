	НоваяФорма = ТаблицаФормОтчета.Добавить();
	НоваяФорма.ФормаОтчета        = "ФормаОтчета2012Кв1";
	НоваяФорма.ОписаниеОтчета     = "Форма утверждена приказом Росстата от 10.08.2011 № 351.";
	НоваяФорма.РедакцияФормы      = "от 10.08.2011 № 351.";
