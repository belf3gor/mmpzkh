		КОГДА (ПВХ.ОбновляетсяССервера = ИСТИНА)
				И ((Рег.ТекущаяВерсияНаКлиенте &lt; Рег.ТекущаяВерсияНаСервере)
					ИЛИ (ЕСТЬNULL(Рег.ТекущаяВерсияНаКлиенте, ДАТАВРЕМЯ(1,1,1)) = ДАТАВРЕМЯ(1,1,1))) ТОГДА
